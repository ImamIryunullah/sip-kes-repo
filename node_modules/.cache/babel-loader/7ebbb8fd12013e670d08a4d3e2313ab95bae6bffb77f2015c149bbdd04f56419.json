{"ast":null,"code":"import NavbarUser from '@/components/NavbarUser.vue';\nexport default {\n  components: {\n    NavbarUser\n  },\n  data() {\n    return {\n      riwayat: [{\n        nama: \"John Doe\",\n        jenisSertifikat: \"Sertifikat Keahlian\",\n        tanggal: \"2024-02-10\",\n        status: \"Diproses\"\n      }, {\n        nama: \"Jane Smith\",\n        jenisSertifikat: \"Sertifikat Pelatihan\",\n        tanggal: \"2024-02-08\",\n        status: \"Disetujui\"\n      }],\n      pengaduan: {\n        nama: '',\n        email: '',\n        kategori: '',\n        deskripsi: ''\n      }\n    };\n  },\n  methods: {\n    getStatusClass(status) {\n      switch (status) {\n        case \"Diproses\":\n          return \"text-yellow-500\";\n        case \"Disetujui\":\n          return \"text-green-500\";\n        case \"Ditolak\":\n          return \"text-red-500\";\n        default:\n          return \"text-gray-500\";\n      }\n    },\n    submitPengaduan() {\n      alert(\"Pengaduan berhasil dikirim!\");\n      // Logika pengiriman pengaduan (misalnya, simpan ke backend) bisa ditambahkan di sini.\n      console.log(this.pengaduan);\n    }\n  }\n};","map":{"version":3,"names":["NavbarUser","components","data","riwayat","nama","jenisSertifikat","tanggal","status","pengaduan","email","kategori","deskripsi","methods","getStatusClass","submitPengaduan","alert","console","log"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\user\\RiwayatPengajuan.vue"],"sourcesContent":["<template>\r\n  <div class=\"w-screen h-screen flex flex-col mt-12\">\r\n    <NavbarUser />\r\n\r\n    <!-- Content Section -->\r\n    <div class=\"flex flex-col items-center p-10 bg-gray-100 flex-grow\">\r\n      \r\n      <!-- Riwayat Pengajuan Sertifikat -->\r\n      <div class=\"bg-white w-full max-w-3xl p-6 rounded-lg shadow-md mb-10\">\r\n        <h2 class=\"text-2xl font-semibold text-[#03a980] text-center mb-6\">Riwayat Pengajuan Sertifikat</h2>\r\n        \r\n        <div v-if=\"riwayat.length > 0\" class=\"space-y-4\">\r\n          <div v-for=\"(item, index) in riwayat\" :key=\"index\" class=\"p-4 bg-gray-50 rounded-lg shadow-md border border-gray-300\">\r\n            <p><strong>Nama:</strong> {{ item.nama }}</p>\r\n            <p><strong>Jenis Sertifikat:</strong> {{ item.jenisSertifikat }}</p>\r\n            <p><strong>Tanggal Pengajuan:</strong> {{ item.tanggal }}</p>\r\n            <p><strong>Status:</strong> <span :class=\"getStatusClass(item.status)\">{{ item.status }}</span></p>\r\n          </div>\r\n        </div>\r\n        \r\n        <div v-else class=\"text-center text-gray-500\">Belum ada pengajuan sertifikat.</div>\r\n      </div>\r\n\r\n      <!-- Form Pengaduan -->\r\n      <div class=\"bg-white w-full max-w-3xl p-6 rounded-lg shadow-md\">\r\n        <h2 class=\"text-2xl font-semibold text-[#03a980] text-center mb-6\">Form Pengaduan</h2>\r\n\r\n        <form @submit.prevent=\"submitPengaduan\" class=\"space-y-4\">\r\n          <div>\r\n            <label for=\"nama\" class=\"text-sm text-gray-600\">Nama Lengkap</label>\r\n            <input v-model=\"pengaduan.nama\" id=\"nama\" type=\"text\" class=\"input-field\" placeholder=\"Nama Lengkap\" required />\r\n          </div>\r\n          \r\n          <div>\r\n            <label for=\"email\" class=\"text-sm text-gray-600\">Email</label>\r\n            <input v-model=\"pengaduan.email\" id=\"email\" type=\"email\" class=\"input-field\" placeholder=\"Email\" required />\r\n          </div>\r\n\r\n          <div>\r\n            <label for=\"kategori\" class=\"text-sm text-gray-600\">Kategori Pengaduan</label>\r\n            <select v-model=\"pengaduan.kategori\" id=\"kategori\" class=\"input-field\" required>\r\n              <option value=\"\">Pilih Kategori</option>\r\n              <option value=\"Layanan\">Layanan</option>\r\n              <option value=\"Sertifikat\">Sertifikat</option>\r\n              <option value=\"Prosedur\">Prosedur</option>\r\n            </select>\r\n          </div>\r\n\r\n          <div>\r\n            <label for=\"deskripsi\" class=\"text-sm text-gray-600\">Deskripsi Pengaduan</label>\r\n            <textarea v-model=\"pengaduan.deskripsi\" id=\"deskripsi\" rows=\"4\" class=\"input-field\" placeholder=\"Deskripsikan pengaduan Anda\" required></textarea>\r\n          </div>\r\n\r\n          <button type=\"submit\" class=\"w-full bg-[#03a980] text-white py-2 rounded-lg hover:bg-[#028a66] transition\">Kirim Pengaduan</button>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavbarUser from '@/components/NavbarUser.vue';\r\n\r\nexport default {\r\n  components: {\r\n    NavbarUser\r\n  },\r\n  data() {\r\n    return {\r\n      riwayat: [\r\n        {\r\n          nama: \"John Doe\",\r\n          jenisSertifikat: \"Sertifikat Keahlian\",\r\n          tanggal: \"2024-02-10\",\r\n          status: \"Diproses\"\r\n        },\r\n        {\r\n          nama: \"Jane Smith\",\r\n          jenisSertifikat: \"Sertifikat Pelatihan\",\r\n          tanggal: \"2024-02-08\",\r\n          status: \"Disetujui\"\r\n        }\r\n      ],\r\n      pengaduan: {\r\n        nama: '',\r\n        email: '',\r\n        kategori: '',\r\n        deskripsi: ''\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    getStatusClass(status) {\r\n      switch (status) {\r\n        case \"Diproses\": return \"text-yellow-500\";\r\n        case \"Disetujui\": return \"text-green-500\";\r\n        case \"Ditolak\": return \"text-red-500\";\r\n        default: return \"text-gray-500\";\r\n      }\r\n    },\r\n    submitPengaduan() {\r\n      alert(\"Pengaduan berhasil dikirim!\");\r\n      // Logika pengiriman pengaduan (misalnya, simpan ke backend) bisa ditambahkan di sini.\r\n      console.log(this.pengaduan);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.input-field {\r\n  @apply w-full p-3 border border-gray-300 rounded-lg bg-gray-100 focus:outline-none;\r\n}\r\n</style>\r\n"],"mappings":"AA6DA,OAAOA,UAAS,MAAO,6BAA6B;AAEpD,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,CACP;QACEC,IAAI,EAAE,UAAU;QAChBC,eAAe,EAAE,qBAAqB;QACtCC,OAAO,EAAE,YAAY;QACrBC,MAAM,EAAE;MACV,CAAC,EACD;QACEH,IAAI,EAAE,YAAY;QAClBC,eAAe,EAAE,sBAAsB;QACvCC,OAAO,EAAE,YAAY;QACrBC,MAAM,EAAE;MACV,EACD;MACDC,SAAS,EAAE;QACTJ,IAAI,EAAE,EAAE;QACRK,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE,EAAE;QACZC,SAAS,EAAE;MACb;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,cAAcA,CAACN,MAAM,EAAE;MACrB,QAAQA,MAAM;QACZ,KAAK,UAAU;UAAE,OAAO,iBAAiB;QACzC,KAAK,WAAW;UAAE,OAAO,gBAAgB;QACzC,KAAK,SAAS;UAAE,OAAO,cAAc;QACrC;UAAS,OAAO,eAAe;MACjC;IACF,CAAC;IACDO,eAAeA,CAAA,EAAG;MAChBC,KAAK,CAAC,6BAA6B,CAAC;MACpC;MACAC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACT,SAAS,CAAC;IAC7B;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}