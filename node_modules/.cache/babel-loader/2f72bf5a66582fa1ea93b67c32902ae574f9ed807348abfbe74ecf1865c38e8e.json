ajuan sertifikat Anda ditolak karena tidak memenuhi syarat.\"\r\n          }\r\n        }\r\n      ]\r\n    };\r\n  },\r\n  methods: {\r\n    getStatusClass(status) {\r\n      switch (status) {\r\n        case \"Diproses\": return \"text-yellow-500\";\r\n        case \"Disetujui\": return \"text-green-500\";\r\n        case \"Ditolak\": return \"text-red-500\";\r\n        default: return \"text-gray-500\";\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.input-field {\r\n  @apply w-full p-3 border border-gray-300 rounded-lg bg-gray-100 focus:outline-none;\r\n}\r\n</style>\r\n"],"mappings":"AAkCA,OAAOA,UAAS,MAAO,6BAA6B;AAEpD,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,CACP;QACEC,IAAI,EAAE,UAAU;QAChBC,eAAe,EAAE,qBAAqB;QACtCC,OAAO,EAAE,YAAY;QACrBC,MAAM,EAAE,UAAU;QAClBC,QAAQ,EAAE;UACRD,MAAM,EAAE,WAAW;UACnBE,SAAS,EAAE;QACb;MACF,CAAC,EACD;QACEL,IAAI,EAAE,YAAY;QAClBC,eAAe,EAAE,sBAAsB;QACvCC,OAAO,EAAE,YAAY;QACrBC,MAAM,EAAE,WAAW;QACnBC,QAAQ,EAAE;UACRD,MAAM,EAAE,SAAS;UACjBE,SAAS,EAAE;QACb;MACF;IAEJ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,cAAcA,CAACJ,MAAM,EAAE;MACrB,QAAQA,MAAM;QACZ,KAAK,UAAU;UAAE,OAAO,iBAAiB;QACzC,KAAK,WAAW;UAAE,OAAO,gBAAgB;QACzC,KAAK,SAAS;UAAE,OAAO,cAAc;QACrC;UAAS,OAAO,eAAe;MACjC;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}