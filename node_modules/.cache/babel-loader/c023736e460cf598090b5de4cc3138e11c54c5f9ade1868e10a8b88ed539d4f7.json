{"ast":null,"code":"import NavbarUser from '@/components/NavbarUser.vue';\nexport default {\n  components: {\n    NavbarUser\n  },\n  data() {\n    return {\n      amount: '',\n      // Jumlah pembayaran\n      selectedPaymentMethod: '',\n      // Metode pembayaran yang dipilih\n      selectedBank: '',\n      // Bank yang dipilih untuk transfer\n      transactionStatus: null,\n      // Status transaksi\n      bankAccount: '',\n      // Nomor rekening untuk transfer bank\n      dummyAccountNumber: '123-456-7890',\n      // Dummy account number for bank transfer\n      transferAmount: '1000000',\n      // Dummy amount for bank transfer\n      selectedFile: null // Selected file for proof of transaction\n    };\n  },\n  methods: {\n    async processTransaction() {\n      // Validasi jika jumlah pembayaran dan metode pembayaran dipilih\n      if (!this.amount || !this.selectedPaymentMethod) {\n        alert(\"Silakan lengkapi semua data transaksi.\");\n        return;\n      }\n\n      // Simulasi pemrosesan transaksi\n      try {\n        // Proses transaksi (ini bisa diganti dengan API call untuk pembayaran)\n        const response = await this.fakePaymentAPI();\n\n        // Update status transaksi berdasarkan response dari server\n        if (response.success) {\n          this.transactionStatus = 'success';\n        } else {\n          this.transactionStatus = 'failed';\n        }\n      } catch (error) {\n        console.error(\"Terjadi kesalahan:\", error);\n        this.transactionStatus = 'failed';\n      }\n    },\n    onFileChange(event) {\n      const file = event.target.files[0];\n      if (file) {\n        this.selectedFile = file;\n      }\n    },\n    // Simulasi API pembayaran\n    fakePaymentAPI() {\n      return new Promise(resolve => {\n        setTimeout(() => {\n          const success = Math.random() > 0.5; // 50% chance success\n          resolve({\n            success\n          });\n        }, 2000);\n      });\n    }\n  }\n};","map":{"version":3,"names":["NavbarUser","components","data","amount","selectedPaymentMethod","selectedBank","transactionStatus","bankAccount","dummyAccountNumber","transferAmount","selectedFile","methods","processTransaction","alert","response","fakePaymentAPI","success","error","console","onFileChange","event","file","target","files","Promise","resolve","setTimeout","Math","random"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\user\\TransaksiUserView.vue"],"sourcesContent":["<template>\r\n  <div class=\"w-screen h-screen flex flex-col mt-12\">\r\n    <!-- Navbar -->\r\n    <NavbarUser />\r\n    \r\n    <!-- Main Content Section -->\r\n    <div class=\"flex flex-col items-center p-10 bg-gray-100 flex-grow\">\r\n      <div class=\"bg-white w-full max-w-xl p-8 rounded-lg shadow-md\">\r\n        <h2 class=\"text-2xl font-semibold text-[#03a980] text-center mb-6\">Transaksi Pembayaran</h2>\r\n\r\n        <!-- Form untuk memasukkan detail transaksi -->\r\n        <form @submit.prevent=\"processTransaction\">\r\n          <div class=\"space-y-4\">\r\n            <!-- Payment Method Selection -->\r\n            <div>\r\n              <div class=\"mt-4\">\r\n                <label for=\"\" class=\"block text-sm font-medium text-gray-700\">Masukkan Nama Pengirim</label>\r\n                <input type=\"text\" name=\"namaPengirim\" id=\"\" class=\"input-field w-full mb-4\" placeholder=\"Masukan Nama Pengirim\">\r\n                <label for=\"\" class=\"block text-sm font-medium text-gray-400 mt-2 mb-1\">Silahkan pilih metode pembayaran dibawah ini!</label>\r\n              </div> \r\n              <label for=\"paymentMethod\" class=\"block text-sm font-medium text-gray-700\">Metode Pembayaran</label>\r\n              <select v-model=\"selectedPaymentMethod\" id=\"paymentMethod\" class=\"input-field w-full\" required>\r\n                <option disabled value=\"\">Pilih Metode Pembayaran</option>\r\n                <option value=\"Transfer Bank\">Transfer Bank</option>\r\n                <option value=\"E-wallet\">E-wallet</option>\r\n                <option value=\"QRIS\">QRIS</option>\r\n              </select>\r\n            </div>\r\n\r\n            <!-- Bank Transfer Input -->\r\n            <div v-if=\"selectedPaymentMethod === 'Transfer Bank'\" class=\"space-y-4\">\r\n              <label class=\"block text-sm font-medium text-gray-700\">Bank Tujuan</label>\r\n              <select v-model=\"selectedBank\" id=\"bankSelection\" class=\"input-field w-full\" required>\r\n                <option disabled value=\"\">Pilih Bank</option>\r\n                <option value=\"BCA\">BCA</option>\r\n                <option value=\"Mandiri\">Mandiri</option>\r\n                <option value=\"BNI\">BNI</option>\r\n                <option value=\"BRI\">BRI</option>\r\n                <option value=\"Citi Bank\">Citi Bank</option>\r\n                <option value=\"Permata Bank\">Permata Bank</option>\r\n              </select>\r\n              \r\n              <!-- Dummy Bank Account & Transfer Amount -->\r\n              <div v-if=\"selectedBank\">\r\n                <p class=\"mt-2 text-sm text-gray-700\">Nomor Rekening Bank: <strong>{{ dummyAccountNumber }}</strong></p>\r\n                <p class=\"mt-2 text-sm text-gray-700\">Jumlah Transfer yang Diminta: <strong>Rp {{ transferAmount }}</strong></p>\r\n              </div>\r\n\r\n              <!-- Upload Transaction Proof -->\r\n              <div class=\"mt-4\">\r\n                <label for=\"paymentProof\" class=\"block text-sm font-medium text-gray-700\">Upload Bukti Pembayaran</label>\r\n                <input type=\"file\" id=\"paymentProof\" @change=\"onFileChange\" class=\"input-field w-full\" />\r\n              </div>\r\n            </div>\r\n\r\n            <!-- E-wallet Selection -->\r\n            <div v-if=\"selectedPaymentMethod === 'E-wallet'\" class=\"space-y-4\">\r\n                <label class=\"block text-sm font-medium text-gray-700\">Pilih E-Wallet</label>\r\n                <div class=\"flex justify-around\">\r\n                  <button type=\"button\" class=\"payment-button\">\r\n                    <img src=\"@/assets/dana.jpg\" alt=\"Dana\" class=\"payment-button w-8 h-8 mr-2\" />\r\n                    Dana\r\n                  </button>\r\n                  <button type=\"button\" class=\"payment-button\">\r\n                    <img src=\"@/assets/ovo.png\" alt=\"OVO\" class=\"payment-button w-8 h-8 mr-2\" />\r\n                    OVO\r\n                  </button>\r\n                  <button type=\"button\" class=\"payment-button\">\r\n                    <img src=\"@/assets/linkaja.jpg\" alt=\"LinkAja\" class=\"payment-button w-8 h-8 mr-2\" />\r\n                    LinkAja\r\n                  </button>\r\n                  <button type=\"button\" class=\"payment-button\">\r\n                    <img src=\"@/assets/shopeepay.png\" alt=\"ShopeePay\" class=\"payment-button w-8 h-8 mr-2\" />\r\n                    ShopeePay\r\n                  </button>\r\n                </div>\r\n              </div>\r\n\r\n\r\n            <!-- QRIS Details -->\r\n            <div v-if=\"selectedPaymentMethod === 'QRIS'\" class=\"space-y-4\">\r\n              <label class=\"block text-sm font-medium text-gray-700\">Scan QRIS</label>\r\n              <img src=\"qriscontoh\" alt=\"QRIS Code\" class=\"w-full h-auto rounded-lg mb-4\" />\r\n              <p class=\"text-sm text-center text-gray-500\">Scan the QR code to complete your payment</p>\r\n            </div>\r\n\r\n          </div>\r\n\r\n          <!-- Tombol untuk melakukan pembayaran -->\r\n          <button type=\"submit\" class=\"mt-6 w-full bg-[#03a980] text-white px-6 py-2 rounded-lg font-semibold hover:bg-[#028a66] transition\">\r\n            Bayar Sekarang\r\n          </button>\r\n        </form>\r\n\r\n        <!-- Konfirmasi Pembayaran -->\r\n        <div v-if=\"transactionStatus\" class=\"mt-6 text-center\">\r\n          <p v-if=\"transactionStatus === 'success'\" class=\"text-green-500\">Pembayaran berhasil! Terima kasih.</p>\r\n          <p v-if=\"transactionStatus === 'failed'\" class=\"text-red-500\">Pembayaran gagal. Silakan coba lagi.</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavbarUser from '@/components/NavbarUser.vue';\r\n\r\nexport default {\r\n  components: {\r\n    NavbarUser\r\n  },\r\n  data() {\r\n    return {\r\n      amount: '', // Jumlah pembayaran\r\n      selectedPaymentMethod: '', // Metode pembayaran yang dipilih\r\n      selectedBank: '', // Bank yang dipilih untuk transfer\r\n      transactionStatus: null, // Status transaksi\r\n      bankAccount: '', // Nomor rekening untuk transfer bank\r\n      dummyAccountNumber: '123-456-7890', // Dummy account number for bank transfer\r\n      transferAmount: '1000000', // Dummy amount for bank transfer\r\n      selectedFile: null, // Selected file for proof of transaction\r\n    };\r\n  },\r\n  methods: {\r\n    async processTransaction() {\r\n      // Validasi jika jumlah pembayaran dan metode pembayaran dipilih\r\n      if (!this.amount || !this.selectedPaymentMethod) {\r\n        alert(\"Silakan lengkapi semua data transaksi.\");\r\n        return;\r\n      }\r\n\r\n      // Simulasi pemrosesan transaksi\r\n      try {\r\n        // Proses transaksi (ini bisa diganti dengan API call untuk pembayaran)\r\n        const response = await this.fakePaymentAPI();\r\n\r\n        // Update status transaksi berdasarkan response dari server\r\n        if (response.success) {\r\n          this.transactionStatus = 'success';\r\n        } else {\r\n          this.transactionStatus = 'failed';\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Terjadi kesalahan:\", error);\r\n        this.transactionStatus = 'failed';\r\n      }\r\n    },\r\n\r\n    onFileChange(event) {\r\n      const file = event.target.files[0];\r\n      if (file) {\r\n        this.selectedFile = file;\r\n      }\r\n    },\r\n\r\n    // Simulasi API pembayaran\r\n    fakePaymentAPI() {\r\n      return new Promise((resolve) => {\r\n        setTimeout(() => {\r\n          const success = Math.random() > 0.5; // 50% chance success\r\n          resolve({ success });\r\n        }, 2000);\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Styling untuk form input dan tombol */\r\n.input-field {\r\n  @apply p-3 border border-gray-300 rounded-lg bg-gray-100 focus:outline-none focus:ring-2 focus:ring-[#03a980];\r\n}\r\n\r\n/* Styling untuk form dan tombol */\r\nbutton {\r\n  @apply p-3 bg-[#03a980] text-white rounded-lg font-semibold hover:bg-[#028a66] transition;\r\n}\r\n\r\n/* Styling QRIS Section */\r\n.payment-button {\r\n  @apply p-3 bg-gray-100 border border-gray-300 rounded-md text-sm font-semibold hover:bg-gray-200 transition;\r\n  min-width: 80px;\r\n  text-align: center;\r\n}\r\n\r\n.payment-button:hover {\r\n  background-color: #028a66;\r\n  color: white;\r\n}\r\n\r\nimg {\r\n  max-width: 100%;\r\n  height: auto;\r\n}\r\n</style>\r\n"],"mappings":"AAyGA,OAAOA,UAAS,MAAO,6BAA6B;AAEpD,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MAAE;MACZC,qBAAqB,EAAE,EAAE;MAAE;MAC3BC,YAAY,EAAE,EAAE;MAAE;MAClBC,iBAAiB,EAAE,IAAI;MAAE;MACzBC,WAAW,EAAE,EAAE;MAAE;MACjBC,kBAAkB,EAAE,cAAc;MAAE;MACpCC,cAAc,EAAE,SAAS;MAAE;MAC3BC,YAAY,EAAE,IAAI,CAAE;IACtB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,kBAAkBA,CAAA,EAAG;MACzB;MACA,IAAI,CAAC,IAAI,CAACT,MAAK,IAAK,CAAC,IAAI,CAACC,qBAAqB,EAAE;QAC/CS,KAAK,CAAC,wCAAwC,CAAC;QAC/C;MACF;;MAEA;MACA,IAAI;QACF;QACA,MAAMC,QAAO,GAAI,MAAM,IAAI,CAACC,cAAc,CAAC,CAAC;;QAE5C;QACA,IAAID,QAAQ,CAACE,OAAO,EAAE;UACpB,IAAI,CAACV,iBAAgB,GAAI,SAAS;QACpC,OAAO;UACL,IAAI,CAACA,iBAAgB,GAAI,QAAQ;QACnC;MACF,EAAE,OAAOW,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,oBAAoB,EAAEA,KAAK,CAAC;QAC1C,IAAI,CAACX,iBAAgB,GAAI,QAAQ;MACnC;IACF,CAAC;IAEDa,YAAYA,CAACC,KAAK,EAAE;MAClB,MAAMC,IAAG,GAAID,KAAK,CAACE,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;MAClC,IAAIF,IAAI,EAAE;QACR,IAAI,CAACX,YAAW,GAAIW,IAAI;MAC1B;IACF,CAAC;IAED;IACAN,cAAcA,CAAA,EAAG;MACf,OAAO,IAAIS,OAAO,CAAEC,OAAO,IAAK;QAC9BC,UAAU,CAAC,MAAM;UACf,MAAMV,OAAM,GAAIW,IAAI,CAACC,MAAM,CAAC,IAAI,GAAG,EAAE;UACrCH,OAAO,CAAC;YAAET;UAAQ,CAAC,CAAC;QACtB,CAAC,EAAE,IAAI,CAAC;MACV,CAAC,CAAC;IACJ;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}