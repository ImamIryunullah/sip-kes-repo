       </div>\r\n          <div class=\"p-4 bg-white rounded-lg shadow-md flex justify-between items-center cursor-pointer\">\r\n            <a href=\"/assets/idcardbelakangprovinsi.png\" download>Unduh ID Card Belakang</a>\r\n            <span>&gt;</span>\r\n          </div>\r\n          <div class=\"p-4 bg-white rounded-lg shadow-md flex justify-between items-center cursor-pointer\" @click=\"openChangePasswordModal\">\r\n            <span>Ganti Password</span>\r\n            <span>&gt;</span>\r\n          </div>\r\n          <div class=\"p-4 bg-white rounded-lg shadow-md flex justify-between items-center cursor-pointer\" @click=\"logout\">\r\n            <span>Logout</span>\r\n            <span>&gt;</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Modal Edit Profile -->\r\n      <div v-if=\"isEditModalOpen\" class=\"fixed inset-0 flex items-center justify-center bg-gray-900 bg-opacity-50\">\r\n        <div class=\"bg-white p-6 rounded-lg shadow-lg w-96\">\r\n          <h3 class=\"text-xl font-semibold text-center mb-4\">Edit Profile</h3>\r\n          <div class=\"grid grid-cols-1 gap-4\">\r\n            <div>\r\n              <label class=\"text-sm text-gray-600\">Full Name</label>\r\n              <input type=\"text\" v-model=\"user.fullName\" class=\"input-field\" />\r\n            </div>\r\n            <div>\r\n              <label class=\"text-sm text-gray-600\">Email</label>\r\n              <input type=\"email\" v-model=\"user.email\" class=\"input-field\" disabled />\r\n            </div>\r\n          </div>\r\n          <div class=\"mt-4 flex justify-between\">\r\n            <button @click=\"saveProfile\" class=\"bg-blue-500 text-white px-4 py-2 rounded-lg\">Save</button>\r\n            <button @click=\"closeEditModal\" class=\"bg-gray-400 text-white px-4 py-2 rounded-lg\">Cancel</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport NavbarUser from \"@/components/NavbarUser.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    NavbarUser,\r\n  },\r\n  data() {\r\n    return {\r\n      user: {\r\n        username: \"\",\r\n        firstName: \"\",\r\n        lastName: \"\",\r\n        email: \"\",\r\n        contactNumber: \"\",\r\n      },\r\n      isEditModalOpen: false,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchUserData();\r\n  },\r\n  methods: {\r\n    // Fetch user data with the logged-in user ID\r\n    async fetchUserData() {\r\n      const userId = localStorage.getItem(\"userId\"); // Assuming you stored user ID in localStorage\r\n\r\n      if (!userId) {\r\n        alert(\"User ID not found. Please login again.\");\r\n        return;\r\n      }\r\n\r\n      try {\r\n        const response = await axios.get(`http://192.168.1.11:5000/users/${userId}`);\r\n        this.user = response.data.user || {}; // Handle user data\r\n      } catch (error) {\r\n        console.error(\"Error fetching user data:\", error);\r\n        alert(\"Gagal mengambil data pengguna.\");\r\n      }\r\n    },\r\n    openEditModal() {\r\n      this.isEditModalOpen = true;\r\n    },\r\n    closeEditModal() {\r\n      this.isEditModalOpen = false;\r\n    },\r\n    // Save profile with the logged-in user ID\r\n    async saveProfile() {\r\n      const formData = new FormData();\r\n      formData.append(\"fullName\", this.user.firstName + ' ' + this.user.lastName);\r\n\r\n      try {\r\n        const Userid = localStorage.getItem(\"userId\"); // Assuming you stored user ID in localStorage\r\n        await axios.put(`http://192.168.1.11:5000/users/update`, formData, {\r\n          headers: {\r\n            \"Authorization\": `Bearer ${localStorage.getItem(\"authToken\")}` // Assuming you're using token-based auth\r\n          }\r\n        });\r\n        alert(\"Profil berhasil diperbarui!\");\r\n        this.isEditModalOpen = false;\r\n      } catch (error) {\r\n        console.error(\"Error updating profile:\", error);\r\n        alert(\"Gagal memperbarui profil.\");\r\n      }\r\n    },\r\n    logout() {\r\n      localStorage.removeItem(\"authToken\");\r\n      localStorage.removeItem(\"userId\");\r\n      alert(\"Logout berhasil!\");\r\n      this.$router.push(\"/login\");\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.input-field {\r\n  @apply w-full p-3 border border-gray-300 rounded-lg bg-gray-100 focus:outline-none;\r\n}\r\n</style>\r\n"],"mappings":";AAkEA,OAAOA,KAAI,MAAO,OAAO;AACzB,OAAOC,UAAS,MAAO,6BAA6B;AAEpD,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,QAAQ,EAAE,EAAE;QACZC,SAAS,EAAE,EAAE;QACbC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,aAAa,EAAE;MACjB,CAAC;MACDC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,aAAa,CAAC,CAAC;EACtB,CAAC;EACDC,OAAO,EAAE;IACP;IACA,MAAMD,aAAaA,CAAA,EAAG;MACpB,MAAME,MAAK,GAAIC,YAAY,CAACC,OAAO,CAAC,QAAQ,CAAC,EAAE;;MAE/C,IAAI,CAACF,MAAM,EAAE;QACXG,KAAK,CAAC,wCAAwC,CAAC;QAC/C;MACF;MAEA,IAAI;QACF,MAAMC,QAAO,GAAI,MAAMlB,KAAK,CAACmB,GAAG,CAAC,kCAAkCL,MAAM,EAAE,CAAC;QAC5E,IAAI,CAACV,IAAG,GAAIc,QAAQ,CAACf,IAAI,CAACC,IAAG,IAAK,CAAC,CAAC,EAAE;MACxC,EAAE,OAAOgB,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,2BAA2B,EAAEA,KAAK,CAAC;QACjDH,KAAK,CAAC,gCAAgC,CAAC;MACzC;IACF,CAAC;IACDK,aAAaA,CAAA,EAAG;MACd,IAAI,CAACZ,eAAc,GAAI,IAAI;IAC7B,CAAC;IACDa,cAAcA,CAAA,EAAG;MACf,IAAI,CAACb,eAAc,GAAI,KAAK;IAC9B,CAAC;IACD;IACA,MAAMc,WAAWA,CAAA,EAAG;MAClB,MAAMC,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC/BD,QAAQ,CAACE,MAAM,CAAC,UAAU,EAAE,IAAI,CAACvB,IAAI,CAACE,SAAQ,GAAI,GAAE,GAAI,IAAI,CAACF,IAAI,CAACG,QAAQ,CAAC;MAE3E,IAAI;QACF,MAAMqB,MAAK,GAAIb,YAAY,CAACC,OAAO,CAAC,QAAQ,CAAC,EAAE;QAC/C,MAAMhB,KAAK,CAAC6B,GAAG,CAAC,uCAAuC,EAAEJ,QAAQ,EAAE;UACjEK,OAAO,EAAE;YACP,eAAe,EAAE,UAAUf,YAAY,CAACC,OAAO,CAAC,WAAW,CAAC,EAAC,CAAE;UACjE;QACF,CAAC,CAAC;QACFC,KAAK,CAAC,6BAA6B,CAAC;QACpC,IAAI,CAACP,eAAc,GAAI,KAAK;MAC9B,EAAE,OAAOU,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,yBAAyB,EAAEA,KAAK,CAAC;QAC/CH,KAAK,CAAC,