{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport ApiService from \"@/service/api.js\";\nexport default {\n  data() {\n    return {\n      form: {\n        jenisPendaftaran: \"\",\n        provinsi: \"\",\n        kotaKabupaten: \"\",\n        firstName: \"\",\n        lastName: \"\",\n        contactNo: \"\",\n        emailUs: \"\",\n        username: \"\",\n        password: \"\",\n        role: \"\" // Ditentukan berdasarkan jenis pendaftaran\n      }\n    };\n  },\n  methods: {\n    resetForm() {\n      this.form.provinsi = \"\";\n      this.form.kotaKabupaten = \"\";\n    },\n    submitForm() {\n      if (!this.form.jenisPendaftaran) {\n        alert(\"Silakan pilih jenis pendaftaran.\");\n        return;\n      }\n\n      // Tentukan role berdasarkan jenis pendaftaran\n      this.form.role = this.form.jenisPendaftaran === \"Wilayah\" ? \"Pusat\" : \"Admin\";\n      const userData = {\n        ...this.form\n      };\n      ApiService.registerUser(userData).then(() => {\n        alert(\"Pendaftaran berhasil, menunggu persetujuan.\");\n        this.$router.push(\"/login\");\n      }).catch(error => {\n        console.error(error);\n        alert(\"Pendaftaran gagal, silakan coba lagi.\");\n      });\n    }\n  }\n};","map":{"version":3,"names":["ApiService","data","form","jenisPendaftaran","provinsi","kotaKabupaten","firstName","lastName","contactNo","emailUs","username","password","role","methods","resetForm","submitForm","alert","userData","registerUser","then","$router","push","catch","error","console"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\pusat\\Register.vue"],"sourcesContent":["<template>\r\n    <div class=\"w-screen h-screen flex flex-col items-center justify-center bg-gray-100\">\r\n      <div class=\"bg-white w-full max-w-md p-6 rounded-lg shadow-md\">\r\n        <h2 class=\"text-2xl font-semibold text-[#03a980] text-center mb-6\">Pendaftaran User</h2>\r\n        \r\n        <form @submit.prevent=\"submitForm\" class=\"grid grid-cols-2 gap-4\">\r\n          <!-- Pilihan Jenis Pendaftaran -->\r\n          <select v-model=\"form.jenisPendaftaran\" @change=\"resetForm\" class=\"input-field col-span-2\">\r\n            <option disabled value=\"\">Pilih Jenis Pendaftaran</option>\r\n            <option value=\"Daerah\">Daerah</option>\r\n            <option value=\"Wilayah\">Wilayah</option>\r\n          </select>\r\n  \r\n          <!-- Input yang tampil sesuai jenis pendaftaran -->\r\n          <input v-if=\"form.jenisPendaftaran === 'Wilayah'\" v-model=\"form.provinsi\" type=\"text\" placeholder=\"Provinsi\" class=\"input-field col-span-2\" />\r\n          <template v-if=\"form.jenisPendaftaran === 'Daerah'\">\r\n            <input v-model=\"form.provinsi\" type=\"text\" placeholder=\"Provinsi\" class=\"input-field\" />\r\n            <input v-model=\"form.kotaKabupaten\" type=\"text\" placeholder=\"Kota/Kabupaten\" class=\"input-field\" />\r\n          </template>\r\n  \r\n          <input v-model=\"form.firstName\" type=\"text\" placeholder=\"First Name\" class=\"input-field\" />\r\n          <input v-model=\"form.lastName\" type=\"text\" placeholder=\"Last Name\" class=\"input-field\" />\r\n          <input v-model=\"form.contactNo\" type=\"text\" placeholder=\"Contact No\" class=\"input-field\" />\r\n          <input v-model=\"form.emailUs\" type=\"email\" placeholder=\"Email\" class=\"input-field\" />\r\n          <input v-model=\"form.username\" type=\"text\" placeholder=\"Username\" class=\"input-field\" />\r\n          <input v-model=\"form.password\" type=\"password\" placeholder=\"Password\" class=\"input-field\" />\r\n  \r\n          <!-- Tombol Submit -->\r\n          <button type=\"submit\" class=\"col-span-2 mt-4 w-full bg-[#03a980] text-white px-6 py-2 rounded-lg font-bold hover:bg-[#028a66] transition\">\r\n            Daftar\r\n          </button>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import ApiService from \"@/service/api.js\";\r\n  \r\n  export default {\r\n    data() {\r\n      return {\r\n        form: {\r\n          jenisPendaftaran: \"\",\r\n          provinsi: \"\",\r\n          kotaKabupaten: \"\",\r\n          firstName: \"\",\r\n          lastName: \"\",\r\n          contactNo: \"\",\r\n          emailUs: \"\",\r\n          username: \"\",\r\n          password: \"\",\r\n          role: \"\", // Ditentukan berdasarkan jenis pendaftaran\r\n        }\r\n      };\r\n    },\r\n    methods: {\r\n      resetForm() {\r\n        this.form.provinsi = \"\";\r\n        this.form.kotaKabupaten = \"\";\r\n      },\r\n      submitForm() {\r\n        if (!this.form.jenisPendaftaran) {\r\n          alert(\"Silakan pilih jenis pendaftaran.\");\r\n          return;\r\n        }\r\n  \r\n        // Tentukan role berdasarkan jenis pendaftaran\r\n        this.form.role = this.form.jenisPendaftaran === \"Wilayah\" ? \"Pusat\" : \"Admin\";\r\n  \r\n        const userData = { ...this.form };\r\n  \r\n        ApiService.registerUser(userData)\r\n          .then(() => {\r\n            alert(\"Pendaftaran berhasil, menunggu persetujuan.\");\r\n            this.$router.push(\"/login\");\r\n          })\r\n          .catch(error => {\r\n            console.error(error);\r\n            alert(\"Pendaftaran gagal, silakan coba lagi.\");\r\n          });\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .input-field {\r\n    @apply w-full p-3 border border-gray-300 rounded-lg bg-gray-100 focus:outline-none;\r\n  }\r\n  </style>\r\n  "],"mappings":";AAqCE,OAAOA,UAAS,MAAO,kBAAkB;AAEzC,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,gBAAgB,EAAE,EAAE;QACpBC,QAAQ,EAAE,EAAE;QACZC,aAAa,EAAE,EAAE;QACjBC,SAAS,EAAE,EAAE;QACbC,QAAQ,EAAE,EAAE;QACZC,SAAS,EAAE,EAAE;QACbC,OAAO,EAAE,EAAE;QACXC,QAAQ,EAAE,EAAE;QACZC,QAAQ,EAAE,EAAE;QACZC,IAAI,EAAE,EAAE,CAAE;MACZ;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,SAASA,CAAA,EAAG;MACV,IAAI,CAACZ,IAAI,CAACE,QAAO,GAAI,