{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport NavbarPusat from '@/components/NavbarPusat.vue';\nexport default {\n  components: {\n    NavbarPusat\n  },\n  data() {\n    return {\n      selectedFilter: 'all',\n      isModalOpen: false,\n      selectedImage: '',\n      transactions: [{\n        userName: \"John Doe\",\n        bank: \"BCA\",\n        paymentMethod: \"Transfer Bank\",\n        status: \"Berhasil\",\n        date: \"2023-08-01\",\n        proofUrl: \"/assets/proof_1.jpg\"\n      }, {\n        userName: \"Jane Smith\",\n        bank: \"Mandiri\",\n        paymentMethod: \"QRIS\",\n        status: \"Gagal\",\n        date: \"2023-08-02\",\n        proofUrl: \"/assets/proof_2.jpg\"\n      }, {\n        userName: \"Ahmad Rahman\",\n        bank: \"BNI\",\n        paymentMethod: \"E-Wallet\",\n        status: \"Berhasil\",\n        date: \"2023-08-05\",\n        proofUrl: \"/assets/proof_3.jpg\"\n      }, {\n        userName: \"Siti Nurhaliza\",\n        bank: null,\n        paymentMethod: null,\n        status: \"Belum Transaksi\",\n        date: \"-\",\n        proofUrl: null\n      }]\n    };\n  },\n  computed: {\n    filteredTransactions() {\n      if (this.selectedFilter === 'all') {\n        return this.transactions;\n      }\n      if (this.selectedFilter === 'transacted') {\n        return this.transactions.filter(transaction => transaction.status !== 'Belum Transaksi');\n      }\n      if (this.selectedFilter === 'not_transacted') {\n        return this.transactions.filter(transaction => transaction.status === 'Belum Transaksi');\n      }\n      return this.transactions;\n    }\n  },\n  methods: {\n    filterTransactions(filter) {\n      this.selectedFilter = filter;\n    },\n    getTransactionStatusClass(status) {\n      switch (status) {\n        case 'Berhasil':\n          return 'text-green-500 font-semibold';\n        case 'Gagal':\n          return 'text-red-500 font-semibold';\n        case 'Diproses':\n          return 'text-yellow-500 font-semibold';\n        case 'Belum Transaksi':\n          return 'text-gray-500 italic';\n        default:\n          return 'text-gray-500';\n      }\n    },\n    openModal(imageUrl) {\n      this.selectedImage = imageUrl;\n      this.isModalOpen = true;\n    },\n    closeModal() {\n      this.isModalOpen = false;\n      this.selectedImage = '';\n    }\n  }\n};","map":{"version":3,"names":["NavbarPusat","components","data","selectedFilter","isModalOpen","selectedImage","transactions","userName","bank","paymentMethod","status","date","proofUrl","computed","filteredTransactions","filter","transaction","methods","filterTransactions","getTransactionStatusClass","openModal","imageUrl","closeModal"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\pusat\\PusatDataTransaksiView.vue"],"sourcesContent":["<template>\r\n  <div class=\"w-screen h-screen flex flex-col mt-12 mb-3\">\r\n    <NavbarPusat />\r\n\r\n    <div class=\"flex flex-col items-center p-6 md:p-10 bg-gray-100 flex-grow\">\r\n      <div class=\"bg-white w-full max-w-6xl p-6 md:p-8 rounded-lg shadow-md\">\r\n        <h2 class=\"text-2xl font-semibold text-[#03a980] text-center mb-7\">Daftar Transaksi Pengguna</h2>\r\n\r\n        <!-- Filter Transaksi -->\r\n        <div class=\"flex flex-wrap justify-center gap-4 mb-6\">\r\n          <button\r\n            @click=\"filterTransactions('all')\"\r\n            :class=\"['filter-button', selectedFilter === 'all' ? 'bg-[#03a980] text-white' : 'bg-gray-200 text-gray-700']\"\r\n          >\r\n            Semua Transaksi\r\n          </button>\r\n          <button\r\n            @click=\"filterTransactions('transacted')\"\r\n            :class=\"['filter-button', selectedFilter === 'transacted' ? 'bg-[#03a980] text-white' : 'bg-gray-200 text-gray-700']\"\r\n          >\r\n            Sudah Transaksi\r\n          </button>\r\n          <button\r\n            @click=\"filterTransactions('not_transacted')\"\r\n            :class=\"['filter-button', selectedFilter === 'not_transacted' ? 'bg-[#03a980] text-white' : 'bg-gray-200 text-gray-700']\"\r\n          >\r\n            Belum Transaksi\r\n          </button>\r\n        </div>\r\n        <div class=\"flex flex-wrap justify-center gap-4 mb-6\">\r\n         <select name=\"\" id=\"\">\r\n          <option value=\"\" @click=\"filterTransactions('transacted')\"\r\n          :class=\"['filter-button', selectedFilter === 'transacted' ? 'bg-[#03a980] text-white' : 'bg-gray-200 text-gray-700']\"\r\n          >Semua Transaksi\r\n          </option>\r\n          \r\n          <option value=\"\">\r\n\r\n\r\n            \r\n          </option>\r\n          <option value=\"\"></option>\r\n          <option value=\"\"></option>\r\n\r\n         </select>\r\n        </div>    \r\n        \r\n\r\n        <!-- Tabel Transaksi -->\r\n        <div v-if=\"filteredTransactions.length > 0\" class=\"overflow-x-auto\">\r\n          <table class=\"w-full table-auto border-collapse text-lg\">\r\n            <thead class=\"bg-[#028a66] text-white\">\r\n              <tr>\r\n                <th class=\"px-6 py-4 text-left\">Nama Pengguna</th>\r\n                <th class=\"px-6 py-4 text-left\">Metode Pembayaran</th>\r\n                <th class=\"px-6 py-4 text-left\">Jenis Bank</th>\r\n                <th class=\"px-6 py-4 text-left\">Status Transaksi</th>\r\n                <th class=\"px-6 py-4 text-left\">Tanggal Transaksi</th>\r\n                <th class=\"px-6 py-4 text-left\">Bukti Transaksi</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <tr v-for=\"(transaction, index) in filteredTransactions\" :key=\"index\" class=\"border-b hover:bg-gray-100\">\r\n                <td class=\"px-6 py-4\">{{ transaction.userName }}</td>\r\n                <td class=\"px-6 py-4\">{{ transaction.paymentMethod }}</td>\r\n                <td class=\"px-6 py-4\">{{ transaction.bank }}</td>\r\n                <td class=\"px-6 py-4\">\r\n                  <span :class=\"getTransactionStatusClass(transaction.status)\">\r\n                    {{ transaction.status }}\r\n                  </span>\r\n                </td>\r\n                <td class=\"px-6 py-4\">{{ transaction.date }}</td>\r\n                <td class=\"px-6 py-4\">\r\n                  <button\r\n                    v-if=\"transaction.proofUrl\"\r\n                    @click=\"openModal(transaction.proofUrl)\"\r\n                    class=\"text-blue-500 hover:underline\"\r\n                  >\r\n                    Lihat Bukti\r\n                  </button>\r\n                  <span v-else class=\"text-gray-400\">Tidak Ada</span>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n\r\n        <!-- Jika tidak ada transaksi -->\r\n        <div v-else class=\"text-center text-gray-500 mt-4\">\r\n          Tidak ada data transaksi yang ditemukan.\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Modal Bukti Transaksi -->\r\n    <div v-if=\"isModalOpen\" class=\"fixed inset-0 flex items-center justify-center bg-black bg-opacity-50 z-50\">\r\n      <div class=\"bg-white p-6 rounded-lg shadow-lg w-11/12 max-w-lg relative\">\r\n        <!-- Tombol Close -->\r\n        <button @click=\"closeModal\" class=\"absolute top-3 right-3 text-gray-600 hover:text-gray-900\">\r\n          âœ•\r\n        </button>\r\n        \r\n        <h3 class=\"text-xl font-semibold text-center mb-4\">Bukti Transaksi</h3>\r\n\r\n        <div class=\"flex justify-center\">\r\n          <img :src=\"selectedImage\" alt=\"Bukti Transaksi\" class=\"max-w-full h-auto rounded-lg shadow-md\" />\r\n        </div>\r\n\r\n        <button @click=\"closeModal\" class=\"mt-4 w-full bg-gray-300 text-gray-800 px-4 py-2 rounded-lg hover:bg-gray-400\">\r\n          Tutup\r\n        </button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavbarPusat from '@/components/NavbarPusat.vue';\r\n\r\nexport default {\r\n  components: {\r\n    NavbarPusat,\r\n  },\r\n  data() {\r\n    return {\r\n      selectedFilter: 'all',\r\n      isModalOpen: false,\r\n      selectedImage: '',\r\n      transactions: [\r\n        {\r\n          userName: \"John Doe\",\r\n          bank: \"BCA\",\r\n          paymentMethod: \"Transfer Bank\",\r\n          status: \"Berhasil\",\r\n          date: \"2023-08-01\",\r\n          proofUrl: \"/assets/proof_1.jpg\",\r\n        },\r\n        {\r\n          userName: \"Jane Smith\",\r\n          bank: \"Mandiri\",\r\n          paymentMethod: \"QRIS\",\r\n          status: \"Gagal\",\r\n          date: \"2023-08-02\",\r\n          proofUrl: \"/assets/proof_2.jpg\",\r\n        },\r\n        {\r\n          userName: \"Ahmad Rahman\",\r\n          bank: \"BNI\",\r\n          paymentMethod: \"E-Wallet\",\r\n          status: \"Berhasil\",\r\n          date: \"2023-08-05\",\r\n          proofUrl: \"/assets/proof_3.jpg\",\r\n        },\r\n        {\r\n          userName: \"Siti Nurhaliza\",\r\n          bank: null,\r\n          paymentMethod: null,\r\n          status: \"Belum Transaksi\",\r\n          date: \"-\",\r\n          proofUrl: null,\r\n        },\r\n      ],\r\n    };\r\n  },\r\n  computed: {\r\n    filteredTransactions() {\r\n      if (this.selectedFilter === 'all') {\r\n        return this.transactions;\r\n      }\r\n      if (this.selectedFilter === 'transacted') {\r\n        return this.transactions.filter(transaction => transaction.status !== 'Belum Transaksi');\r\n      }\r\n      if (this.selectedFilter === 'not_transacted') {\r\n        return this.transactions.filter(transaction => transaction.status === 'Belum Transaksi');\r\n      }\r\n      return this.transactions;\r\n    },\r\n  },\r\n  methods: {\r\n    filterTransactions(filter) {\r\n      this.selectedFilter = filter;\r\n    },\r\n    getTransactionStatusClass(status) {\r\n      switch (status) {\r\n        case 'Berhasil':\r\n          return 'text-green-500 font-semibold';\r\n        case 'Gagal':\r\n          return 'text-red-500 font-semibold';\r\n        case 'Diproses':\r\n          return 'text-yellow-500 font-semibold';\r\n        case 'Belum Transaksi':\r\n          return 'text-gray-500 italic';\r\n        default:\r\n          return 'text-gray-500';\r\n      }\r\n    },\r\n    openModal(imageUrl) {\r\n      this.selectedImage = imageUrl;\r\n      this.isModalOpen = true;\r\n    },\r\n    closeModal() {\r\n      this.isModalOpen = false;\r\n      this.selectedImage = '';\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Button Filter */\r\n.filter-button {\r\n  @apply px-4 py-2 rounded-md text-sm font-semibold transition-all duration-300;\r\n}\r\n\r\n/* Tabel */\r\ntable {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n}\r\n\r\nth,\r\ntd {\r\n  padding: 16px;\r\n}\r\n\r\nth {\r\n  background-color: #028a66;\r\n  color: white;\r\n}\r\n\r\ntd {\r\n  border-bottom: 1px solid #ddd;\r\n}\r\n\r\ntr:hover {\r\n  background-color: #f5f5f5;\r\n}\r\n</style>\r\n"],"mappings":";;AAqHA,OAAOA,WAAU,MAAO,8BAA8B;AAEtD,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,KAAK;MACrBC,WAAW,EAAE,KAAK;MAClBC,aAAa,EAAE,EAAE;MACjBC,YAAY,EAAE,CACZ;QACEC,QAAQ,EAAE,UAAU;QACpBC,IAAI,EAAE,KAAK;QACXC,aAAa,EAAE,eAAe;QAC9BC,MAAM,EAAE,UAAU;QAClBC,IAAI,EAAE,YAAY;QAClBC,QAAQ,EAAE;MACZ,CAAC,EACD;QACEL,QAAQ,EAAE,YAAY;QACtBC,IAAI,EAAE,SAAS;QACfC,aAAa,EAAE,MAAM;QACrBC,MAAM,EAAE,OAAO;QACfC,IAAI,EAAE,YAAY;QAClBC,QAAQ,EAAE;MACZ,CAAC,EACD;QACEL,QAAQ,EAAE,cAAc;QACxBC,IAAI,EAAE,KAAK;QACXC,aAAa,EAAE,UAAU;QACzBC,MAAM,EAAE,UAAU;QAClBC,IAAI,EAAE,YAAY;QAClBC,QAAQ,EAAE;MACZ,CAAC,EACD;QACEL,QAAQ,EAAE,gBAAgB;QAC1BC,IAAI,EAAE,IAAI;QACVC,aAAa,EAAE,IAAI;QACnBC,MAAM,EAAE,iBAAiB;QACzBC,IAAI,EAAE,GAAG;QACTC,QAAQ,EAAE;MACZ,CAAC;IAEL,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,oBAAoBA,CAAA,EAAG;MACrB,IAAI,IAAI,CAACX,cAAa,KAAM,KAAK,EAAE;Q