{"ast":null,"code":"import NavbarUser from '@/components/NavbarUser.vue';\nexport default {\n  components: {\n    NavbarUser\n  },\n  data() {\n    return {\n      anggota: {\n        nama: \"\",\n        nik: \"\",\n        noTelepon: \"\",\n        email: \"\",\n        tipePendaftaran: \"\",\n        provinsi: \"\",\n        kotaKabupaten: \"\",\n        jabatan: \"\",\n        alamat: \"\",\n        instansi: \"\",\n        foto3x4: null,\n        fotoKtp: null\n      }\n    };\n  },\n  methods: {\n    handleFoto3x4Upload(event) {\n      this.anggota.foto3x4 = event.target.files[0];\n    },\n    handleKtpUpload(event) {\n      this.anggota.fotoKtp = event.target.files[0];\n    },\n    async submitPendaftaran() {\n      if (!this.anggota.tipePendaftaran) {\n        alert(\"Silakan pilih tipe pendaftaran.\");\n        return;\n      }\n      if (this.anggota.tipePendaftaran === 'Kota/Kabupaten' && !this.anggota.kotaKabupaten) {\n        alert(\"Silakan pilih Kota/Kabupaten.\");\n        return;\n      }\n      if (this.anggota.tipePendaftaran === 'Provinsi' && !this.anggota.provinsi) {\n        alert(\"Silakan pilih Provinsi.\");\n        return;\n      }\n      if (!this.anggota.foto3x4 || !this.anggota.fotoKtp) {\n        alert(\"Silakan unggah Foto 3x4 dan Foto KTP.\");\n        return;\n      }\n      if (!this.anggota.jabatan) {\n        alert(\"Silakan pilih Jabatan LPKNI.\");\n        return;\n      }\n      const formData = new FormData();\n      formData.append(\"nama\", this.anggota.nama);\n      formData.append(\"nik\", this.anggota.nik);\n      formData.append(\"noTelepon\", this.anggota.noTelepon);\n      formData.append(\"email\", this.anggota.email);\n      formData.append(\"tipePendaftaran\", this.anggota.tipePendaftaran);\n      formData.append(\"provinsi\", this.anggota.provinsi);\n      formData.append(\"kotaKabupaten\", this.anggota.kotaKabupaten);\n      formData.append(\"jabatan\", this.anggota.jabatan);\n      formData.append(\"alamat\", this.anggota.alamat);\n      formData.append(\"instansi\", this.anggota.instansi);\n      formData.append(\"foto3x4\", this.anggota.foto3x4);\n      formData.append(\"fotoKtp\", this.anggota.fotoKtp);\n      try {\n        const response = await fetch(\"/api/pusat/pendaftaran\", {\n          method: \"POST\",\n          body: formData\n        });\n        if (response.ok) {\n          alert(\"Pendaftaran Anggota berhasil dikirim!\");\n        } else {\n          alert(\"Terjadi kesalahan. Silakan coba lagi.\");\n        }\n      } catch (error) {\n        console.error(\"Error:\", error);\n        alert(\"Gagal mengirim data ke pusat.\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["NavbarUser","components","data","anggota","nama","nik","noTelepon","email","tipePendaftaran","provinsi","kotaKabupaten","jabatan","alamat","instansi","foto3x4","fotoKtp","methods","handleFoto3x4Upload","event","target","files","handleKtpUpload","submitPendaftaran","alert","formData","FormData","append","response","fetch","method","body","ok","error","console"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\user\\PendaftaranAnggotaView.vue"],"sourcesContent":["<template>\r\n  <div class=\"w-screen h-screen flex flex-col mt-10\">\r\n    <NavbarUser />\r\n\r\n    <div class=\"flex flex-col items-center p-10 bg-gray-100 flex-grow\">\r\n      <div class=\"bg-white w-full max-w-2xl p-6 rounded-lg shadow-md\">\r\n        <h2 class=\"text-2xl font-semibold text-[#03a980] text-center mb-6\">Pendaftaran Anggota</h2>\r\n\r\n        <form @submit.prevent=\"submitPendaftaran\">\r\n          <div class=\"grid grid-cols-1 gap-4\">\r\n            <input v-model=\"anggota.nama\" type=\"text\" placeholder=\"Nama Lengkap\" class=\"input-field\" required />\r\n            <input v-model=\"anggota.nik\" type=\"text\" placeholder=\"NIK\" class=\"input-field\" required />\r\n            <input v-model=\"anggota.noTelepon\" type=\"text\" placeholder=\"Nomor Telepon\" class=\"input-field\" required />\r\n            <input v-model=\"anggota.email\" type=\"email\" placeholder=\"Email\" class=\"input-field\" required />\r\n\r\n            <!-- Pilih untuk Daftar Provinsi atau Kota/Kabupaten -->\r\n            <select v-model=\"anggota.tipePendaftaran\" class=\"input-field\" required>\r\n              <option disabled value=\"\">Pilih Tipe Pendaftaran</option>\r\n              <option value=\"Provinsi\">Provinsi</option>\r\n              <option value=\"Kota/Kabupaten\">Kota/Kabupaten</option>\r\n            </select>\r\n\r\n            <!-- Provinsi -->\r\n            <select v-if=\"anggota.tipePendaftaran === 'Provinsi'\" v-model=\"anggota.provinsi\" class=\"input-field\" required>\r\n              <option disabled value=\"\">Pilih Provinsi</option>\r\n              <option value=\"NTB\">NTB</option>\r\n              <option value=\"Jawa Timur\">Jawa Timur</option>\r\n              <option value=\"Sumatera Barat\">Sumatera Barat</option>\r\n            </select>\r\n\r\n            <!-- Kota/Kabupaten hanya muncul jika tipe pendaftaran adalah Kota/Kabupaten -->\r\n            <select v-if=\"anggota.tipePendaftaran === 'Kota/Kabupaten'\" v-model=\"anggota.kotaKabupaten\" class=\"input-field\" required>\r\n              <option disabled value=\"\">Pilih Kota/Kabupaten</option>\r\n              <option value=\"Kota Malang\">Kota Malang</option>\r\n              <option value=\"Surabaya\">Surabaya</option>\r\n              <option value=\"Gresik\">Gresik</option>\r\n            </select>\r\n\r\n            <!-- Jabatan LPKNI -->\r\n            <select v-model=\"anggota.jabatan\" class=\"input-field\" required>\r\n              <option disabled value=\"\">Pilih Jabatan LPKNI</option>\r\n              <option value=\"Anggota Direktorat\">Anggota Direktorat</option>\r\n              <option value=\"Anggota Sekretariat\">Anggota Sekretariat</option>\r\n              <option value=\"Anggota Divisi Keuangan\">Anggota Divisi Keuangan</option>\r\n              <option value=\"Anggota Divisi Sumber Daya Manusia\">Anggota Divisi Sumber Daya Manusia</option>\r\n              <option value=\"Lainnya\">Lainnya</option>\r\n            </select>\r\n\r\n            <input v-model=\"anggota.alamat\" type=\"text\" placeholder=\"Alamat Lengkap\" class=\"input-field\" required />\r\n            <!-- Upload Foto 3x4 -->\r\n            <label class=\"text-sm text-gray-600\">Upload Foto 3x4:</label>\r\n            <input type=\"file\" @change=\"handleFoto3x4Upload\" class=\"input-field\" required />\r\n\r\n            <!-- Upload Foto KTP -->\r\n            <label class=\"text-sm text-gray-600\">Upload Foto KTP:</label>\r\n            <input type=\"file\" @change=\"handleKtpUpload\" class=\"input-field\" required />\r\n\r\n            <button type=\"submit\" class=\"mt-6 w-full bg-[#03a980] text-white px-6 py-2 rounded-lg font-bold hover:bg-[#028a66] transition\">\r\n              Ajukan Pendaftaran\r\n            </button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavbarUser from '@/components/NavbarUser.vue';\r\n\r\nexport default {\r\n  components: {\r\n    NavbarUser\r\n  },\r\n  data() {\r\n    return {\r\n      anggota: {\r\n        nama: \"\",\r\n        nik: \"\",\r\n        noTelepon: \"\",\r\n        email: \"\",\r\n        tipePendaftaran: \"\", \r\n        provinsi: \"\",\r\n        kotaKabupaten: \"\",\r\n        jabatan: \"\",  \r\n        alamat: \"\",\r\n        instansi: \"\",\r\n        foto3x4: null,\r\n        fotoKtp: null,\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    handleFoto3x4Upload(event) {\r\n      this.anggota.foto3x4 = event.target.files[0];\r\n    },\r\n    handleKtpUpload(event) {\r\n      this.anggota.fotoKtp = event.target.files[0];\r\n    },\r\n    async submitPendaftaran() {\r\n      \r\n      if (!this.anggota.tipePendaftaran) {\r\n        alert(\"Silakan pilih tipe pendaftaran.\");\r\n        return;\r\n      }\r\n\r\n      if (this.anggota.tipePendaftaran === 'Kota/Kabupaten' && !this.anggota.kotaKabupaten) {\r\n        alert(\"Silakan pilih Kota/Kabupaten.\");\r\n        return;\r\n      }\r\n\r\n      if (this.anggota.tipePendaftaran === 'Provinsi' && !this.anggota.provinsi) {\r\n        alert(\"Silakan pilih Provinsi.\");\r\n        return;\r\n      }\r\n\r\n      if (!this.anggota.foto3x4 || !this.anggota.fotoKtp) {\r\n        alert(\"Silakan unggah Foto 3x4 dan Foto KTP.\");\r\n        return;\r\n      }\r\n\r\n      if (!this.anggota.jabatan) {\r\n        alert(\"Silakan pilih Jabatan LPKNI.\");\r\n        return;\r\n      }\r\n\r\n      const formData = new FormData();\r\n      formData.append(\"nama\", this.anggota.nama);\r\n      formData.append(\"nik\", this.anggota.nik);\r\n      formData.append(\"noTelepon\", this.anggota.noTelepon);\r\n      formData.append(\"email\", this.anggota.email);\r\n      formData.append(\"tipePendaftaran\", this.anggota.tipePendaftaran);\r\n      formData.append(\"provinsi\", this.anggota.provinsi);\r\n      formData.append(\"kotaKabupaten\", this.anggota.kotaKabupaten);\r\n      formData.append(\"jabatan\", this.anggota.jabatan);  \r\n      formData.append(\"alamat\", this.anggota.alamat);\r\n      formData.append(\"instansi\", this.anggota.instansi);\r\n      formData.append(\"foto3x4\", this.anggota.foto3x4);\r\n      formData.append(\"fotoKtp\", this.anggota.fotoKtp);\r\n\r\n      try {\r\n        const response = await fetch(\"/api/pusat/pendaftaran\", {\r\n          method: \"POST\",\r\n          body: formData,\r\n        });\r\n\r\n        if (response.ok) {\r\n          alert(\"Pendaftaran Anggota berhasil dikirim!\");\r\n        } else {\r\n          alert(\"Terjadi kesalahan. Silakan coba lagi.\");\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Error:\", error);\r\n        alert(\"Gagal mengirim data ke pusat.\");\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.input-field {\r\n  @apply p-3 border border-gray-300 rounded-lg bg-gray-100 focus:outline-none focus:ring-2 focus:ring-[#03a980];\r\n}\r\n</style>\r\n"],"mappings":"AAoEA,OAAOA,UAAS,MAAO,6BAA6B;AAEpD,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;QACPC,IAAI,EAAE,EAAE;QACRC,GAAG,EAAE,EAAE;QACPC,SAAS,EAAE,EAAE;QACbC,KAAK,EAAE,EAAE;QACTC,eAAe,EAAE,EAAE;QACnBC,QAAQ,EAAE,EAAE;QACZC,aAAa,EAAE,EAAE;QACjBC,OAAO,EAAE,EAAE;QACXC,MAAM,EAAE,EAAE;QACVC,QAAQ,EAAE,EAAE;QACZC,OAAO,EAAE,IAAI;QACbC,OAAO,EAAE;MACX;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,mBAAmBA,CAACC,KAAK,EAAE;MACzB,IAAI,CAACf,OAAO,CAACW,OAAM,GAAII,KAAK,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;IAC9C,CAAC;IACDC,eAAeA,CAACH,KAAK,EAAE;MACrB,IAAI,CAACf,OAAO,CAACY,OAAM,GAAIG,KAAK,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;IAC9C,CAAC;IACD,MAAME,iBAAiBA,CAAA,EAAG;MAExB,IAAI,CAAC,IAAI,CAACnB,OAAO,CAACK,eAAe,EAAE;QACjCe,KAAK,CAAC,iCAAiC,CAAC;QACxC;MACF;MAEA,IAAI,IAAI,CAACpB,OAAO,CAACK,eAAc,KAAM,gBAAe,IAAK,CAAC,IAAI,CAACL,OAAO,CAACO,aAAa,EAAE;QACpFa,KAAK,CAAC,+BAA+B,CAAC;QACtC;MACF;MAEA,IAAI,IAAI,CAACpB,OAAO,CAACK,eAAc,KAAM,UAAS,IAAK,CAAC,IAAI,CAACL,OAAO,CAACM,QAAQ,EAAE;QACzEc,KAAK,CAAC,yBAAyB,CAAC;QAChC;MACF;MAEA,IAAI,CAAC,IAAI,CAACpB,OAAO,CAACW,OAAM,IAAK,CAAC,IAAI,CAACX,OAAO,CAACY,OAAO,EAAE;QAClDQ,KAAK,CAAC,uCAAuC,CAAC;QAC9C;MACF;MAEA,IAAI,CAAC,IAAI,CAACpB,OAAO,CAACQ,OAAO,EAAE;QACzBY,KAAK,CAAC,8BAA8B,CAAC;QACrC;MACF;MAEA,MAAMC,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC/BD,QAAQ,CAACE,MAAM,CAAC,MAAM,EAAE,IAAI,CAACvB,OAAO,CAACC,IAAI,CAAC;MAC1CoB,QAAQ,CAACE,MAAM,CAAC,KAAK,EAAE,IAAI,CAACvB,OAAO,CAACE,GAAG,CAAC;MACxCmB,QAAQ,CAACE,MAAM,CAAC,WAAW,EAAE,IAAI,CAACvB,OAAO,CAACG,SAAS,CAAC;MACpDkB,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAE,IAAI,CAACvB,OAAO,CAACI,KAAK,CAAC;MAC5CiB,QAAQ,CAACE,MAAM,CAAC,iBAAiB,EAAE,IAAI,CAACvB,OAAO,CAACK,eAAe,CAAC;MAChEgB,QAAQ,CAACE,MAAM,CAAC,UAAU,EAAE,IAAI,CAACvB,OAAO,CAACM,QAAQ,CAAC;MAClDe,QAAQ,CAACE,MAAM,CAAC,eAAe,EAAE,IAAI,CAACvB,OAAO,CAACO,aAAa,CAAC;MAC5Dc,QAAQ,CAACE,MAAM,CAAC,SAAS,EAAE,IAAI,CAACvB,OA