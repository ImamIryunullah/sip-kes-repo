{"ast":null,"code":"import LineChart from '@/components/LineChart.vue';\nimport NavbarPusat from '@/components/NavbarPusat.vue';\nexport default {\n  components: {\n    NavbarPusat,\n    LineChart\n  },\n  data() {\n    return {\n      filters: [\"Hari ini\", \"7 hari terakhir\", \"28 hari terakhir\", \"Perbulan\", \"Pertahun\"],\n      activeFilter: \"7 hari terakhir\",\n      chartData: {\n        labels: [\"24 Sept\", \"25 Sept\", \"26 Sept\", \"27 Sept\", \"28 Sept\", \"29 Sept\"],\n        datasets: [{\n          label: \"Pendapatan\",\n          data: [5000000, 8000000, 6000000, 4000000, 12000000, 9000000],\n          borderColor: \"#03a980\",\n          backgroundColor: \"rgba(3, 169, 128, 0.2)\",\n          tension: 0.3,\n          fill: true\n        }]\n      },\n      monthlyData: {\n        labels: [\"Jan\", \"Feb\", \"Mar\", \"Apr\", \"Mei\", \"Jun\", \"Jul\", \"Agu\", \"Sep\", \"Okt\", \"Nov\", \"Des\"],\n        datasets: [{\n          label: \"Pendapatan Bulanan\",\n          data: [15000000, 18000000, 21000000, 25000000, 22000000, 27000000, 30000000, 32000000, 31000000, 28000000, 26000000, 29000000],\n          borderColor: \"#ff9800\",\n          backgroundColor: \"rgba(255, 152, 0, 0.2)\",\n          tension: 0.3,\n          fill: true\n        }]\n      },\n      yearlyData: {\n        labels: [\"2020\", \"2021\", \"2022\", \"2023\", \"2024\"],\n        datasets: [{\n          label: \"Pendapatan Tahunan\",\n          data: [200000000, 220000000, 250000000, 280000000, 310000000],\n          borderColor: \"#3f51b5\",\n          backgroundColor: \"rgba(63, 81, 181, 0.2)\",\n          tension: 0.3,\n          fill: true\n        }]\n      },\n      searchQuery: '',\n      selectedCategory: '',\n      transactionCategories: ['Kategori 1', 'Kategori 2', 'Kategori 3'],\n      filteredTransactions: [{\n        id: 1,\n        date: '2023-02-10',\n        description: 'Pembelian Barang',\n        category: 'Kategori 1',\n        amount: 1500000\n      }, {\n        id: 2,\n        date: '2023-02-12',\n        description: 'Pembayaran Gaji',\n        category: 'Kategori 2',\n        amount: 5000000\n      }]\n    };\n  },\n  methods: {\n    setActiveFilter(filter) {\n      this.activeFilter = filter;\n    },\n    formatCurrency(amount) {\n      return new Intl.NumberFormat('id-ID', {\n        style: 'currency',\n        currency: 'IDR'\n      }).format(amount);\n    },\n    getAmountClass(amount) {\n      return amount < 0 ? 'text-red-500' : 'text-green-500';\n    }\n  }\n};","map":{"version":3,"names":["LineChart","NavbarPusat","components","data","filters","activeFilter","chartData","labels","datasets","label","borderColor","backgroundColor","tension","fill","monthlyData","yearlyData","searchQuery","selectedCategory","transactionCategories","filteredTransactions","id","date","description","category","amount","methods","setActiveFilter","filter","formatCurrency","Intl","NumberFormat","style","currency","format","getAmountClass"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\pusat\\DataGrafikView.vue"],"sourcesContent":["<template>\r\n  <div class=\"w-full min-h-screen bg-gray-100 pt-20 mb-8\">\r\n    <!-- Navbar -->\r\n    <NavbarPusat />\r\n\r\n    <!-- Filter Waktu (Dropdown) -->\r\n    <div class=\"flex justify-center mt-6\">\r\n      <select\r\n        v-model=\"activeFilter\"\r\n        @change=\"setActiveFilter(activeFilter)\"\r\n        class=\"px-4 py-2 text-sm font-semibold bg-white border rounded-lg focus:outline-none focus:ring-2 focus:ring-[#03a980]\"\r\n      >\r\n        <option v-for=\"filter in filters\" :key=\"filter\" :value=\"filter\">\r\n          {{ filter }}\r\n        </option>\r\n      </select>\r\n    </div>\r\n\r\n    <!-- Ringkasan Keuangan -->\r\n    <div class=\"flex justify-center mt-6\">\r\n      <div class=\"grid grid-cols-1 sm:grid-cols-2 gap-8 w-[90%] md:w-[80%] custom-width\">\r\n        <!-- Ringkasan Pemasukan -->\r\n        <div class=\"bg-white p-6 rounded-lg shadow-md\">\r\n          <h3 class=\"text-lg font-semibold\">Ringkasan Pemasukan</h3>\r\n          <p class=\"text-gray-500 text-sm\">Terakhir diperbarui 4 Feb 10:00 AM</p>\r\n          <h2 class=\"text-2xl font-bold text-black mt-2\">Rp12.000.000</h2>\r\n          <p class=\"text-gray-500 text-sm\">Pemasukan dalam {{ activeFilter }} terakhir</p>\r\n          <LineChart :chartData=\"chartData\" />\r\n        </div>\r\n\r\n        <!-- Ringkasan Pengeluaran -->\r\n        <div class=\"bg-white p-6 rounded-lg shadow-md\">\r\n          <h3 class=\"text-lg font-semibold\">Ringkasan Pengeluaran</h3>\r\n          <p class=\"text-gray-500 text-sm\">Terakhir diperbarui 4 Feb 10:00 AM</p>\r\n          <h2 class=\"text-2xl font-bold text-black mt-2\">Rp9.500.000</h2>\r\n          <p class=\"text-gray-500 text-sm\">Pengeluaran dalam {{ activeFilter }} terakhir</p>\r\n          <LineChart :chartData=\"chartData\" />\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Grafik Perbulan & Pertahun -->\r\n    <div class=\"flex justify-center mt-6\">\r\n      <div class=\"grid grid-cols-1 sm:grid-cols-2 gap-6 w-full sm:w-[90%] md:w-[80%]\">\r\n        <div class=\"bg-white p-6 rounded-lg shadow-md\">\r\n          <h3 class=\"text-lg font-semibold\">Pendapatan Perbulan</h3>\r\n          <LineChart :chartData=\"monthlyData\" />\r\n        </div>\r\n        <div class=\"bg-white p-6 rounded-lg shadow-md\">\r\n          <h3 class=\"text-lg font-semibold\">Pendapatan Pertahun</h3>\r\n          <LineChart :chartData=\"yearlyData\" />\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Riwayat Transaksi -->\r\n    <div class=\"flex justify-center mt-6\">\r\n      <div class=\"bg-white p-6 rounded-lg shadow-md w-full sm:w-[90%] md:w-[80%]\">\r\n        <h3 class=\"text-lg font-semibold mb-4\">Riwayat Transaksi</h3>\r\n\r\n        <!-- Filter Transaksi -->\r\n        <div class=\"flex flex-col sm:flex-row justify-between mb-4\">\r\n          <input\r\n            type=\"text\"\r\n            v-model=\"searchQuery\"\r\n            placeholder=\"Cari transaksi...\"\r\n            class=\"w-full sm:w-1/3 p-2 border border-gray-300 rounded-lg focus:ring-2 focus:ring-[#03a980] mb-4 sm:mb-0\"\r\n          />\r\n          <select v-model=\"selectedCategory\" class=\"w-full sm:w-1/3 p-2 border border-gray-300 rounded-lg focus:ring-2 focus:ring-[#03a980]\">\r\n            <option value=\"\">Semua Kategori</option>\r\n            <option v-for=\"category in transactionCategories\" :key=\"category\" :value=\"category\">\r\n              {{ category }}\r\n            </option>\r\n          </select>\r\n        </div>\r\n\r\n        <!-- Tabel Riwayat -->\r\n        <div class=\"overflow-x-auto\">\r\n          <table class=\"min-w-full table-auto border-collapse border border-gray-300\">\r\n            <thead class=\"bg-[#03a980] text-white\">\r\n              <tr>\r\n                <th class=\"py-2 px-4 border\">Tanggal</th>\r\n                <th class=\"py-2 px-4 border\">Deskripsi</th>\r\n                <th class=\"py-2 px-4 border\">Kategori</th>\r\n                <th class=\"py-2 px-4 border\">Jumlah</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <tr v-for=\"transaction in filteredTransactions\" :key=\"transaction.id\" class=\"border\">\r\n                <td class=\"py-2 px-4\">{{ transaction.date }}</td>\r\n                <td class=\"py-2 px-4\">{{ transaction.description }}</td>\r\n                <td class=\"py-2 px-4\">{{ transaction.category }}</td>\r\n                <td class=\"py-2 px-4 font-semibold\" :class=\"getAmountClass(transaction.amount)\">\r\n                  {{ formatCurrency(transaction.amount) }}\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n\r\n        <!-- Jika Tidak Ada Transaksi -->\r\n        <p v-if=\"filteredTransactions.length === 0\" class=\"text-center text-gray-500 mt-4\">\r\n          Tidak ada transaksi yang sesuai dengan filter.\r\n        </p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport LineChart from '@/components/LineChart.vue';\r\nimport NavbarPusat from '@/components/NavbarPusat.vue';\r\n\r\nexport default {\r\n  components: {\r\n    NavbarPusat,\r\n    LineChart\r\n  },\r\n  data() {\r\n    return {\r\n      filters: [\"Hari ini\", \"7 hari terakhir\", \"28 hari terakhir\", \"Perbulan\", \"Pertahun\"],\r\n      activeFilter: \"7 hari terakhir\",\r\n      chartData: {\r\n        labels: [\"24 Sept\", \"25 Sept\", \"26 Sept\", \"27 Sept\", \"28 Sept\", \"29 Sept\"],\r\n        datasets: [\r\n          {\r\n            label: \"Pendapatan\",\r\n            data: [5000000, 8000000, 6000000, 4000000, 12000000, 9000000],\r\n            borderColor: \"#03a980\",\r\n            backgroundColor: \"rgba(3, 169, 128, 0.2)\",\r\n            tension: 0.3,\r\n            fill: true,\r\n          },\r\n        ],\r\n      },\r\n      monthlyData: {\r\n        labels: [\"Jan\", \"Feb\", \"Mar\", \"Apr\", \"Mei\", \"Jun\", \"Jul\", \"Agu\", \"Sep\", \"Okt\", \"Nov\", \"Des\"],\r\n        datasets: [\r\n          {\r\n            label: \"Pendapatan Bulanan\",\r\n            data: [15000000, 18000000, 21000000, 25000000, 22000000, 27000000, 30000000, 32000000, 31000000, 28000000, 26000000, 29000000],\r\n            borderColor: \"#ff9800\",\r\n            backgroundColor: \"rgba(255, 152, 0, 0.2)\",\r\n            tension: 0.3,\r\n            fill: true,\r\n          },\r\n        ],\r\n      },\r\n      yearlyData: {\r\n        labels: [\"2020\", \"2021\", \"2022\", \"2023\", \"2024\"],\r\n        datasets: [\r\n          {\r\n            label: \"Pendapatan Tahunan\",\r\n            data: [200000000, 220000000, 250000000, 280000000, 310000000],\r\n            borderColor: \"#3f51b5\",\r\n            backgroundColor: \"rgba(63, 81, 181, 0.2)\",\r\n            tension: 0.3,\r\n            fill: true,\r\n          },\r\n        ],\r\n      },\r\n      searchQuery: '',\r\n      selectedCategory: '',\r\n      transactionCategories: ['Kategori 1', 'Kategori 2', 'Kategori 3'],\r\n      filteredTransactions: [\r\n        { id: 1, date: '2023-02-10', description: 'Pembelian Barang', category: 'Kategori 1', amount: 1500000 },\r\n        { id: 2, date: '2023-02-12', description: 'Pembayaran Gaji', category: 'Kategori 2', amount: 5000000 },\r\n      ]\r\n    };\r\n  },\r\n  methods: {\r\n    setActiveFilter(filter) {\r\n      this.activeFilter = filter;\r\n    },\r\n    formatCurrency(amount) {\r\n      return new Intl.NumberFormat('id-ID', { style: 'currency', currency: 'IDR' }).format(amount);\r\n    },\r\n    getAmountClass(amount) {\r\n      return amount < 0 ? 'text-red-500' : 'text-green-500';\r\n    },\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Ensure no horizontal scroll */\r\nbody, html {\r\n  overflow-x: hidden;\r\n}\r\n\r\n/* Responsif Layout Adjustments */\r\n@media (max-width: 768px) {\r\n  .text-lg {\r\n    font-size: 14px;\r\n  }\r\n\r\n  .grid {\r\n    grid-template-columns: 1fr;\r\n  }\r\n\r\n  .w-full {\r\n    width: 100% !important;\r\n  }\r\n\r\n  table {\r\n    font-size: 14px;\r\n  }\r\n\r\n  th, td {\r\n    padding: 12px 8px;\r\n  }\r\n}\r\n\r\n/* @media (max-width: 1024px) {\r\n  .w-[80%] {\r\n    width: 90% !important;\r\n  }\r\n} */\r\n</style>\r\n"],"mappings":"AA8GA,OAAOA,SAAQ,MAAO,4BAA4B;AAClD,OAAOC,WAAU,MAAO,8BAA8B;AAEtD,eAAe;EACbC,UAAU,EAAE;IACVD,WAAW;IACXD;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,CAAC,UAAU,EAAE,iBAAiB,EAAE,kBAAkB,EAAE,UAAU,EAAE,UAAU,CAAC;MACpFC,YAAY,EAAE,iBAAiB;MAC/BC,SAAS,EAAE;QACTC,MAAM,EAAE,CAAC,SAAS,EAAE,SAAS,EAAE,SAAS,EAAE,SAAS,EAAE,SAAS,EAAE,SAAS,CAAC;QAC1EC,QAAQ,EAAE,CACR;UACEC,KAAK,EAAE,YAAY;UACnBN,IAAI,EAAE,CAAC,OAAO,EAAE,OAAO,EAAE,OAAO,EAAE,OAAO,EAAE,QAAQ,EAAE,OAAO,CAAC;UAC7DO,WAAW,EAAE,SAAS;UACtBC,eAAe,EAAE,wBAAwB;UACzCC,OAAO,EAAE,GAAG;UACZC,IAAI,EAAE;QACR,CAAC;MAEL,CAAC;MACDC,WAAW,EAAE;QA