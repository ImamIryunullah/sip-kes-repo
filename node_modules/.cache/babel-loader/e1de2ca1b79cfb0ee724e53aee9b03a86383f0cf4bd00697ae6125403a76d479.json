{"ast":null,"code":"import NavbarUser from '@/components/NavbarUser.vue';\nexport default {\n  components: {\n    NavbarUser\n  },\n  data() {\n    return {\n      riwayat: [{\n        nama: \"John Doe\",\n        provinsi: \"Jawa Timur\",\n        kotaKabupaten: \"Kota Malang\",\n        status: \"Diproses\",\n        anggotaType: \"Provinsi\" // Provinsi or Kota/Kabupaten\n      }, {\n        nama: \"John Doe\",\n        provinsi: \"Jawa Timur\",\n        kotaKabupaten: \"Kota Malang\",\n        status: \"Diproses\",\n        anggotaType: \"Provinsi\"\n      }],\n      isModalOpen: false,\n      selectedPendaftaran: null\n    };\n  },\n  methods: {\n    getStatusClass(status) {\n      switch (status) {\n        case \"Diproses\":\n          return \"text-yellow-500\";\n        case \"Disetujui\":\n          return \"text-green-500\";\n        case \"Ditolak\":\n          return \"text-red-500\";\n        default:\n          return \"text-gray-500\";\n      }\n    },\n    // Open modal with selected user data\n    openModal(pendaftaran) {\n      this.selectedPendaftaran = pendaftaran;\n      this.isModalOpen = true;\n    },\n    // Close the modal\n    closeModal() {\n      this.isModalOpen = false;\n    },\n    // Method to handle ID card generation\n    async generateIdCard(pendaftaran) {\n      // Here you can call the method to generate ID card for either Provinsi or Kota/Kabupaten\n      if (pendaftaran.anggotaType === \"Provinsi\") {\n        console.log('Generate ID Card for Provinsi');\n        // Call your generate PDF method for Provinsi\n      } else if (pendaftaran.anggotaType === \"KotaKabupaten\") {\n        console.log('Generate ID Card for Kota/Kabupaten');\n        // Call your generate PDF method for Kota/Kabupaten\n      }\n      this.closeModal(); // Close modal after generation\n    }\n  }\n};","map":{"version":3,"names":["NavbarUser","components","data","riwayat","nama","provinsi","kotaKabupaten","status","anggotaType","isModalOpen","selectedPendaftaran","methods","getStatusClass","openModal","pendaftaran","closeModal","generateIdCard","console","log"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\user\\RiwayatPengajuan.vue"],"sourcesContent":["<template>\r\n  <div class=\"w-screen h-screen flex flex-col mt-12\">\r\n    <NavbarUser />\r\n\r\n    <div class=\"flex flex-col items-center p-10 bg-gray-100 flex-grow\">\r\n      <div class=\"bg-white w-full max-w-3xl p-6 rounded-lg shadow-md\">\r\n        <h2 class=\"text-2xl font-semibold text-[#03a980] text-center mb-6\">Riwayat Pengajuan Pendaftaran Anggota</h2>\r\n\r\n        <div v-if=\"riwayat.length > 0\" class=\"space-y-4\">\r\n          <div v-for=\"(item, index) in riwayat\" :key=\"index\" class=\"p-4 bg-gray-50 rounded-lg shadow-md border border-gray-300\">\r\n            <p><strong>Nama:</strong> {{ item.nama }}</p>\r\n            <p><strong>Provinsi:</strong> {{ item.provinsi }}</p>\r\n            <p><strong>Kota/Kabupaten:</strong> {{ item.kotaKabupaten }}</p>\r\n            <p><strong>Status:</strong> <span :class=\"getStatusClass(item.status)\">{{ item.status }}</span></p>\r\n\r\n            <!-- Only show the print ID card button if approved -->\r\n            <div v-if=\"item.status === 'Disetujui'\">\r\n              <button @click=\"openModal(item)\" class=\"mt-4 w-full bg-[#03a980] text-white px-6 py-2 rounded-lg font-semibold hover:bg-[#028a66] transition\">\r\n                Cetak ID Card\r\n              </button>\r\n            </div>\r\n\r\n            <!-- Show approved/rejected message -->\r\n            <p v-if=\"item.status === 'Disetujui'\">Status: <span class=\"text-green-500\">Disetujui oleh pusat.</span></p>\r\n            <p v-if=\"item.status === 'Ditolak'\">Status: <span class=\"text-red-500\">Ditolak oleh pusat.</span></p>\r\n          </div>\r\n        </div>\r\n\r\n        <div v-else class=\"text-center text-gray-500\">Belum ada pengajuan pendaftaran anggota.</div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n  <!-- Modal for ID Card generation -->\r\n  <div v-if=\"isModalOpen\" class=\"fixed inset-0 bg-gray-600 bg-opacity-50 flex justify-center items-center\">\r\n    <div class=\"bg-white p-6 rounded-lg w-1/3\">\r\n      <h3 class=\"text-xl font-semibold mb-4\">Generate ID Card</h3>\r\n      <p><strong>Nama:</strong> {{ selectedPendaftaran.nama }}</p>\r\n      <p><strong>Jenis Anggota:</strong> {{ selectedPendaftaran.anggotaType }}</p>\r\n\r\n      <!-- Button for generating ID Card -->\r\n      <button @click=\"generateIdCard(selectedPendaftaran)\" class=\"w-full bg-[#03a980] text-white px-6 py-2 rounded-lg font-semibold hover:bg-[#028a66] transition\">\r\n        Generate ID Card\r\n      </button>\r\n\r\n      <button @click=\"closeModal\" class=\"mt-2 w-full bg-red-500 text-white px-6 py-2 rounded-lg font-semibold hover:bg-red-600 transition\">\r\n        Close\r\n      </button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavbarUser from '@/components/NavbarUser.vue';\r\n\r\nexport default {\r\n  components: {\r\n    NavbarUser,\r\n  },\r\n  data() {\r\n    return {\r\n      riwayat: [\r\n        {\r\n          nama: \"John Doe\",\r\n          provinsi: \"Jawa Timur\",\r\n          kotaKabupaten: \"Kota Malang\",\r\n          status: \"Diproses\",\r\n          anggotaType: \"Provinsi\",  // Provinsi or Kota/Kabupaten\r\n        },\r\n        {\r\n          nama: \"John Doe\",\r\n          provinsi: \"Jawa Timur\",\r\n          kotaKabupaten: \"Kota Malang\",\r\n          status: \"Diproses\",\r\n          anggotaType: \"Provinsi\",\r\n        }\r\n      ],\r\n      isModalOpen: false,\r\n      selectedPendaftaran: null,\r\n    };\r\n  },\r\n  methods: {\r\n    getStatusClass(status) {\r\n      switch (status) {\r\n        case \"Diproses\": return \"text-yellow-500\";\r\n        case \"Disetujui\": return \"text-green-500\";\r\n        case \"Ditolak\": return \"text-red-500\";\r\n        default: return \"text-gray-500\";\r\n      }\r\n    },\r\n\r\n    // Open modal with selected user data\r\n    openModal(pendaftaran) {\r\n      this.selectedPendaftaran = pendaftaran;\r\n      this.isModalOpen = true;\r\n    },\r\n\r\n    // Close the modal\r\n    closeModal() {\r\n      this.isModalOpen = false;\r\n    },\r\n\r\n    // Method to handle ID card generation\r\n    async generateIdCard(pendaftaran) {\r\n      // Here you can call the method to generate ID card for either Provinsi or Kota/Kabupaten\r\n      if (pendaftaran.anggotaType === \"Provinsi\") {\r\n        console.log('Generate ID Card for Provinsi');\r\n        // Call your generate PDF method for Provinsi\r\n      } else if (pendaftaran.anggotaType === \"KotaKabupaten\") {\r\n        console.log('Generate ID Card for Kota/Kabupaten');\r\n        // Call your generate PDF method for Kota/Kabupaten\r\n      }\r\n      this.closeModal(); // Close modal after generation\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Styling untuk modal */\r\n.fixed {\r\n  position: fixed;\r\n}\r\n.bg-gray-600 {\r\n  background-color: rgba(0, 0, 0, 0.6);\r\n}\r\n.bg-opacity-50 {\r\n  opacity: 0.5;\r\n}\r\n</style>\r\n"],"mappings":"AAqDA,OAAOA,UAAS,MAAO,6BAA6B;AAEpD,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,CACP;QACEC,IAAI,EAAE,UAAU;QAChBC,QAAQ,EAAE,YAAY;QACtBC,aAAa,EAAE,aAAa;QAC5BC,MAAM,EAAE,UAAU;QAClBC,WAAW,EAAE,UAAU,CAAG;MAC5B,CAAC,EACD;QACEJ,IAAI,EAAE,UAAU;QAChBC,QAAQ,EAAE,YAAY;QACtBC,aAAa,EAAE,aAAa;QAC5BC,MAAM,EAAE,UAAU;QAClBC,WAAW,EAAE;MACf,EACD;MACDC,WAAW,EAAE,KAAK;MAClBC,mBAAmB,EAAE;IACvB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,cAAcA,CAACL,MAAM,EAAE;MACrB,QAAQA,MAAM;QACZ,KAAK,UAAU;UAAE,OAAO,iBAAiB;QACzC,KAAK,WAAW;UAAE,OAAO,gBAAgB;QACzC,KAAK,SAAS;UAAE,OAAO,cAAc;QACrC;UAAS,OAAO,eAAe;MACjC;IACF,CAAC;IAED;IACAM,SAASA,CAACC,WAAW,EAAE;MACrB,IAAI,CAACJ,mBAAkB,GAAII,WAAW;MACtC,IAAI,CAACL,WAAU,GAAI,IAAI;IACzB,CAAC;IAED;IACAM,UAAUA,CAAA,EAAG;MACX,IAAI,CAACN,WAAU,GAAI,KAAK;IAC1B,CAAC;IAE