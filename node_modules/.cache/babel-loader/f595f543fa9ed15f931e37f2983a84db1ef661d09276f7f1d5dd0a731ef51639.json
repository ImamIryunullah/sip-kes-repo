s.users.filter(user => user.id !== userId);\n        alert(\"Pendaftaran telah ditolak.\");\n      }\n    },\n    async deleteUser(userId) {\n      if (confirm(\"Apakah Anda yakin ingin menghapus user ini?\")) {\n        this.users = this.users.filter(user => user.id !== userId);\n        alert(\"User telah dihapus.\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["NavbarPusat","components","data","users","id","firstName","lastName","jenisPendaftaran","provinsi","kotaKabupaten","contactNo","emailUs","username","selectedFilter","loading","showModal","selectedUser","computed","filteredUsers","filter","user","methods","viewUser","approveUser","userId","confirm","alert","rejectUser","deleteUser"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\pusat\\RegisterPusatView.vue"],"sourcesContent":["<template>\r\n  <div class=\"w-screen min-h-screen flex flex-col bg-gray-100\">\r\n    <!-- Navbar -->\r\n    <NavbarPusat />\r\n\r\n    <!-- Content Wrapper -->\r\n    <div class=\"flex flex-col items-center px-10 py-6 mt-[90px] flex-grow\">\r\n      <div class=\"bg-white w-full max-w-4xl p-6 rounded-lg shadow-md\">\r\n        <h2 class=\"text-2xl font-semibold text-[#03a980] text-center mb-6\">Verifikasi Pendaftaran User</h2>\r\n        \r\n        <!-- Filter Jenis Pendaftaran -->\r\n        <div class=\"flex justify-between mb-4\">\r\n          <label class=\"text-gray-700\">Filter Jenis Pendaftaran:</label>\r\n          <select v-model=\"selectedFilter\" class=\"input-field w-1/3\">\r\n            <option value=\"\">Semua ({{ users.length }})</option>\r\n            <option value=\"Wilayah\">Wilayah ({{ users.filter(user => user.jenisPendaftaran === 'Wilayah').length }})</option>\r\n            <option value=\"Daerah\">Daerah ({{ users.filter(user => user.jenisPendaftaran === 'Daerah').length }})</option>\r\n          </select>\r\n        </div>\r\n\r\n        <!-- Loading Spinner -->\r\n        <div v-if=\"loading\" class=\"flex justify-center my-4\">\r\n          <span class=\"text-[#03a980] font-semibold\">Loading data...</span>\r\n        </div>\r\n\r\n        <!-- Tabel Pendaftaran -->\r\n        <table v-if=\"!loading && filteredUsers.length > 0\" class=\"w-full border-collapse border border-gray-300\">\r\n          <thead class=\"bg-[#03a980] text-white\">\r\n            <tr>\r\n              <th class=\"py-2 px-4 border\">Nama</th>\r\n              <th class=\"py-2 px-4 border\">Jenis</th>\r\n              <th class=\"py-2 px-4 border\">Provinsi</th>\r\n              <th class=\"py-2 px-4 border\">Kota/Kab</th>\r\n              <th class=\"py-2 px-4 border\">Aksi</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"user in filteredUsers\" :key=\"user.id\" class=\"border\">\r\n              <td class=\"py-2 px-4\">{{ user.firstName }} {{ user.lastName }}</td>\r\n              <td class=\"py-2 px-4\">{{ user.jenisPendaftaran }}</td>\r\n              <td class=\"py-2 px-4\">{{ user.provinsi }}</td>\r\n              <td class=\"py-2 px-4\">{{ user.kotaKabupaten || '-' }}</td>\r\n              <td class=\"py-2 px-4 flex gap-1\">\r\n  <button @click=\"viewUser(user)\" class=\"bg-blue-500 text-white px-2 py-1 text-sm rounded hover:bg-blue-700 transition\">üîç</button>\r\n  <button @click=\"approveUser(user.id)\" class=\"bg-green-500 text-white px-2 py-1 text-sm rounded hover:bg-green-700 transition\">‚úî</button>\r\n  <button @click=\"rejectUser(user.id)\" class=\"bg-red-500 text-white px-2 py-1 text-sm rounded hover:bg-red-700 transition\">‚úñ</button>\r\n  <button @click=\"deleteUser(user.id)\" class=\"bg-gray-500 text-white px-2 py-1 text-sm rounded hover:bg-gray-700 transition\">üóë</button>\r\n</td>\r\n\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n\r\n        <!-- Jika Tidak Ada Pendaftaran -->\r\n        <p v-if=\"!loading && filteredUsers.length === 0\" class=\"text-center text-gray-500 mt-4\">\r\n          Tidak ada pendaftaran yang perlu diverifikasi.\r\n        </p>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Modal Detail User -->\r\n    <div v-if=\"showModal\" class=\"fixed inset-0 bg-black bg-opacity-50 flex items-center justify-center\">\r\n      <div class=\"bg-white p-6 rounded-lg shadow-lg w-[400px]\">\r\n        <h3 class=\"text-lg font-semibold text-center mb-4\">Detail Pendaftar</h3>\r\n        <ul class=\"space-y-2\">\r\n          <li><strong>Nama:</strong> {{ selectedUser.firstName }} {{ selectedUser.lastName }}</li>\r\n          <li><strong>Jenis Pendaftaran:</strong> {{ selectedUser.jenisPendaftaran }}</li>\r\n          <li><strong>Provinsi:</strong> {{ selectedUser.provinsi }}</li>\r\n          <li><strong>Kota/Kabupaten:</strong> {{ selectedUser.kotaKabupaten || '-' }}</li>\r\n          <li><strong>Contact:</strong> {{ selectedUser.contactNo }}</li>\r\n          <li><strong>Email:</strong> {{ selectedUser.emailUs }}</li>\r\n          <li><strong>Username:</strong> {{ selectedUser.username }}</li>\r\n        </ul>\r\n        <div class=\"flex justify-end mt-4\">\r\n          <button @click=\"showModal = false\" class=\"bg-gray-500 text-white px-4 py-2 rounded hover:bg-gray-700 transition\">Tutup</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavbarPusat from '@/components/NavbarPusat.vue';\r\n\r\nexport default {\r\n  components: {\r\n    NavbarPusat\r\n  },\r\n  data() {\r\n    return {\r\n      users: [\r\n        { id: 1, firstName: \"Ahmad\", lastName: \"Hidayat\", jenisPendaftaran: \"Wilayah\", provinsi: \"Jawa Barat\", kotaKabupaten: \"\", contactNo: \"08123456789\", emailUs: \"ahmad@mail.com\", username: \"ahmad123\" },\r\n        { id: 2, firstName: \"Rina\", lastName: \"Sutomo\", jenisPendaftaran: \"Daerah\", provinsi: \"Jawa Tengah\", kotaKabupaten: \"Semarang\", contactNo: \"0812121212\", emailUs: \"rina@mail.com\", username: \"rina123\" },\r\n        { id: 3, firstName: \"Bagas\", lastName: \"Santoso\", jenisPendaftaran: \"Wilayah\", provinsi: \"Sumatera Barat\", kotaKabupaten: \"\", contactNo: \"08129876543\", emailUs: \"bagas@mail.com\", username: \"bagas99\" },\r\n      ],\r\n      selectedFilter: \"\", \r\n      loading: false, \r\n      showModal: false, \r\n      selectedUser: {} \r\n    };\r\n  },\r\n  computed: {\r\n    filteredUsers() {\r\n      if (this.selectedFilter) {\r\n        return this.users.filter(user => user.jenisPendaftaran === this.selectedFilter);\r\n      }\r\n      return this.users;\r\n    }\r\n  },\r\n  methods: {\r\n    viewUser(user) {\r\n      this.selectedUser = user;\r\n      this.showModal = true;\r\n    },\r\n    async approveUser(userId) {\r\n      if (confirm(\"Apakah Anda yakin ingin menyetujui user ini?\")) {\r\n        this.users = this.users.filter(user => user.id !== userId);\r\n        alert(\"Pendaftaran berhasil disetujui.\");\r\n      }\r\n    },\r\n    async rejectUser(userId) {\r\n      if (confirm(\"Apakah Anda yakin ingin menolak user ini?\")) {\r\n        this.users = this.users.filter(user => user.id !== userId);\r\n        alert(\"Pendaftaran telah ditolak.\");\r\n      }\r\n    },\r\n    async deleteUser(userId) {\r\n      if (confirm(\"Apakah Anda yakin ingin menghapus user ini?\")) {\r\n        this.users = this.users.filter(user => user.id !== userId);\r\n        alert(\"User telah dihapus.\");\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.input-field {\r\n  @apply w-full p-2 border border-gray-300 rounded-lg bg-gray-100 focus:outline-none;\r\n}\r\n\r\n@media (max-width: 768px) {\r\n  table {\r\n    font-size: 14px;\r\n  }\r\n  th, td {\r\n    padding: 8px;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";;AAkFA,OAAOA,WAAU,MAAO,8BAA8B;AAEtD,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,CACL;QAAEC,EAAE,EAAE,CAAC;QAAEC,SAAS,EAAE,OAAO;QAAEC,QAAQ,EAAE,SAAS;QAAEC,gBAAgB,EAAE,SAAS;QAAEC,QAAQ,EAAE,YAAY;QAAEC,aAAa,EAAE,EAAE;QAAEC,SAAS,EAAE,aAAa;QAAEC,