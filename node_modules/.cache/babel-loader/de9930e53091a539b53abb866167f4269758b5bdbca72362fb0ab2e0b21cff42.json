us }}</td>\r\n              <td class=\"py-2 px-4 flex gap-2\">\r\n                <button @click=\"openEditModal(sponsor)\" class=\"text-blue-500\">‚úèÔ∏è</button>\r\n                <button @click=\"openDeleteModal(sponsor)\" class=\"text-red-500\">üóëÔ∏è</button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n      </div>\r\n    </div>\r\n  \r\n    <!-- Modal Tambah/Edit Sponsor -->\r\n    <div v-if=\"showModal\" class=\"fixed inset-0 flex items-center justify-center bg-black bg-opacity-50\">\r\n      <div class=\"bg-white p-6 rounded-lg shadow-lg w-[500px] relative\">\r\n        <button @click=\"closeModal\" class=\"absolute top-2 right-2 text-gray-500\">‚úñ</button>\r\n        <h2 class=\"text-lg font-semibold mb-4\">{{ isEdit ? 'Edit Sponsor' : 'Tambah Sponsor' }}</h2>\r\n        <div class=\"flex flex-col gap-4\">\r\n          <input class=\"w-full h-[42px] bg-gray-100 border px-4 rounded\" v-model=\"modalSponsor.applicantName\" placeholder=\"Nama Pemohon\" />\r\n          <input class=\"w-full h-[42px] bg-gray-100 border px-4 rounded\" v-model=\"modalSponsor.applicantEmail\" placeholder=\"Email\" />\r\n          <input class=\"w-full h-[42px] bg-gray-100 border px-4 rounded\" v-model=\"modalSponsor.phoneNumber\" placeholder=\"Nomor Telepon\" />\r\n          <input class=\"w-full h-[42px] bg-gray-100 border px-4 rounded\" v-model=\"modalSponsor.sponsorName\" placeholder=\"Nama Sponsor\" />\r\n          <input type=\"number\" class=\"w-full h-[42px] bg-gray-100 border px-4 rounded\" v-model=\"modalSponsor.amountRequested\" placeholder=\"Jumlah Diminta\" />\r\n          <input class=\"w-full h-[42px] bg-gray-100 border px-4 rounded\" v-model=\"modalSponsor.sponsorshipType\" placeholder=\"Jenis Sponsorship\" />\r\n          <textarea class=\"w-full h-[100px] bg-gray-100 border px-4 rounded\" v-model=\"modalSponsor.justification\" placeholder=\"Justifikasi\"></textarea>\r\n        </div>\r\n        <div class=\"flex justify-between items-center mt-6\">\r\n          <button @click=\"closeModal\" class=\"w-[48%] h-[54px] bg-gray-300 rounded-md text-black font-bold\">CANCEL</button>\r\n          <button @click=\"saveSponsor\" class=\"w-[48%] h-[54px] bg-[#03a980] rounded-md text-white font-bold\">{{ isEdit ? 'UPDATE' : 'TAMBAH' }}</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import NavbarView from '@/components/NavbarView.vue';\r\n  \r\n  export default {\r\n    components: {\r\n      NavbarView,\r\n    },\r\n    data() {\r\n      return {\r\n        sponsorList: [\r\n          { id: 1, applicantName: 'John Doe', applicantEmail: 'john@example.com', phoneNumber: '1234567890', sponsorName: 'XYZ Corp', amountRequested: 5000, sponsorshipType: 'Gold', status: 'pending' },\r\n          { id: 2, applicantName: 'Jane Smith', applicantEmail: 'jane@example.com', phoneNumber: '0987654321', sponsorName: 'ABC Ltd', amountRequested: 3000, sponsorshipType: 'Silver', status: 'approved' },\r\n          { id: 3, applicantName: 'Alice Brown', applicantEmail: 'alice@example.com', phoneNumber: '1122334455', sponsorName: 'Tech Inc', amountRequested: 7000, sponsorshipType: 'Platinum', status: 'rejected' }\r\n        ],\r\n        showModal: false,\r\n        isEdit: false,\r\n        modalSponsor: {},\r\n      };\r\n    },\r\n    methods: {\r\n      openAddModal() {\r\n        this.isEdit = false;\r\n        this.modalSponsor = {};\r\n        this.showModal = true;\r\n      },\r\n      openEditModal(sponsor) {\r\n        this.isEdit = true;\r\n        this.modalSponsor = { ...sponsor };\r\n        this.showModal = true;\r\n      },\r\n      closeModal() {\r\n        this.showModal = false;\r\n      },\r\n      saveSponsor() {\r\n        if (this.isEdit) {\r\n          alert('Sponsor berhasil diperbarui!');\r\n        } else {\r\n          alert('Sponsor berhasil ditambahkan!');\r\n        }\r\n        this.closeModal();\r\n      },\r\n    },\r\n  };\r\n 