{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.for-each.js\";\nimport NavbarLandingPage from '@/components/NavbarLandingPage.vue';\nimport FooterLandingPage from '@/components/FooterLandingPage.vue';\nexport default {\n  components: {\n    NavbarLandingPage,\n    FooterLandingPage\n  },\n  data() {\n    return {\n      isMounted: false,\n      form: {\n        nama: '',\n        email: '',\n        wilayah: '',\n        daerah: '',\n        kategori: '',\n        judul: '',\n        deskripsi: '',\n        harapan: '',\n        tindakLanjut: false\n      },\n      file: null,\n      fileName: ''\n    };\n  },\n  mounted() {\n    this.isMounted = true;\n  },\n  methods: {\n    handleFileUpload(event) {\n      const selectedFile = event.target.files[0];\n      if (selectedFile) {\n        this.file = selectedFile;\n        this.fileName = selectedFile.name;\n      }\n    },\n    async submitForm() {\n      if (!this.validateForm()) {\n        alert('Mohon lengkapi semua data dengan benar!');\n        return;\n      }\n      const formData = new FormData();\n      Object.keys(this.form).forEach(key => {\n        formData.append(key, this.form[key]);\n      });\n      if (this.file) {\n        formData.append('lampiran', this.file);\n      }\n      try {\n        const response = await fetch('', {\n          method: 'POST',\n          body: formData\n        });\n        if (response.ok) {\n          alert('Pengaduan berhasil dikirim!');\n          this.resetForm();\n        } else {\n          alert('Terjadi kesalahan, silakan coba lagi.');\n        }\n      } catch (error) {\n        console.error('Error:', error);\n        alert('Gagal mengirim pengaduan.');\n      }\n    },\n    validateForm() {\n      return this.form.nama && this.form.email && this.form.wilayah && this.form.daerah && this.form.kategori && this.form.judul && this.form.deskripsi && this.form.harapan;\n    },\n    resetForm() {\n      this.form = {\n        nama: '',\n        email: '',\n        wilayah: '',\n        daerah: '',\n        kategori: '',\n        judul: '',\n        deskripsi: '',\n        harapan: '',\n        tindakLanjut: false\n      };\n      this.file = null;\n      this.fileName = '';\n    }\n  }\n};","map":{"version":3,"names":["NavbarLandingPage","FooterLandingPage","components","data","isMounted","form","nama","email","wilayah","daerah","kategori","judul","deskripsi","harapan","tindakLanjut","file","fileName","mounted","methods","handleFileUpload","event","selectedFile","target","files","name","submitForm","validateForm","alert","formData","FormData","Object","keys","forEach","key","append","response","fetch","method","body","ok","resetForm","error","console"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\lpkni\\FormPengaduanLpkni.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"isMounted\" class=\"w-screen min-h-screen flex flex-col bg-red-100\">\r\n    <NavbarLandingPage />\r\n\r\n    <!-- Form Container -->\r\n    <div class=\"flex flex-col items-center p-6 flex-grow mt-10\">\r\n      <div class=\"bg-white w-full max-w-screen-md p-6 rounded-lg shadow-lg animate-fadeInUp\">\r\n        <h2 class=\"text-3xl font-semibold text-red-600 text-center mb-6\">Form Pengaduan</h2>\r\n        <form @submit.prevent=\"submitForm\" class=\"space-y-6\">\r\n          <!-- Nama Pengadu -->\r\n          <div class=\"flex items-center space-x-4\">\r\n            <label for=\"nama\" class=\"block text-sm font-semibold text-red-700 w-1/3\">Nama Pengadu</label>\r\n            <input id=\"nama\" v-model=\"form.nama\" type=\"text\" required class=\"input-field w-2/3\" placeholder=\"Nama Pengadu\" />\r\n          </div>\r\n\r\n          <!-- Email -->\r\n          <div class=\"flex items-center space-x-4\">\r\n            <label for=\"email\" class=\"block text-sm font-semibold text-red-700 w-1/3\">Email</label>\r\n            <input id=\"email\" v-model=\"form.email\" type=\"email\" required class=\"input-field w-2/3\" placeholder=\"Email Anda\" />\r\n          </div>\r\n\r\n          <!-- Wilayah -->\r\n          <div class=\"flex items-center space-x-4\">\r\n            <label for=\"wilayah\" class=\"block text-sm font-semibold text-red-700 w-1/3\">Wilayah/Provinsi</label>\r\n            <input id=\"wilayah\" v-model=\"form.wilayah\" type=\"text\" required class=\"input-field w-2/3\" placeholder=\"Provinsi Anda\" />\r\n          </div>\r\n\r\n          <!-- Daerah -->\r\n          <div class=\"flex items-center space-x-4\">\r\n            <label for=\"daerah\" class=\"block text-sm font-semibold text-red-700 w-[100px]\">Daerah</label>\r\n            <input id=\"daerah\" v-model=\"form.daerah\" type=\"text\" required class=\"input-field w-2/3\" placeholder=\"Kota atau Kabupaten\" />\r\n          </div>\r\n\r\n          <!-- Kategori Pengaduan -->\r\n          <div class=\"flex items-center space-x-4\">\r\n            <label for=\"kategori\" class=\"block text-sm font-semibold text-red-700 w-1/3\">Kategori Pengaduan</label>\r\n            <select id=\"kategori\" v-model=\"form.kategori\" required class=\"input-field w-2/3\">\r\n              <option disabled value=\"\">Pilih Kategori</option>\r\n              <option value=\"Layanan Pendidikan\">Layanan Pendidikan</option>\r\n              <option value=\"Sertifikasi\">Sertifikasi</option>\r\n              <option value=\"Fasilitas\">Fasilitas</option>\r\n              <option value=\"Masalah Administrasi\">Masalah Administrasi</option>\r\n              <option value=\"Lainnya\">Lainnya</option>\r\n            </select>\r\n          </div>\r\n\r\n          <!-- Judul Pengaduan -->\r\n          <div class=\"flex items-center space-x-4\">\r\n            <label for=\"judul\" class=\"block text-sm font-semibold text-red-700 w-1/3\">Judul Pengaduan</label>\r\n            <input id=\"judul\" v-model=\"form.judul\" type=\"text\" required class=\"input-field w-2/3\" placeholder=\"Judul Pengaduan\" />\r\n          </div>\r\n\r\n          <!-- Deskripsi Pengaduan -->\r\n          <div class=\"flex items-center space-x-4\">\r\n            <label for=\"deskripsi\" class=\"block text-sm font-semibold text-red-700 w-1/3\">Deskripsi Pengaduan</label>\r\n            <textarea id=\"deskripsi\" v-model=\"form.deskripsi\" required class=\"input-field w-2/3\" placeholder=\"Jelaskan pengaduan Anda\" rows=\"4\"></textarea>\r\n          </div>\r\n\r\n          <!-- Lampiran -->\r\n          <div class=\"flex items-center space-x-4\">\r\n            <label for=\"lampiran\" class=\"block text-sm font-semibold text-red-700 w-1/3\">Lampiran / Bukti</label>\r\n            <input id=\"lampiran\" type=\"file\" @change=\"handleFileUpload\" class=\"input-field w-2/3\" />\r\n            <p v-if=\"fileName\" class=\"text-sm text-red-700 mt-2\">File yang dipilih: {{ fileName }}</p>\r\n          </div>\r\n\r\n          <!-- Harapan Penyelesaian -->\r\n          <div class=\"flex items-center space-x-4\">\r\n            <label for=\"harapan\" class=\"block text-sm font-semibold text-red-700 w-1/3\">Harapan Penyelesaian</label>\r\n            <input id=\"harapan\" v-model=\"form.harapan\" type=\"text\" required class=\"input-field w-2/3\" placeholder=\"Harapan Penyelesaian\" />\r\n          </div>\r\n\r\n          <!-- Tindak Lanjut -->\r\n          <div class=\"flex items-center space-x-4\">\r\n            <input type=\"checkbox\" id=\"tindakLanjut\" v-model=\"form.tindakLanjut\" class=\"mr-2\" />\r\n            <label for=\"tindakLanjut\" class=\"text-sm font-semibold text-red-700\">\r\n              Apakah Anda ingin dihubungi untuk tindak lanjut?\r\n            </label>\r\n          </div>\r\n\r\n          <!-- Submit Button -->\r\n          <button type=\"submit\" class=\"w-full bg-red-600 text-white p-3 rounded-lg font-semibold hover:bg-red-700 transition-transform transform hover:scale-105\">\r\n            Kirim Pengaduan\r\n          </button>\r\n        </form>\r\n      </div>\r\n    </div>\r\n\r\n    <FooterLandingPage />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavbarLandingPage from '@/components/NavbarLandingPage.vue';\r\nimport FooterLandingPage from '@/components/FooterLandingPage.vue';\r\n\r\nexport default {\r\n  components: {\r\n    NavbarLandingPage,\r\n    FooterLandingPage\r\n  },\r\n  data() {\r\n    return {\r\n      isMounted: false,\r\n      form: {\r\n        nama: '',\r\n        email: '',\r\n        wilayah: '',\r\n        daerah: '',\r\n        kategori: '',\r\n        judul: '',\r\n        deskripsi: '',\r\n        harapan: '',\r\n        tindakLanjut: false,\r\n      },\r\n      file: null,\r\n      fileName: '',\r\n    };\r\n  },\r\n  mounted() {\r\n    this.isMounted = true;\r\n  },\r\n  methods: {\r\n    handleFileUpload(event) {\r\n      const selectedFile = event.target.files[0];\r\n      if (selectedFile) {\r\n        this.file = selectedFile;\r\n        this.fileName = selectedFile.name;\r\n      }\r\n    },\r\n    async submitForm() {\r\n      if (!this.validateForm()) {\r\n        alert('Mohon lengkapi semua data dengan benar!');\r\n        return;\r\n      }\r\n      \r\n      const formData = new FormData();\r\n      Object.keys(this.form).forEach(key => {\r\n        formData.append(key, this.form[key]);\r\n      });\r\n      if (this.file) {\r\n        formData.append('lampiran', this.file);\r\n      }\r\n      \r\n      try {\r\n        const response = await fetch('', {\r\n          method: 'POST',\r\n          body: formData\r\n        });\r\n        if (response.ok) {\r\n          alert('Pengaduan berhasil dikirim!');\r\n          this.resetForm();\r\n        } else {\r\n          alert('Terjadi kesalahan, silakan coba lagi.');\r\n        }\r\n      } catch (error) {\r\n        console.error('Error:', error);\r\n        alert('Gagal mengirim pengaduan.');\r\n      }\r\n    },\r\n    validateForm() {\r\n      return (\r\n        this.form.nama &&\r\n        this.form.email &&\r\n        this.form.wilayah &&\r\n        this.form.daerah &&\r\n        this.form.kategori &&\r\n        this.form.judul &&\r\n        this.form.deskripsi &&\r\n        this.form.harapan\r\n      );\r\n    },\r\n    resetForm() {\r\n      this.form = {\r\n        nama: '',\r\n        email: '',\r\n        wilayah: '',\r\n        daerah: '',\r\n        kategori: '',\r\n        judul: '',\r\n        deskripsi: '',\r\n        harapan: '',\r\n        tindakLanjut: false,\r\n      };\r\n      this.file = null;\r\n      this.fileName = '';\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.input-field {\r\n  width: 100%;\r\n  padding: 0.75rem;\r\n  border-radius: 0.375rem;\r\n  border: 1px solid #e5e7eb;\r\n  outline: none;\r\n  transition: border-color 0.2s;\r\n}\r\n.input-field:focus {\r\n  border-color: #ef4444;\r\n}\r\n</style>\r\n"],"mappings":";;AA4FA,OAAOA,iBAAgB,MAAO,oCAAoC;AAClE,OAAOC,iBAAgB,MAAO,oCAAoC;AAElE,eAAe;EACbC,UAAU,EAAE;IACVF,iBAAiB;IACjBC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,KAAK;MAChBC,IAAI,EAAE;QACJC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE,EAAE;QACXC,MAAM,EAAE,EAAE;QACVC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,SAAS,EAAE,EAAE;QACbC,OAAO,EAAE,EAAE;QACXC,YAAY,EAAE;MAChB,CAAC;MACDC,IAAI,EAAE,IAAI;MACVC,QAAQ,EAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACb,SAAQ,GAAI,IAAI;EACvB,CAAC;EACDc,OAAO,EAAE;IACPC,gBAAgBA,CAACC,KAAK,EAAE;MACtB,MAAMC,YAAW,GAAID,KAAK,CAACE,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;MAC1C,IAAIF,YAAY,EAAE;QAChB,IAAI,CAACN,IAAG,GAAIM,YAAY;QACxB,IAAI,CAACL,QAAO,GAAIK,YAAY,CAACG,IAAI;MACnC;IACF,CAAC;IACD,MAAMC,UAAUA,CAAA,EAAG;MACjB,IAAI,CAAC,IAAI,CAACC,YAAY,CAAC,CAAC,EAAE;QACxBC,KAAK,CAAC,yCAAyC,CAAC;QAChD;MACF;MAEA,MAAMC,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC/BC,MAAM,CAACC,IAAI,CAAC,IAAI,CAAC1B,IAAI,CAAC,CAAC2B,OAAO,CAACC,GAAE,IAAK;QACpCL,QAAQ,CAACM,MAAM,CAACD,GAAG,EAAE,IAAI,CAAC5B,IAAI,CAAC4B,GAAG,CAAC,CAAC;MACtC,CAAC,CAAC;MACF,IAAI,IAAI,CAAClB,IAAI,EAAE;QACba,QAAQ,CAACM,MAAM,CAAC,UAAU,EAA