{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport NavbarPusat from '@/components/NavbarPusat.vue';\nexport default {\n  components: {\n    NavbarPusat\n  },\n  data() {\n    return {\n      loading: false,\n      pengajuanList: [...Array(4)].map((_, i) => ({\n        id: i + 1,\n        nama: `User ${i + 1}`,\n        jenisSertifikat: [\"Keahlian\", \"Pelatihan\"][i % 2],\n        instansi: `Instansi ${i + 1}`,\n        status: \"Pending\"\n      }))\n    };\n  },\n  methods: {\n    approvePengajuan(id) {\n      const pengajuan = this.pengajuanList.find(p => p.id === id);\n      if (pengajuan) pengajuan.status = \"Disetujui\";\n    },\n    rejectPengajuan(id) {\n      const pengajuan = this.pengajuanList.find(p => p.id === id);\n      if (pengajuan) pengajuan.status = \"Ditolak\";\n    }\n  }\n};","map":{"version":3,"names":["NavbarPusat","components","data","loading","pengajuanList","Array","map","_","i","id","nama","jenisSertifikat","instansi","status","methods","approvePengajuan","pengajuan","find","p","rejectPengajuan"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\pusat\\PengajuanSertifikatPusat.vue"],"sourcesContent":["<template>\r\n    <div class=\"w-screen min-h-screen flex flex-col bg-gray-100\">\r\n      <!-- Navbar -->\r\n      <NavbarPusat />\r\n  \r\n      <!-- Content Wrapper -->\r\n      <div class=\"flex flex-col items-center px-10 py-6 mt-[100px] flex-grow\">\r\n        <div class=\"bg-white w-full max-w-5xl p-6 rounded-lg shadow-md\">\r\n          <h2 class=\"text-2xl font-semibold text-[#03a980] text-center mb-6\">Kelola Pengajuan Sertifikat</h2>\r\n  \r\n          <!-- Loading -->\r\n          <div v-if=\"loading\" class=\"flex justify-center my-4\">\r\n            <span class=\"text-[#03a980] font-semibold\">Loading data...</span>\r\n          </div>\r\n  \r\n          <!-- Tabel Data Pengajuan -->\r\n          <table v-if=\"!loading && pengajuanList.length > 0\" class=\"w-full border-collapse border border-gray-300\">\r\n            <thead class=\"bg-[#03a980] text-white\">\r\n              <tr>\r\n                <th class=\"py-2 px-4 border\">Nama</th>\r\n                <th class=\"py-2 px-4 border\">Jenis Sertifikat</th>\r\n                <th class=\"py-2 px-4 border\">Instansi</th>\r\n                <th class=\"py-2 px-4 border\">Status</th>\r\n                <th class=\"py-2 px-4 border\">Aksi</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <tr v-for=\"pengajuan in pengajuanList\" :key=\"pengajuan.id\" class=\"border\">\r\n                <td class=\"py-2 px-4\">{{ pengajuan.nama }}</td>\r\n                <td class=\"py-2 px-4\">{{ pengajuan.jenisSertifikat }}</td>\r\n                <td class=\"py-2 px-4\">{{ pengajuan.instansi }}</td>\r\n                <td class=\"py-2 px-4 font-semibold\" :class=\"getStatusClass(pengajuan.status)\">\r\n                  {{ pengajuan.status }}\r\n                </td>\r\n                <td class=\"py-2 px-4 flex gap-2\">\r\n                  <button \r\n                    @click=\"approvePengajuan(pengajuan.id)\" \r\n                    :disabled=\"pengajuan.status !== 'Pending'\"\r\n                    class=\"bg-green-500 text-white px-2 py-1 text-xs rounded hover:bg-green-700 transition disabled:opacity-50\">\r\n                    ✔ Terima\r\n                  </button>\r\n                  <button \r\n                    @click=\"rejectPengajuan(pengajuan.id)\" \r\n                    :disabled=\"pengajuan.status !== 'Pending'\"\r\n                    class=\"bg-red-500 text-white px-2 py-1 text-xs rounded hover:bg-red-700 transition disabled:opacity-50\">\r\n                    ✖ Tolak\r\n                  </button>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n  \r\n          <!-- Jika Tidak Ada Pengajuan -->\r\n          <p v-if=\"!loading && pengajuanList.length === 0\" class=\"text-center text-gray-500 mt-4\">\r\n            Tidak ada pengajuan sertifikat saat ini.\r\n          </p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import NavbarPusat from '@/components/NavbarPusat.vue';\r\n  \r\n  export default {\r\n    components: {\r\n      NavbarPusat\r\n    },\r\n    data() {\r\n      return {\r\n        loading: false,\r\n        pengajuanList: [...Array(4)].map((_, i) => ({\r\n          id: i + 1,\r\n          nama: `User ${i + 1}`,\r\n          jenisSertifikat: [\"Keahlian\", \"Pelatihan\"][i % 2],\r\n          instansi: `Instansi ${i + 1}`,\r\n          status: \"Pending\"\r\n        }))\r\n      };\r\n    },\r\n    methods: {\r\n      approvePengajuan(id) {\r\n        const pengajuan = this.pengajuanList.find(p => p.id === id);\r\n        if (pengajuan) pengajuan.status = \"Disetujui\";\r\n      },\r\n      rejectPengajuan(id) {\r\n        const pengajuan = this.pengajuanList.find(p => p.id === id);\r\n        if (pengajuan) pengajuan.status = \"Ditolak\";\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  "],"mappings":";;;AA8DE,OAAOA,WAAU,MAAO,8BAA8B;AAEtD,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,KAAK;MACdC,aAAa,EAAE,CAAC,GAAGC,KAAK,CAAC,CAAC,CAAC,CAAC,CAACC,GAAG,CAAC,CAACC,CAAC,EAAEC,CAAC,MAAM;QAC1CC,EAAE,EAAED,CAAA,GAAI,CAAC;QACTE,IAAI,EAAE,QAAQF,CAAA,GAAI,CAAC,EAAE;QACrBG,eAAe,EAAE,CAAC,UAAU,EAAE,WAAW,CAAC,CAACH,CAAA,GAAI,CAAC,CAAC;QACjDI,QAAQ,EAAE,YAAYJ,CAAA,GAAI,CAAC,EAAE;QAC7BK,MAAM,EAAE;MACV,CAAC,CAAC;IACJ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,gBAAgBA,CAACN,EAAE,EAAE;MACnB,MAAMO,SAAQ,GAAI,IAAI,CAACZ,aAAa,CAACa,IAAI,CAACC,CAAA,IAAKA,CAAC,CAACT,EAAC,KAAMA,EAAE,CAAC;MAC3D,IAAIO,SAAS,EAAEA,SAAS,CAACH,MAAK,GAAI,WAAW;IAC/C,CAAC;IACDM,eAAeA,CAACV,EAAE,EAAE;MAClB,MAAMO,SAAQ,GAAI,IAAI,CAACZ,aAAa,CAACa,IAAI,CAACC,CAAA,IAAKA,CAAC,CAACT,EAAC,KAAMA,EAAE,CAAC;MAC3D,IAAIO,SA