{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport NavbarPusat from '@/components/NavbarPusat.vue';\nexport default {\n  components: {\n    NavbarUser\n  },\n  data() {\n    return {\n      pendaftaranData: [] // Data pendaftaran anggota yang akan ditampilkan\n    };\n  },\n  mounted() {\n    this.fetchPendaftaranData(); // Ambil data pendaftaran anggota saat halaman dimuat\n  },\n  methods: {\n    async fetchPendaftaranData() {\n      try {\n        // Ambil data pendaftaran dari API atau server\n        const response = await fetch('/api/pusat/pendaftaran');\n        if (response.ok) {\n          const data = await response.json();\n          this.pendaftaranData = data.map(item => ({\n            ...item,\n            foto3x4Url: URL.createObjectURL(item.foto3x4),\n            // Membuat URL untuk foto 3x4\n            fotoKtpUrl: URL.createObjectURL(item.fotoKtp) // Membuat URL untuk foto KTP\n          }));\n        } else {\n          alert(\"Gagal mengambil data pendaftaran.\");\n        }\n      } catch (error) {\n        console.error(\"Error fetching data:\", error);\n        alert(\"Terjadi kesalahan saat mengambil data.\");\n      }\n    },\n    approvePendaftaran(pendaftaran) {\n      // Proses persetujuan pendaftaran anggota\n      alert(`Pendaftaran ${pendaftaran.nama} telah disetujui.`);\n    },\n    rejectPendaftaran(pendaftaran) {\n      // Proses penolakan pendaftaran anggota\n      alert(`Pendaftaran ${pendaftaran.nama} telah ditolak.`);\n    }\n  }\n};","map":{"version":3,"names":["NavbarPusat","components","NavbarUser","data","pendaftaranData","mounted","fetchPendaftaranData","methods","response","fetch","ok","json","map","item","foto3x4Url","URL","createObjectURL","foto3x4","fotoKtpUrl","fotoKtp","alert","error","console","approvePendaftaran","pendaftaran","nama","rejectPendaftaran"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\pusat\\RegisterPusatView.vue"],"sourcesContent":["<template>\r\n  <div class=\"w-screen h-screen flex flex-col mt-10\">\r\n    <NavbarPusat />\r\n    <div class=\"flex flex-col items-center p-10 bg-gray-100 flex-grow\">\r\n      <div class=\"bg-white w-full max-w-4xl p-6 rounded-lg shadow-md\">\r\n        <h2 class=\"text-2xl font-semibold text-[#03a980] text-center mb-6\">Data Pendaftaran Anggota</h2>\r\n\r\n        <!-- Tabel untuk menampilkan data pendaftaran anggota -->\r\n        <div v-if=\"pendaftaranData.length > 0\">\r\n          <table class=\"min-w-full table-auto\">\r\n            <thead>\r\n              <tr class=\"border-b\">\r\n                <th class=\"px-4 py-2 text-left\">Nama</th>\r\n                <th class=\"px-4 py-2 text-left\">NIK</th>\r\n                <th class=\"px-4 py-2 text-left\">Provinsi</th>\r\n                <th class=\"px-4 py-2 text-left\">Kota/Kabupaten</th>\r\n                <th class=\"px-4 py-2 text-left\">Instansi</th>\r\n                <th class=\"px-4 py-2 text-left\">Status</th>\r\n                <th class=\"px-4 py-2 text-left\">Foto 3x4</th>\r\n                <th class=\"px-4 py-2 text-left\">Foto KTP</th>\r\n                <th class=\"px-4 py-2 text-left\">Aksi</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <tr v-for=\"(pendaftaran, index) in pendaftaranData\" :key=\"index\" class=\"border-b\">\r\n                <td class=\"px-4 py-2\">{{ pendaftaran.nama }}</td>\r\n                <td class=\"px-4 py-2\">{{ pendaftaran.nik }}</td>\r\n                <td class=\"px-4 py-2\">{{ pendaftaran.provinsi }}</td>\r\n                <td class=\"px-4 py-2\">{{ pendaftaran.kotaKabupaten }}</td>\r\n                <td class=\"px-4 py-2\">{{ pendaftaran.instansi }}</td>\r\n                <td class=\"px-4 py-2\">{{ pendaftaran.status }}</td>\r\n                <td class=\"px-4 py-2\">\r\n                  <img :src=\"pendaftaran.foto3x4Url\" alt=\"Foto 3x4\" class=\"w-12 h-12 object-cover rounded-full\" />\r\n                </td>\r\n                <td class=\"px-4 py-2\">\r\n                  <img :src=\"pendaftaran.fotoKtpUrl\" alt=\"Foto KTP\" class=\"w-12 h-12 object-cover rounded-full\" />\r\n                </td>\r\n                <td class=\"px-4 py-2\">\r\n                  <button @click=\"approvePendaftaran(pendaftaran)\" class=\"bg-green-500 text-white px-4 py-2 rounded-md\">Setujui</button>\r\n                  <button @click=\"rejectPendaftaran(pendaftaran)\" class=\"bg-red-500 text-white px-4 py-2 rounded-md\">Tolak</button>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n        <div v-else class=\"text-center text-gray-500\">Tidak ada data pendaftaran anggota.</div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavbarPusat from '@/components/NavbarPusat.vue';\r\n\r\nexport default {\r\n  components: {\r\n    NavbarUser\r\n  },\r\n  data() {\r\n    return {\r\n      pendaftaranData: [] // Data pendaftaran anggota yang akan ditampilkan\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchPendaftaranData(); // Ambil data pendaftaran anggota saat halaman dimuat\r\n  },\r\n  methods: {\r\n    async fetchPendaftaranData() {\r\n      try {\r\n        // Ambil data pendaftaran dari API atau server\r\n        const response = await fetch('/api/pusat/pendaftaran');\r\n        if (response.ok) {\r\n          const data = await response.json();\r\n          this.pendaftaranData = data.map(item => ({\r\n            ...item,\r\n            foto3x4Url: URL.createObjectURL(item.foto3x4), // Membuat URL untuk foto 3x4\r\n            fotoKtpUrl: URL.createObjectURL(item.fotoKtp), // Membuat URL untuk foto KTP\r\n          }));\r\n        } else {\r\n          alert(\"Gagal mengambil data pendaftaran.\");\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Error fetching data:\", error);\r\n        alert(\"Terjadi kesalahan saat mengambil data.\");\r\n      }\r\n    },\r\n    approvePendaftaran(pendaftaran) {\r\n      // Proses persetujuan pendaftaran anggota\r\n      alert(`Pendaftaran ${pendaftaran.nama} telah disetujui.`);\r\n    },\r\n    rejectPendaftaran(pendaftaran) {\r\n      // Proses penolakan pendaftaran anggota\r\n      alert(`Pendaftaran ${pendaftaran.nama} telah ditolak.`);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Styling untuk tabel */\r\ntable {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n}\r\nth, td {\r\n  text-align: left;\r\n  padding: 12px;\r\n}\r\nth {\r\n  background-color: #028a66;\r\n  color: white;\r\n}\r\ntr:hover {\r\n  background-color: #f5f5f5;\r\n}\r\nbutton {\r\n  margin-right: 10px;\r\n}\r\n</style>\r\n"],"mappings":";;;;;AAoDA,OAAOA,WAAU,MAAO,8BAA8B;AAEtD,eAAe;EACbC,UAAU,EAAE;IACVC;EACF,CAAC;EACDC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,eAAe,EAAE,EAAC,CAAE;IACtB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,oBAAoB,CAAC,CAAC,EAAE;EAC/B,CAAC;EACDC,OAAO,EAAE;IACP,MAAMD,oBAAoBA,CAAA,EAAG;MAC3B,IAAI;QACF;QACA,MAAME,QAAO,GAAI,MAAMC,KAAK,CAAC,wBAAwB,CAAC;QACtD,IAAID,QAAQ,CAACE,EAAE,EAAE;UACf,MAAMP,IAAG,GAAI,MAAMK,QAAQ,CAACG,IAAI,CAAC,CAAC;UAClC,IAAI,CAACP,eAAc,GAAID,IAAI,CAACS,GAAG,CAACC,IAAG,KAAM;YACvC,GAAGA,IAAI;YACPC,UAAU,EAAEC,GAAG,CAACC,eAAe,CAACH,IAAI,CAACI,OAAO,CAAC;YAAE;YAC/CC,UAAU,EAAEH,GAAG,CAACC,eAAe,CAACH,IAAI,CAACM,OAAO,CAAC,CAAE;UACjD,CAAC,CAAC,CAAC;QACL,OAAO;UACLC,KAAK,CAAC,mCAAmC,CAAC;QAC5C;MACF,EAAE,OAAOC,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,sBAAsB,EAAEA,KAAK,CAAC;QAC5CD,KAAK,CAAC,wCAAwC,CAAC;MACjD;IACF,CAAC;IACDG,kBAAkBA,CAACC,WAAW,EAAE;MAC9B;MACAJ,KAAK,CAAC,eAAeI,WAAW,CAACC,IAAI,mBAAmB,CAAC;IAC3D,CAAC;IACDC,iBAAiBA,CAACF,WAAW,EAAE;MAC7B;MACAJ,KAAK,CAAC,eAAeI,WAAW,CAACC,IAAI,iBAAiB,CAAC;IACzD;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"s