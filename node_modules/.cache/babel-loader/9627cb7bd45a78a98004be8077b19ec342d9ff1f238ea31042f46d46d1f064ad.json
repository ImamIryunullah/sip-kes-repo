{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport NavbarPusat from \"@/components/NavbarPusat.vue\";\nimport ApiService from \"@/service/api.js\";\nexport default {\n  components: {\n    NavbarPusat\n  },\n  data() {\n    return {\n      categories: [\"Politik\", \"Olahraga\", \"Teknologi\", \"Nasional\", \"Internasional\"],\n      newsList: [],\n      currentPage: 1,\n      itemsPerPage: 5,\n      showModal: false,\n      isEdit: false,\n      modalNews: {\n        id: null,\n        title: \"\",\n        description: \"\",\n        date: \"\",\n        category: \"\",\n        gambar: null\n      },\n      selectedImage: null,\n      selectedCategory: \"\",\n      errorMessage: \"\"\n    };\n  },\n  mounted() {\n    this.getNews();\n  },\n  computed: {\n    totalPages() {\n      return Math.ceil(this.filteredNews.length / this.itemsPerPage);\n    },\n    paginatedNews() {\n      const start = (this.currentPage - 1) * this.itemsPerPage;\n      return this.filteredNews.slice(start, start + this.itemsPerPage);\n    },\n    filteredNews() {\n      return this.newsList.filter(news => !this.selectedCategory || news.category === this.selectedCategory);\n    }\n  },\n  methods: {\n    getNews() {\n      ApiService.getBerita().then(response => {\n        this.newsList = response.data;\n      }).catch(error => {\n        this.errorMessage = \"Failed to load news, please try again later.\";\n        console.error(error);\n      });\n    },\n    openAddModal() {\n      this.isEdit = false;\n      this.modalNews = {\n        id: null,\n        title: \"\",\n        description: \"\",\n        date: \"\",\n        category: \"\",\n        gambar: null\n      };\n      this.showModal = true;\n    },\n    openEditModal(news) {\n      this.isEdit = true;\n      this.modalNews = {\n        ...news\n      };\n      this.showModal = true;\n    },\n    closeModal() {\n      this.showModal = false;\n    },\n    saveNews() {\n      const formData = {\n        title: this.modalNews.title,\n        description: this.modalNews.description,\n        date: this.modalNews.date,\n        category: this.modalNews.category,\n        jumlah_pengunjung: 0\n      };\n      const action = this.isEdit ? ApiService.updateBerita(this.modalNews.id, formData) : ApiService.createBerita(formData);\n      action.then(response => {\n        if (this.selectedImage && response.data.id) {\n          const imageFormData = new FormData();\n          imageFormData.append(\"gambar\", this.selectedImage);\n          ApiService.addGambarBerita(response.data.id, imageFormData).then(() => {\n            this.getNews();\n            this.closeModal();\n          }).catch(error => {\n            console.error(\"Failed to upload image:\", error);\n          });\n        } else {\n          this.getNews();\n          this.closeModal();\n        }\n      }).catch(error => {\n        this.errorMessage = \"Failed to save news, please try again.\";\n        console.error(error);\n      });\n    },\n    deleteNews(id) {\n      ApiService.deleteBerita(id).then(() => {\n        this.getNews();\n      }).catch(error => {\n        this.errorMessage = \"Failed to delete news, please try again.\";\n        console.error(error);\n      });\n    },\n    deleteGambar(beritaId, gambarId) {\n      ApiService.deleteGambarBerita(beritaId, gambarId).then(() => {\n        this.getNews();\n      }).catch(error => {\n        console.error(\"Failed to delete image:\", error);\n      });\n    },\n    handleFileChange(event) {\n      this.selectedImage = event.target.files[0];\n    },\n    setActiveCategory(category) {\n      this.selectedCategory = category;\n    },\n    nextPage() {\n      if (this.currentPage < this.totalPages) {\n        this.currentPage++;\n      }\n    },\n    prevPage() {\n      if (this.currentPage > 1) {\n        this.currentPage--;\n      }\n    }\n  }\n};","map":{"version":3,"names":["NavbarPusat","ApiService","components","data","categories","newsList","currentPage","itemsPerPage","showModal","isEdit","modalNews","id","title","description","date","category","gambar","selectedImage","selectedCategory","errorMessage","mounted","getNews","computed","totalPages","Math","ceil","filteredNews","length","paginatedNews","start","slice","filter","news","methods","getBerita","then","response","catch","error","console","openAddModal","openEditModal","closeModal","saveNews","formData","jumlah_pengunjung","action","updateBerita","createBerita","imageFormData","FormData","append","addGambarBerita","deleteNews","deleteBerita","deleteGambar","beritaId","gambarId","deleteGambarBerita","handleFileChange","event","target","files","setActiveCategory","nextPage","prevPage"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\pusat\\KelolaBeritaView.vue"],"sourcesContent":["<template>\r\n  <div class=\"w-screen min-h-screen bg-gray-100\">\r\n    <!-- Navbar -->\r\n    <NavbarPusat />\r\n\r\n    <!-- Filter Kategori -->\r\n    <div class=\"flex justify-center space-x-4 mt-6\">\r\n      <button\r\n        v-for=\"category in categories\"\r\n        :key=\"category\"\r\n        @click=\"setActiveCategory(category)\"\r\n        :class=\"['px-4 py-2 rounded-full text-sm font-semibold transition', selectedCategory === category ? 'bg-[#03a980] text-white' : 'bg-gray-200 text-gray-700']\">\r\n        {{ category }}\r\n      </button>\r\n    </div>\r\n\r\n    <!-- Modal Tambah/Edit Berita -->\r\n    <div v-if=\"showModal\" class=\"fixed inset-0 flex items-center justify-center bg-black bg-opacity-50\">\r\n      <div class=\"bg-white p-6 rounded-lg shadow-lg w-[500px] relative\">\r\n        <button @click=\"closeModal\" class=\"absolute top-2 right-2 text-gray-500\">‚úñ</button>\r\n        <h2 class=\"text-lg font-semibold mb-4\">{{ isEdit ? 'Edit Berita' : 'Tambah Berita' }}</h2>\r\n        <div class=\"flex flex-col gap-4\">\r\n          <input class=\"w-full h-[42px] bg-gray-100 border px-4 rounded\" v-model=\"modalNews.title\" placeholder=\"Judul\" />\r\n          <textarea class=\"w-full h-[100px] bg-gray-100 border px-4 rounded\" v-model=\"modalNews.description\" placeholder=\"Deskripsi\"></textarea>\r\n          <input type=\"date\" class=\"w-full h-[42px] bg-gray-100 border px-4 rounded\" v-model=\"modalNews.date\" />\r\n          <select v-model=\"modalNews.category\" class=\"w-full h-[42px] bg-gray-100 border px-4 rounded\">\r\n            <option disabled value=\"\">Pilih Kategori</option>\r\n            <option v-for=\"category in categories\" :key=\"category\">{{ category }}</option>\r\n          </select>\r\n          <input type=\"file\" class=\"w-full h-[42px] bg-gray-100 border px-4 rounded\" @change=\"handleFileChange\" />\r\n        </div>\r\n        <div class=\"flex justify-between items-center mt-6\">\r\n          <button @click=\"closeModal\" class=\"w-[48%] h-[54px] bg-gray-300 rounded-md text-black font-bold\">CANCEL</button>\r\n          <button @click=\"saveNews\" class=\"w-[48%] h-[54px] bg-[#03a980] rounded-md text-white font-bold\">\r\n            {{ isEdit ? 'UPDATE' : 'TAMBAH' }}\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Table Daftar Berita -->\r\n    <div class=\"p-8 mt-[100px]\">\r\n      <div class=\"bg-gray-100 p-6 rounded-lg shadow\">\r\n        <div class=\"flex justify-between items-center mb-4\">\r\n          <h2 class=\"text-lg font-semibold\">Daftar Berita</h2>\r\n          <button @click=\"openAddModal\" class=\"bg-[#03a980] text-white px-4 py-2 rounded-md hover:bg-[#028970]\">\r\n            ‚ûï Tambah Berita\r\n          </button>\r\n        </div>\r\n        <table class=\"w-full bg-white border border-gray-300 rounded-lg\">\r\n          <thead>\r\n            <tr class=\"border-b\">\r\n              <th class=\"py-2 px-4 text-left\">ID</th>\r\n              <th class=\"py-2 px-4 text-left\">Judul</th>\r\n              <th class=\"py-2 px-4 text-left\">Deskripsi</th>\r\n              <th class=\"py-2 px-4 text-left\">Tanggal Publikasi</th>\r\n              <th class=\"py-2 px-4 text-left\">Kategori</th>\r\n              <th class=\"py-2 px-4 text-left\">Foto</th>\r\n              <th class=\"py-2 px-4 text-left\">Action</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"news in paginatedNews\" :key=\"news.id\" class=\"border-b\">\r\n              <td class=\"py-2 px-4\">{{ news.id }}</td>\r\n              <td class=\"py-2 px-4\">{{ news.title }}</td>\r\n              <td class=\"py-2 px-4\">{{ news.description }}</td>\r\n              <td class=\"py-2 px-4\">{{ news.date }}</td>\r\n              <td class=\"py-2 px-4 font-semibold text-[#03a980]\">{{ news.category }}</td>\r\n              <td class=\"py-2 px-4 flex flex-col items-center\">\r\n                <img v-if=\"news.gambar.length > 0\" :src=\"news.gambar[0]?.url\" alt=\"Foto Berita\" class=\"w-16 h-16 rounded-md object-cover mb-2\" />\r\n                <button v-if=\"news.gambar.length > 0\" @click=\"deleteGambar(news.id, news.gambar[0].id)\" class=\"text-red-500\">üóëÔ∏è Hapus Foto</button>\r\n              </td>\r\n              <td class=\"py-2 px-4 flex gap-2\">\r\n                <button @click=\"openEditModal(news)\" class=\"text-blue-500\">‚úèÔ∏è</button>\r\n                <button @click=\"deleteNews(news.id)\" class=\"text-red-500\">üóëÔ∏è</button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n\r\n        <!-- Pagination Controls -->\r\n        <div v-if=\"!showModal\" class=\"flex justify-center mt-6 space-x-2\">\r\n          <button @click=\"prevPage\" :disabled=\"currentPage === 1\" class=\"px-4 py-2 bg-[#03a980] text-white rounded-md hover:bg-[#028a66] disabled:opacity-50\">Previous</button>\r\n          <button v-for=\"page in totalPages\" :key=\"page\" @click=\"currentPage = page\" class=\"px-4 py-2 border rounded-md\" :class=\"currentPage === page ? 'bg-[#03a980] text-white' : 'bg-white text-black hover:bg-gray-200'\">{{ page }}</button>\r\n          <button @click=\"nextPage\" :disabled=\"currentPage === totalPages\" class=\"px-4 py-2 bg-[#03a980] text-white rounded-md hover:bg-[#028a66] disabled:opacity-50\">Next</button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavbarPusat from \"@/components/NavbarPusat.vue\";\r\nimport ApiService from \"@/service/api.js\";\r\n\r\nexport default {\r\n  components: {\r\n    NavbarPusat,\r\n  },\r\n  data() {\r\n    return {\r\n      categories: [\"Politik\", \"Olahraga\", \"Teknologi\", \"Nasional\", \"Internasional\"],\r\n      newsList: [],\r\n      currentPage: 1,\r\n      itemsPerPage: 5,\r\n      showModal: false,\r\n      isEdit: false,\r\n      modalNews: {\r\n        id: null,\r\n        title: \"\",\r\n        description: \"\",\r\n        date: \"\",\r\n        category: \"\",\r\n        gambar: null,\r\n      },\r\n      selectedImage: null,\r\n      selectedCategory: \"\",\r\n      errorMessage: \"\",\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getNews();\r\n  },\r\n  computed: {\r\n    totalPages() {\r\n      return Math.ceil(this.filteredNews.length / this.itemsPerPage);\r\n    },\r\n    paginatedNews() {\r\n      const start = (this.currentPage - 1) * this.itemsPerPage;\r\n      return this.filteredNews.slice(start, start + this.itemsPerPage);\r\n    },\r\n    filteredNews() {\r\n      return this.newsList.filter(\r\n        (news) =>\r\n          (!this.selectedCategory || news.category === this.selectedCategory)\r\n      );\r\n    },\r\n  },\r\n  methods: {\r\n    getNews() {\r\n      ApiService.getBerita()\r\n        .then((response) => {\r\n          this.newsList = response.data;\r\n        })\r\n        .catch((error) => {\r\n          this.errorMessage = \"Failed to load news, please try again later.\";\r\n          console.error(error);\r\n        });\r\n    },\r\n    openAddModal() {\r\n      this.isEdit = false;\r\n      this.modalNews = { id: null, title: \"\", description: \"\", date: \"\", category: \"\", gambar: null };\r\n      this.showModal = true;\r\n    },\r\n    openEditModal(news) {\r\n      this.isEdit = true;\r\n      this.modalNews = { ...news };\r\n      this.showModal = true;\r\n    },\r\n    closeModal() {\r\n      this.showModal = false;\r\n    },\r\n    saveNews() {\r\n      const formData = {\r\n        title: this.modalNews.title,\r\n        description: this.modalNews.description,\r\n        date: this.modalNews.date,\r\n        category: this.modalNews.category,\r\n        jumlah_pengunjung: 0,\r\n      };\r\n\r\n      const action = this.isEdit\r\n        ? ApiService.updateBerita(this.modalNews.id, formData)\r\n        : ApiService.createBerita(formData);\r\n\r\n      action\r\n        .then((response) => {\r\n          if (this.selectedImage && response.data.id) {\r\n            const imageFormData = new FormData();\r\n            imageFormData.append(\"gambar\", this.selectedImage);\r\n\r\n            ApiService.addGambarBerita(response.data.id, imageFormData)\r\n              .then(() => {\r\n                this.getNews();\r\n                this.closeModal();\r\n              })\r\n              .catch((error) => {\r\n                console.error(\"Failed to upload image:\", error);\r\n              });\r\n          } else {\r\n            this.getNews();\r\n            this.closeModal();\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          this.errorMessage = \"Failed to save news, please try again.\";\r\n          console.error(error);\r\n        });\r\n    },\r\n    deleteNews(id) {\r\n      ApiService.deleteBerita(id)\r\n        .then(() => {\r\n          this.getNews();\r\n        })\r\n        .catch((error) => {\r\n          this.errorMessage = \"Failed to delete news, please try again.\";\r\n          console.error(error);\r\n        });\r\n    },\r\n    deleteGambar(beritaId, gambarId) {\r\n      ApiService.deleteGambarBerita(beritaId, gambarId)\r\n        .then(() => {\r\n          this.getNews();\r\n        })\r\n        .catch((error) => {\r\n          console.error(\"Failed to delete image:\", error);\r\n        });\r\n    },\r\n    handleFileChange(event) {\r\n      this.selectedImage = event.target.files[0];\r\n    },\r\n    setActiveCategory(category) {\r\n      this.selectedCategory = category;\r\n    },\r\n    nextPage() {\r\n      if (this.currentPage < this.totalPages) {\r\n        this.currentPage++;\r\n      }\r\n    },\r\n    prevPage() {\r\n      if (this.currentPage > 1) {\r\n        this.currentPage--;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"mappings":";;AA4FA,OAAOA,WAAU,MAAO,8BAA8B;AACtD,OAAOC,UAAS,MAAO,kBAAkB;AAEzC,eAAe;EACbC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,CAAC,SAAS,EAAE,UAAU,EAAE,WAAW,EAAE,UAAU,EAAE,eAAe,CAAC;MAC7EC,QAAQ,EAAE,EAAE;MACZC,WAAW,EAAE,CAAC;MACdC,YAAY,EAAE,CAAC;MACfC,SAAS,EAAE,KAAK;MAChBC,MAAM,EAAE,KAAK;MACbC,SAAS,EAAE;QACTC,EAAE,EAAE,IAAI;QACRC,KAAK,EAAE,EAAE;QACTC,WAAW,EAAE,EAAE;QACfC,IAAI,EAAE,EAAE;QACRC,QAAQ,EAAE,EAAE;QACZC,MAAM,EAAE;MACV,CAAC;MACDC,aAAa,EAAE,IAAI;MACnBC,gBAAgB,EAAE,EAAE;MACpBC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,OAAO,CAAC,CAAC;EAChB,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,OAAOC,IAAI,CAACC,IAAI,CAAC,IAAI,CAACC,YAAY,CAACC,MAAK,GAAI,IAAI,CAACpB,YAAY,CAAC;IAChE,CAAC;IACDqB,aAAaA,CAAA,EAAG;MACd,MAAMC,KAAI,GAAI,CAAC,IAAI,CAACvB,WAAU,GAAI,CAAC,IAAI,IAAI,CAACC,YAAY;MACxD,OAAO,IAAI,CAACmB,YAAY,CAACI,KAAK,CAACD,KAAK,EAAEA,KAAI,GAAI,IAAI,CAACtB,YAAY,CAAC;IAClE,CAAC;IACDmB,YAAYA,CAAA,EAAG;MACb,OAAO,IAAI,CAACrB,QAAQ,CAAC0B,MAAM,CACxBC,IAAI,IACF,CAAC,IAAI,CAACd,gBAAe,IAAKc,IAAI,CAACjB,QAAO,KAAM,IAAI,CAACG,gBACtD,CAAC;IACH;EACF,CAAC;EACDe,OAAO,EAAE;IACPZ,OAAOA,CAAA,EAAG;MACRpB,UAAU,CAACiC,SAAS,CAAC,EAClBC,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAAC/B,QAAO,GAAI+B,QAAQ,CAACjC,IAAI;MAC/B,CAAC,EACAkC,KAAK,CAAEC,