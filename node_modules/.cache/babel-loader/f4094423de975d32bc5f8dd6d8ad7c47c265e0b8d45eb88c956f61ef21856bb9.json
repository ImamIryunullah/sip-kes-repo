{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport NavbarUser from '@/components/NavbarUser.vue';\nimport axios from \"axios\";\nexport default {\n  components: {\n    NavbarUser\n  },\n  data() {\n    return {\n      form: {\n        nama: '',\n        email: '',\n        wilayah: '',\n        daerah: '',\n        kategori: '',\n        judul: '',\n        deskripsi: '',\n        harapan: '',\n        tindakLanjut: false\n      },\n      fileName: '',\n      file: null\n    };\n  },\n  methods: {\n    handleFileUpload(event) {\n      const file = event.target.files[0];\n      if (file) {\n        this.fileName = file.name;\n        this.file = file;\n      }\n    },\n    submitForm() {\n      const formData = new FormData();\n      for (const key in this.form) {\n        formData.append(key, this.form[key]);\n      }\n      if (this.file) {\n        formData.append('lampiran', this.file);\n      }\n      axios.post('http://192.168.1.11:5000/pengaduan', formData).then(response => {\n        alert(\"Pengaduan Anda telah dikirimkan.\");\n        this.$router.push('/');\n      }).catch(error => {\n        alert(\"Terjadi kesalahan saat mengirim pengaduan.\");\n      });\n    }\n  }\n};","map":{"version":3,"names":["NavbarUser","axios","components","data","form","nama","email","wilayah","daerah","kategori","judul","deskripsi","harapan","tindakLanjut","fileName","file","methods","handleFileUpload","event","target","files","name","submitForm","formData","FormData","key","append","post","then","response","alert","$router","push","catch","error"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\user\\FormPengaduanView.vue"],"sourcesContent":["<template>\r\n  <div class=\"w-screen min-h-screen flex flex-col bg-gray-100\">\r\n    <NavbarUser />\r\n    <div class=\"flex flex-col items-center p-6 flex-grow mt-10\">\r\n      <div class=\"bg-white w-full max-w-screen-md p-6 rounded-lg shadow-lg animate-fadeInUp\">\r\n        <h2 class=\"text-3xl font-semibold text-red-600 text-center mb-6\">Form Pengaduan</h2>\r\n        <form @submit.prevent=\"submitForm\" class=\"space-y-6\">\r\n          <!-- Nama Pengadu -->\r\n          <div>\r\n            <label for=\"nama\" class=\"block text-sm font-semibold text-red-700\">Nama Pengadu</label>\r\n            <input id=\"nama\" v-model=\"form.nama\" type=\"text\" required class=\"input-field\" placeholder=\"Nama Pengadu\" />\r\n          </div>\r\n\r\n          <!-- Email -->\r\n          <div>\r\n            <label for=\"email\" class=\"block text-sm font-semibold text-red-700\">Email</label>\r\n            <input id=\"email\" v-model=\"form.email\" type=\"email\" required class=\"input-field\" placeholder=\"Email Anda\" />\r\n          </div>\r\n\r\n          <!-- Wilayah -->\r\n          <div>\r\n            <label for=\"wilayah\" class=\"block text-sm font-semibold text-red-700\">Wilayah/Provinsi</label>\r\n            <input id=\"wilayah\" v-model=\"form.wilayah\" type=\"text\" required class=\"input-field\" placeholder=\"Provinsi Anda\" />\r\n          </div>\r\n\r\n          <!-- Daerah -->\r\n          <div>\r\n            <label for=\"daerah\" class=\"block text-sm font-semibold text-red-700\">Daerah</label>\r\n            <input id=\"daerah\" v-model=\"form.daerah\" type=\"text\" required class=\"input-field\" placeholder=\"Kota atau Kabupaten\" />\r\n          </div>\r\n\r\n          <!-- Kategori Pengaduan -->\r\n          <div>\r\n            <label for=\"kategori\" class=\"block text-sm font-semibold text-red-700\">Kategori Pengaduan</label>\r\n            <select id=\"kategori\" v-model=\"form.kategori\" required class=\"input-field\">\r\n              <option disabled value=\"\">Pilih Kategori</option>\r\n              <option value=\"Layanan Pendidikan\">Layanan Pendidikan</option>\r\n              <option value=\"Sertifikasi\">Sertifikasi</option>\r\n              <option value=\"Fasilitas\">Fasilitas</option>\r\n              <option value=\"Masalah Administrasi\">Masalah Administrasi</option>\r\n              <option value=\"Lainnya\">Lainnya</option>\r\n            </select>\r\n          </div>\r\n\r\n          <!-- Judul Pengaduan -->\r\n          <div>\r\n            <label for=\"judul\" class=\"block text-sm font-semibold text-red-700\">Judul Pengaduan</label>\r\n            <input id=\"judul\" v-model=\"form.judul\" type=\"text\" required class=\"input-field\" placeholder=\"Judul Pengaduan\" />\r\n          </div>\r\n\r\n          <!-- Deskripsi Pengaduan -->\r\n          <div>\r\n            <label for=\"deskripsi\" class=\"block text-sm font-semibold text-red-700\">Deskripsi Pengaduan</label>\r\n            <textarea id=\"deskripsi\" v-model=\"form.deskripsi\" required class=\"input-field\" placeholder=\"Jelaskan pengaduan Anda\" rows=\"4\"></textarea>\r\n          </div>\r\n\r\n          <!-- Lampiran -->\r\n          <div>\r\n            <label for=\"lampiran\" class=\"block text-sm font-semibold text-red-700\">Lampiran / Bukti</label>\r\n            <input id=\"lampiran\" type=\"file\" @change=\"handleFileUpload\" class=\"input-field\" />\r\n            <p v-if=\"fileName\" class=\"text-sm text-red-700 mt-2\">File yang dipilih: {{ fileName }}</p>\r\n          </div>\r\n\r\n          <!-- Harapan Penyelesaian -->\r\n          <div>\r\n            <label for=\"harapan\" class=\"block text-sm font-semibold text-red-700\">Harapan Penyelesaian</label>\r\n            <input id=\"harapan\" v-model=\"form.harapan\" type=\"text\" required class=\"input-field\" placeholder=\"Harapan Penyelesaian\" />\r\n          </div>\r\n\r\n          <!-- Tindak Lanjut -->\r\n          <div class=\"flex items-center\">\r\n            <input type=\"checkbox\" id=\"tindakLanjut\" v-model=\"form.tindakLanjut\" class=\"mr-2\" />\r\n            <label for=\"tindakLanjut\" class=\"text-sm font-semibold text-red-700\">\r\n              Apakah Anda ingin dihubungi untuk tindak lanjut?\r\n            </label>\r\n          </div>\r\n\r\n          <!-- Submit Button -->\r\n          <button type=\"submit\" class=\"w-full bg-red-600 text-white p-3 rounded-lg font-semibold hover:bg-red-700 transition-transform transform hover:scale-105\">\r\n            Kirim Pengaduan\r\n          </button>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavbarUser from '@/components/NavbarUser.vue';\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  components: {\r\n    NavbarUser\r\n  },\r\n  data() {\r\n    return {\r\n      form: {\r\n        nama: '',\r\n        email: '',\r\n        wilayah: '',\r\n        daerah: '',\r\n        kategori: '',\r\n        judul: '',\r\n        deskripsi: '',\r\n        harapan: '',\r\n        tindakLanjut: false\r\n      },\r\n      fileName: '',\r\n      file: null\r\n    };\r\n  },\r\n  methods: {\r\n    handleFileUpload(event) {\r\n      const file = event.target.files[0];\r\n      if (file) {\r\n        this.fileName = file.name;\r\n        this.file = file;\r\n      }\r\n    },\r\n    submitForm() {\r\n      const formData = new FormData();\r\n      for (const key in this.form) {\r\n        formData.append(key, this.form[key]);\r\n      }\r\n      if (this.file) {\r\n        formData.append('lampiran', this.file);\r\n      }\r\n\r\n      axios.post('http://192.168.1.11:5000/pengaduan', formData)\r\n        .then(response => {\r\n          alert(\"Pengaduan Anda telah dikirimkan.\");\r\n          this.$router.push('/'); \r\n        })\r\n        .catch(error => {\r\n          alert(\"Terjadi kesalahan saat mengirim pengaduan.\");\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n@keyframes fadeInUp {\r\n  0% {\r\n    opacity: 0;\r\n    transform: translateY(20px);\r\n  }\r\n  100% {\r\n    opacity: 1;\r\n    transform: translateY(0);\r\n  }\r\n}\r\n\r\n.animate-fadeInUp {\r\n  animation: fadeInUp 0.6s ease-out;\r\n}\r\n\r\n.input-field {\r\n  @apply p-3 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-red-600;\r\n  transition: all 0.3s ease-in-out;\r\n}\r\n\r\nbutton:hover {\r\n  transition: all 0.3s ease-in-out;\r\n}\r\n\r\nbutton:active {\r\n  transform: scale(0.98);\r\n}\r\n</style>\r\n"],"mappings":";AAwFA,OAAOA,UAAS,MAAO,6BAA6B;AACpD,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE,EAAE;QACXC,MAAM,EAAE,EAAE;QACVC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,SAAS,EAAE,EAAE;QACbC,OAAO,EAAE,EAAE;QACXC,YAAY,EAAE;MAChB,CAAC;MACDC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,gBAAgBA,CAACC,KAAK,EAAE;MACtB,MAAMH,IAAG,GAAIG,KAAK,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;MAClC,IAAIL,IAAI,EAAE;QACR,IAAI,CAACD,QAAO,GAAIC,IAAI,CAACM,IAAI;QACzB,IAAI,CAACN,IAAG,GAAIA,IAAI;MAClB;IACF,CAAC;IACDO,UAAUA,CAAA,EAAG;MACX,MAAMC,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC/B,KAAK,MAAMC,GAAE,IAAK,IAAI,CAACrB,IAAI,EAAE;QAC3BmB,QAAQ,CAACG,MAAM,CAACD,GAAG,EAAE,IAAI,CAACrB,IAAI,CAACqB,GAAG,CAAC,CAAC;MACtC;MACA,IAAI,IAAI,CAACV,IAAI,EAAE;QACbQ,QAAQ,CAACG,MAAM,CAAC,UAAU,EAAE,IAAI,CAACX,IAAI,CAAC;MACxC;MAEAd,KAAK,CAAC0B,IAAI,CAAC,oCAAoC,EAAEJ,QAAQ,EACtDK,IAAI,CAACC,QAAO,IAAK;QAChBC,KAAK,CAAC,kCAAkC,CAAC;QACzC,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,GAAG,CAAC;MACxB,CAAC,EACAC,KAAK,CAACC,KAAI,IAAK;QACdJ,KAAK,CAAC,4CAA4C,CAAC;MACrD,CAAC,CAAC;IACN;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}