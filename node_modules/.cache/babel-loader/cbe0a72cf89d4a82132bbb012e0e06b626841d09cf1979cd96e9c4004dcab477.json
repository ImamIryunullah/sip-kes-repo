{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport NavbarPusat from '@/components/NavbarPusat.vue';\nexport default {\n  components: {\n    NavbarPusat\n  },\n  data() {\n    return {\n      complaints: [/* Data pengaduan */],\n      currentPage: 1,\n      itemsPerPage: 2\n    };\n  },\n  computed: {\n    totalPages() {\n      return Math.ceil(this.complaints.length / this.itemsPerPage);\n    },\n    paginatedComplaints() {\n      const start = (this.currentPage - 1) * this.itemsPerPage;\n      const end = start + this.itemsPerPage;\n      return this.complaints.slice(start, end);\n    }\n  },\n  methods: {\n    getStatusClass(status) {\n      return status === 'Diproses' ? 'text-yellow-500' : status === 'Disetujui' ? 'text-green-500' : 'text-red-500';\n    },\n    updateComplaintStatus(complaint) {\n      console.log('Updated status:', complaint);\n    },\n    sendResponse(complaint) {\n      if (complaint.tanggapan.trim()) {\n        alert('Tanggapan telah dikirim!');\n        complaint.tanggapan = '';\n      } else {\n        alert('Tanggapan tidak boleh kosong!');\n      }\n    },\n    deleteComplaint(id) {\n      this.complaints = this.complaints.filter(complaint => complaint.id !== id);\n      alert('Pengaduan dengan ID ' + id + ' telah dihapus.');\n    }\n  }\n};","map":{"version":3,"names":["NavbarPusat","components","data","complaints","currentPage","itemsPerPage","computed","totalPages","Math","ceil","length","paginatedComplaints","start","end","slice","methods","getStatusClass","status","updateComplaintStatus","complaint","console","log","sendResponse","tanggapan","trim","alert","deleteComplaint","id","filter"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\pusat\\DataPengaduanView.vue"],"sourcesContent":["<template>\r\n  <div class=\"w-screen bg-gray-100 mt-12\">\r\n    <NavbarPusat />\r\n\r\n    <div class=\"flex flex-col items-center p-10 bg-gray-100 flex-grow\">\r\n      <div class=\"bg-white w-full max-w-4xl p-6 rounded-lg shadow-md\">\r\n        <h2 class=\"text-2xl font-semibold text-[#03a980] text-center mb-6\">Layanan Pengaduan - Pusat</h2>\r\n\r\n        <div v-if=\"paginatedComplaints.length > 0\" class=\"space-y-4\">\r\n          <div v-for=\"(complaint, index) in paginatedComplaints\" :key=\"index\" class=\"p-4 bg-gray-50 rounded-lg shadow-md border border-gray-300\">\r\n            <p><strong>ID Pengaduan:</strong> {{ complaint.id }}</p>\r\n            <p><strong>Daerah:</strong> {{ complaint.daerah }}</p>\r\n            <p><strong>Kategori:</strong> {{ complaint.kategori }}</p>\r\n            <p><strong>Nama Pengguna:</strong> {{ complaint.firstName }} {{ complaint.lastName }}</p>\r\n            <p><strong>Judul:</strong> {{ complaint.judul }}</p>\r\n            <p><strong>Deskripsi:</strong> {{ complaint.deskripsi }}</p>\r\n            <p><strong>Status:</strong> <span :class=\"getStatusClass(complaint.status)\">{{ complaint.status }}</span></p>\r\n            <div class=\"flex space-x-2 mt-4\">\r\n              <select v-model=\"complaint.status\" @change=\"updateComplaintStatus(complaint)\" class=\"p-2 bg-[#03a980] text-white rounded-md\">\r\n                <option value=\"Diproses\">Diproses</option>\r\n                <option value=\"Disetujui\">Disetujui</option>\r\n                <option value=\"Ditolak\">Ditolak</option>\r\n              </select>\r\n              <button @click=\"deleteComplaint(complaint.id)\" class=\"p-2 bg-red-500 text-white rounded-md hover:bg-red-600\">Hapus</button>\r\n            </div>\r\n          </div>\r\n          <div class=\"flex justify-center mt-6\">\r\n            <button v-for=\"page in totalPages\" :key=\"page\" @click=\"currentPage = page\" :class=\"['px-4 py-2 mx-1 rounded-md', currentPage === page ? 'bg-[#03a980] text-white' : 'bg-gray-200']\">\r\n              {{ page }}\r\n            </button>\r\n          </div>\r\n        </div>\r\n        <div v-else class=\"text-center text-gray-500\">Tidak ada pengaduan yang masuk.</div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavbarPusat from '@/components/NavbarPusat.vue';\r\n\r\nexport default {\r\n  components: { NavbarPusat },\r\n  data() {\r\n    return {\r\n      complaints: [/* Data pengaduan */],\r\n      currentPage: 1,\r\n      itemsPerPage: 2\r\n    };\r\n  },\r\n  computed: {\r\n    totalPages() {\r\n      return Math.ceil(this.complaints.length / this.itemsPerPage);\r\n    },\r\n    paginatedComplaints() {\r\n      const start = (this.currentPage - 1) * this.itemsPerPage;\r\n      const end = start + this.itemsPerPage;\r\n      return this.complaints.slice(start, end);\r\n    }\r\n  },\r\n  methods: {\r\n    getStatusClass(status) { return status === 'Diproses' ? 'text-yellow-500' : status === 'Disetujui' ? 'text-green-500' : 'text-red-500'; },\r\n    updateComplaintStatus(complaint) { console.log('Updated status:', complaint); },\r\n    sendResponse(complaint) { if (complaint.tanggapan.trim()) { alert('Tanggapan telah dikirim!'); complaint.tanggapan = ''; } else { alert('Tanggapan tidak boleh kosong!'); } },\r\n    deleteComplaint(id) { this.complaints = this.complaints.filter(complaint => complaint.id !== id); alert('Pengaduan dengan ID ' + id + ' telah dihapus.'); }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\ntextarea { resize: vertical; }\r\n</style>\r\n"],"mappings":";;AAuCA,OAAOA,WAAU,MAAO,8BAA8B;AAEtD,eAAe;EACbC,UAAU,EAAE;IAAED;EAAY,CAAC;EAC3BE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,CAAC,qBAAqB;MAClCC,WAAW,EAAE,CAAC;MACdC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,OAAOC,IAAI,CAACC,IAAI,CAAC,IAAI,CAACN,UAAU,CAACO,MAAK,GAAI,IAAI,CAACL,YAAY,CAAC;IAC9D,CAAC;IACDM,mBAAmBA,CAAA,EAAG;MACpB,MAAMC,KAAI,GAAI,CAAC,IAAI,CAACR,WAAU,GAAI,CAAC,IAAI,IAAI,CAACC,YAAY;MACxD,MAAMQ,GAAE,GAAID,KAAI,GAAI,IAAI,CAACP,YAAY;MACrC,OAAO,IAAI,CAACF,UAAU,CAACW,KAAK,CAACF,KAAK,EAAEC,GAAG,C