ign the uploaded file\n      }\n    },\n    handleKtpUpload(event) {\n      const file = event.target.files[0];\n      if (file) {\n        this.pendaftaranData.fotoKtp = file;\n      }\n    },\n    // This method handles the pendaftaran submission\n    async submitPendaftaran() {\n      // Validasi pendaftaran\n      if (!this.pendaftaranData.tipePendaftaran) {\n        alert(\"Silakan pilih tipe pendaftaran.\");\n        return;\n      }\n      if (this.pendaftaranData.tipePendaftaran === 'Kota/Kabupaten' && !this.pendaftaranData.kotaKabupaten) {\n        alert(\"Silakan pilih Kota/Kabupaten.\");\n        return;\n      }\n      if (this.pendaftaranData.tipePendaftaran === 'Provinsi' && !this.pendaftaranData.provinsi) {\n        alert(\"Silakan pilih Provinsi.\");\n        return;\n      }\n      if (!this.pendaftaranData.foto3x4 || !this.pendaftaranData.fotoKtp) {\n        alert(\"Silakan unggah Foto 3x4 dan Foto KTP.\");\n        return;\n      }\n      if (!this.pendaftaranData.jabatan) {\n        alert(\"Silakan pilih Jabatan LPKNI.\");\n        return;\n      }\n\n      // Create pendaftaranData to send the data and files to backend\n      const formData = new FormData();\n      formData.append(\"nama\", this.pendaftaranData.nama);\n      formData.append(\"nik\", this.pendaftaranData.nik);\n      formData.append(\"noTelepon\", this.pendaftaranData.noTelepon);\n      formData.append(\"email\", this.pendaftaranData.email);\n      formData.append(\"tipePendaftaran\", this.pendaftaranData.tipePendaftaran);\n      formDatqaa.append(\"provinsi\", this.pendaftaranData.provinsi);\n      formData.append(\"kotaKabupaten\", this.pendaftaranData.kotaKabupaten);\n      formData.append(\"jabatan\", this.pendaftaranData.jabatan);\n      formData.append(\"alamat\", this.pendaftaranData.alamat);\n      formData.append(\"foto3x4\", this.pendaftaranData.foto3x4);\n      formData.append(\"fotoKtp\", this.pendaftaranData.fotoKtp);\n      try {\n        const response = await ApiService.createPendaftaran(formData); // Menggunakan ApiService untuk membuat pendaftaran\n        if (response.status === 200) {\n          alert(\"Pendaftaran Anggota berhasil dikirim!\");\n          this.$router.push(\"/profile\"); // Redirect to another page\n        } else {\n          alert(\"Terjadi kesalahan saat mengirim pendaftaran.\");\n        }\n      } catch (error) {\n        console.error(\"Error:\", error);\n        alert(\"Terjadi kesalahan saat mengirim pendaftaran.\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["NavbarUser","ApiService","components","data","pendaftaranData","nama","nik","noTelepon","email","tipePendaftaran","provinsi","kotaKabupaten","jabatan","alamat","foto3x4","fotoKtp","jabatanProvinsi","jabatanKotaKabupaten","computed","jabatanOptions","methods","handleFoto3x4Upload","event","file","target","files","handleKtpUpload","submitPendaftaran","alert","formData","FormData","append","formDatqaa","response","createPendaftaran","status","$router","push","error","console"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\user\\PendaftaranAnggotaView.vue"],"sourcesContent":["<template>\r\n  <div class=\"w-screen h-screen flex flex-col mt-10\">\r\n    <!-- Navbar -->\r\n    <NavbarUser />\r\n\r\n    <div class=\"flex flex-col items-center p-10 bg-gray-100 flex-grow\">\r\n      <div class=\"bg-white w-full max-w-2xl p-6 rounded-lg shadow-md\">\r\n        <h2 class=\"text-2xl font-semibold text-[#03a980] text-center mb-6\">Pendaftaran Anggota</h2>\r\n\r\n        <!-- Form Pendaftaran -->\r\n        <form @submit.prevent=\"submitPendaftaran\">\r\n          <div class=\"grid grid-cols-1 gap-4\">\r\n            <input v-model=\"pendaftaranData.nama\" type=\"text\" placeholder=\"Nama Lengkap\" class=\"input-field\" required />\r\n            <input v-model=\"pendaftaranData.nik\" type=\"text\" placeholder=\"NIK\" class=\"input-field\" required />\r\n            <input v-model=\"pendaftaranData.noTelepon\" type=\"text\" placeholder=\"Nomor Telepon\" class=\"input-field\" required />\r\n            <input v-model=\"pendaftaranData.email\" type=\"email\" placeholder=\"Email\" class=\"input-field\" required />\r\n            \r\n            <!-- Pilih untuk Daftar Provinsi atau Kota/Kabupaten -->\r\n            <select v-model=\"pendaftaranData.tipePendaftaran\" class=\"input-field\" required>\r\n              <option disabled value=\"\">Pilih Tipe Pendaftaran</option>\r\n              <option value=\"Provinsi\">Provinsi</option>\r\n              <option value=\"Kota/Kabupaten\">Kota/Kabupaten</option>\r\n            </select>\r\n\r\n            <!-- Provinsi -->\r\n            <select v-if=\"pendaftaranData.tipePendaftaran === 'Provinsi'\" v-model=\"pendaftaranData.provinsi\" class=\"input-field\" required>\r\n              <option disabled value=\"\">Pilih Provinsi</option>\r\n              <option value=\"NTB\">NTB</option>\r\n              <option value=\"Jawa Timur\">Jawa Timur</option>\r\n              <option value=\"Sumatera Barat\">Sumatera Barat</option>\r\n            </select>\r\n\r\n            <!-- Kota/Kabupaten hanya muncul jika tipe pendaftaran adalah Kota/Kabupaten -->\r\n            <select v-if=\"pendaftaranData.tipePendaftaran === 'Kota/Kabupaten'\" v-model=\"pendaftaranData.kotaKabupaten\" class=\"input-field\" required>\r\n              <option disabled value=\"\">Pilih Kota/Kabupaten</option>\r\n              <option value=\"Kota Malang\">Kota Malang</option>\r\n              <option value=\"Surabaya\">Surabaya</option>\r\n              <option value=\"Gresik\">Gresik</option>\r\n            </select>\r\n\r\n            <!-- Jabatan LPKNI -->\r\n            <select v-model=\"pendaftaranData.jabatan\" class=\"input-field\" required>\r\n              <option disabled value=\"\">Pilih Jabatan LPKNI</option>\r\n              <!-- Conditional Jabatan options -->\r\n              <option v-for=\"jabatan in jabatanOptions\" :key=\"jabatan\" :value=\"jabatan\">{{ jabatan }}</option>\r\n            </select>\r\n\r\n            <input v-model=\"pendaftaranData.alamat\" type=\"text\" placeholder=\"Alamat Lengkap\" class=\"input-field\" required />\r\n            \r\n            <!-- Upload Foto 3x4 -->\r\n            <label class=\"text-sm text-gray-600\">Upload Foto 3x4:</label>\r\n            <input type=\"file\" @change=\"handleFoto3x4Upload\" class=\"input-field\" required />\r\n\r\n            <!-- Upload Foto KTP -->\r\n            <label class=\"text-sm text-gray-600\">Upload Foto KTP:</label>\r\n            <input type=\"file\" @change=\"handleKtpUpload\" class=\"input-field\" required />\r\n\r\n            <button type=\"submit\" class=\"mt-6 w-full bg-[#03a980] text-white px-6 py-2 rounded-lg font-bold hover:bg-[#028a66] transition\">\r\n              Ajukan Pendaftaran\r\n            </button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavbarUser from '@/components/NavbarUser.vue';\r\nimport ApiService from '@/service/api.js';\r\n\r\nexport default {\r\n  components: {\r\n    NavbarUser\r\n  },\r\n  data() {\r\n    return {\r\n      pendaftaranData: {\r\n        nama: \"\",\r\n        nik: \"\",\r\n        noTelepon: \"\",\r\n        email: \"\",\r\n        tipePendaftaran: \"\",\r\n        provinsi: \"\",\r\n        kotaKabupaten: \"\",\r\n        jabatan: \"\",\r\n        alamat: \"\",\r\n        foto3x4: null,\r\n        fotoKtp: null,\r\n      },\r\n      jabatanProvinsi: [\r\n        \"Staf Ahli / Komisioner\",\r\n        \"Sekretariat\",\r\n        \"Direktorat, pendidikan, latihan, SDM & LPKSM\",\r\n        \"Direktorat BPSK\",\r\n        \"Perhimpunan Driver Online Indonesia (PDOI), Lembaga Bantuan Hukum Nasional Indonesia (LBH-NI)\",\r\n        \"Direktorat Pengawasan Makanan dan Obat\",\r\n        \"Direktorat Pengawasan bank, finance dan koperasi\",\r\n        \"Direktorat media & komunikasi (suara konsumen)\",\r\n        \"Direktorat standarisasi warung & sertifikasi\",\r\n        \"Perhimpunan Bekam Nasional Indonesia (PBNI)\"\r\n      ],\r\n      jabatanKotaKabupaten: [\r\n        \"Staf Ahli / Komisioner\",\r\n        \"Sekretariat\",\r\n        \"Biro Pendidikan, Latihan, SDM & LPKSM\",\r\n        \"Biro BPSK\",\r\n        \"Perhimpunan Driver Online Indonesia (PDOI)\",\r\n        \"Lembaga Bantuan Hukum Nasional Indonesia (LBH-NI)\",\r\n        \"Biro Pengawasan Makanan dan Obat\",\r\n        \"Biro Pengawasan Bank Finance & Koperasi\",\r\n        \"Biro media & komunikasi (suara konsumen)\",\r\n        \"Biro Standarisasi Warung & Sertifikasi\",\r\n        \"Perhimpunan Bekam Nasional Indonesia (PBNI)\"\r\n      ]\r\n    };\r\n  },\r\n  computed: {\r\n    jabatanOptions() {\r\n      if (this.pendaftaranData.tipePendaftaran === 'Provinsi') {\r\n        return this.jabatanProvinsi;\r\n      } else if (this.pendaftaranData.tipePendaftaran === 'Kota/Kabupaten') {\r\n        return this.jabatanKotaKabupaten;\r\n      } else {\r\n        return [];\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    handleFoto3x4Upload(event) {\r\n      const file = event.target.files[0];\r\n      if (file) {\r\n        this.pendaftaranData.foto3x4 = file; // Assign the uploaded file\r\n      }\r\n    },\r\n    handleKtpUpload(event) {\r\n      const file = event.target.files[0];\r\n      if (file) {\r\n        this.pendaftaranData.fotoKtp = file;  \r\n      }\r\n    },\r\n\r\n    // This method handles the pendaftaran submission\r\n    async submitPendaftaran() {\r\n      // Validasi pendaftaran\r\n      if (!this.pendaftaranData.tipePendaftaran) {\r\n        alert(\"Silakan pilih tipe pendaftaran.\");\r\n        return;\r\n      }\r\n\r\n      if (this.pendaftaranData.tipePendaftaran === 'Kota/Kabupaten' && !this.pendaftaranData.kotaKabupaten) {\r\n        alert(\"Silakan pilih Kota/Kabupaten.\");\r\n        return;\r\n      }\r\n\r\n      if (this.pendaftaranData.tipePendaftaran === 'Provinsi' && !this.pendaftaranData.provinsi) {\r\n        alert(\"Silakan pilih Provinsi.\");\r\n        return;\r\n      }\r\n\r\n      if (!this.pendaftaranData.foto3x4 || !this.pendaftaranData.fotoKtp) {\r\n        alert(\"Silakan unggah Foto 3x4 dan Foto KTP.\");\r\n        return;\r\n      }\r\n\r\n      if (!this.pendaftaranData.jabatan) {\r\n        alert(\"Silakan pilih Jabatan LPKNI.\");\r\n        return;\r\n      }\r\n\r\n      // Create pendaftaranData to send the data and files to backend\r\n      const formData = new FormData();\r\n      formData.append(\"nama\", this.pendaftaranData.nama);\r\n      formData.append(\"nik\", this.pendaftaranData.nik);\r\n      formData.append(\"noTelepon\", this.pendaftaranData.noTelepon);\r\n      formData.append(\"email\", this.pendaftaranData.email);\r\n      formData.append(\"tipePendaftaran\", this.pendaftaranData.tipePendaftaran);\r\n      formDatqaa.append(\"provinsi\", this.pendaftaranData.provinsi);\r\n      formData.append(\"kotaKabupaten\", this.pendaftaranData.kotaKabupaten);\r\n      formData.append(\"jabatan\", this.pendaftaranData.jabatan);\r\n      formData.append(\"alamat\", this.pendaftaranData.alamat);\r\n      formData.append(\"foto3x4\", this.pendaftaranData.foto3x4);\r\n      formData.append(\"fotoKtp\", this.pendaftaranData.fotoKtp);\r\n\r\n      try {\r\n        const response = await ApiService.createPendaftaran(formData); // Menggunakan ApiService untuk membuat pendaftaran\r\n        if (response.status === 200) {\r\n          alert(\"Pendaftaran Anggota berhasil dikirim!\");\r\n          this.$router.push(\"/profile\"); // Redirect to another page\r\n        } else {\r\n          alert(\"Terjadi kesalahan saat mengirim pendaftaran.\");\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Error:\", error);\r\n        alert(\"Terjadi kesalahan saat mengirim pendaftaran.\");\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.input-field {\r\n  width: 100%;\r\n  padding: 10px