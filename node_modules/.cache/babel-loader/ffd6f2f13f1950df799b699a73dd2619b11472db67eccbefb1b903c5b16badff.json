{"ast":null,"code":"import NavbarUser from \"@/components/NavbarUser.vue\";\nimport jsPDF from \"jspdf\";\nexport default {\n  components: {\n    NavbarUser\n  },\n  data() {\n    return {\n      namaPengirim: \"\",\n      selectedPaymentMethod: \"\",\n      selectedBank: \"\",\n      transactionStatus: null,\n      dummyAccountNumber: \"123-456-7890\",\n      transferAmount: \"1.000.000\",\n      invoiceNumber: `INV-${new Date().getTime()}`,\n      // Generate Nomor Invoice\n      selectedFile: null\n    };\n  },\n  methods: {\n    async processTransaction() {\n      if (!this.namaPengirim || !this.selectedPaymentMethod) {\n        alert(\"Silakan lengkapi semua data transaksi.\");\n        return;\n      }\n      try {\n        const response = await this.fakePaymentAPI();\n        if (response.success) {\n          this.transactionStatus = \"success\";\n        } else {\n          this.transactionStatus = \"failed\";\n        }\n      } catch (error) {\n        console.error(\"Terjadi kesalahan:\", error);\n        this.transactionStatus = \"failed\";\n      }\n    },\n    onFileChange(event) {\n      const file = event.target.files[0];\n      if (file) {\n        this.selectedFile = file;\n      }\n    },\n    fakePaymentAPI() {\n      return new Promise(resolve => {\n        setTimeout(() => {\n          const success = Math.random() > 0.5;\n          resolve({\n            success\n          });\n        }, 2000);\n      });\n    },\n    generateInvoice() {\n      const doc = new jsPDF();\n\n      // Header Invoice\n      doc.setFontSize(18);\n      doc.text(\"Invoice Pembayaran\", 20, 20);\n      doc.setFontSize(12);\n      doc.text(`Nomor Invoice: ${this.invoiceNumber}`, 20, 30);\n      doc.text(`Tanggal: ${new Date().toLocaleDateString()}`, 20, 40);\n\n      // Detail Transaksi\n      doc.setFontSize(14);\n      doc.text(\"Detail Pembayaran:\", 20, 55);\n      doc.setFontSize(12);\n      doc.text(`Nama Pengirim: ${this.namaPengirim}`, 20, 65);\n      doc.text(`Metode Pembayaran: ${this.selectedPaymentMethod}`, 20, 75);\n      if (this.selectedPaymentMethod === \"Transfer Bank\") {\n        doc.text(`Bank: ${this.selectedBank}`, 20, 85);\n        doc.text(`Nomor Rekening: ${this.dummyAccountNumber}`, 20, 95);\n      }\n      doc.text(`Jumlah: Rp ${this.transferAmount}`, 20, 105);\n\n      // Footer\n      doc.setFontSize(10);\n      doc.text(\"Terima kasih atas pembayaran Anda!\", 20, 120);\n\n      // Simpan PDF\n      doc.save(`Invoice_${this.invoiceNumber}.pdf`);\n    }\n  }\n};","map":{"version":3,"names":["NavbarUser","jsPDF","components","data","namaPengirim","selectedPaymentMethod","selectedBank","transactionStatus","dummyAccountNumber","transferAmount","invoiceNumber","Date","getTime","selectedFile","methods","processTransaction","alert","response","fakePaymentAPI","success","error","console","onFileChange","event","file","target","files","Promise","resolve","setTimeout","Math","random","generateInvoice","doc","setFontSize","text","toLocaleDateString","save"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\user\\TransaksiUserView.vue"],"sourcesContent":["<template>\r\n  <div class=\"w-screen h-screen flex flex-col mt-12\">\r\n    <NavbarUser />\r\n\r\n    <div class=\"flex flex-col items-center p-10 bg-gray-100 flex-grow\">\r\n      <div class=\"bg-white w-full max-w-xl p-8 rounded-lg shadow-md\">\r\n        <h2 class=\"text-2xl font-semibold text-[#03a980] text-center mb-6\">Transaksi Pembayaran</h2>\r\n\r\n        <!-- Form untuk pembayaran -->\r\n        <form @submit.prevent=\"processTransaction\">\r\n          <div class=\"space-y-4\">\r\n            <div>\r\n              <label class=\"block text-sm font-medium text-gray-700\">Masukkan Nama Pengirim</label>\r\n              <input v-model=\"namaPengirim\" type=\"text\" class=\"input-field w-full mb-4\" placeholder=\"Masukkan Nama Pengirim\" required />\r\n              \r\n              <label class=\"block text-sm font-medium text-gray-700\">Metode Pembayaran</label>\r\n              <select v-model=\"selectedPaymentMethod\" class=\"input-field w-full\" required>\r\n                <option disabled value=\"\">Pilih Metode Pembayaran</option>\r\n                <option value=\"Transfer Bank\">Transfer Bank</option>\r\n                <option value=\"E-wallet\">E-wallet</option>\r\n                <option value=\"QRIS\">QRIS</option>\r\n              </select>\r\n            </div>\r\n\r\n            <!-- Transfer Bank -->\r\n            <div v-if=\"selectedPaymentMethod === 'Transfer Bank'\" class=\"space-y-4\">\r\n              <label class=\"block text-sm font-medium text-gray-700\">Bank Tujuan</label>\r\n              <select v-model=\"selectedBank\" class=\"input-field w-full\" required>\r\n                <option disabled value=\"\">Pilih Bank</option>\r\n                <option value=\"BCA\">BCA</option>\r\n                <option value=\"Mandiri\">Mandiri</option>\r\n                <option value=\"BNI\">BNI</option>\r\n                <option value=\"BRI\">BRI</option>\r\n              </select>\r\n\r\n              <div v-if=\"selectedBank\">\r\n                <p class=\"mt-2 text-sm text-gray-700\">Nomor Rekening Bank: <strong>{{ dummyAccountNumber }}</strong></p>\r\n                <p class=\"mt-2 text-sm text-gray-700\">Jumlah Transfer: <strong>Rp {{ transferAmount }}</strong></p>\r\n              </div>\r\n\r\n              <div class=\"mt-4\">\r\n                <label for=\"paymentProof\" class=\"block text-sm font-medium text-gray-700\">Upload Bukti Pembayaran</label>\r\n                <input type=\"file\" id=\"paymentProof\" @change=\"onFileChange\" class=\"input-field w-full\" required />\r\n              </div>\r\n            </div>\r\n\r\n            <!-- QRIS -->\r\n            <div v-if=\"selectedPaymentMethod === 'QRIS'\" class=\"space-y-4\">\r\n              <label class=\"block text-sm font-medium text-gray-700\">Scan QRIS</label>\r\n              <img src=\"@/assets/qriscontoh.png\" alt=\"QRIS Code\" class=\"w-full h-auto rounded-lg mb-4\" />\r\n            </div>\r\n          </div>\r\n\r\n          <!-- Tombol Bayar -->\r\n          <button type=\"submit\" class=\"mt-6 w-full bg-[#03a980] text-white px-6 py-2 rounded-lg font-semibold hover:bg-[#028a66] transition\">\r\n            Bayar Sekarang\r\n          </button>\r\n        </form>\r\n\r\n        <!-- Notifikasi & Tombol Download Invoice -->\r\n        <div v-if=\"transactionStatus\" class=\"mt-6 text-center\">\r\n          <p v-if=\"transactionStatus === 'success'\" class=\"text-green-500\">Pembayaran berhasil! Terima kasih.</p>\r\n          <p v-if=\"transactionStatus === 'failed'\" class=\"text-red-500\">Pembayaran gagal. Silakan coba lagi.</p>\r\n\r\n          <!-- Tombol Cetak Invoice -->\r\n          <button v-if=\"transactionStatus === 'success'\" @click=\"generateInvoice\" class=\"mt-4 w-full bg-blue-500 text-white px-6 py-2 rounded-lg font-semibold hover:bg-blue-600 transition\">\r\n            Download Invoice\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavbarUser from \"@/components/NavbarUser.vue\";\r\nimport jsPDF from \"jspdf\";\r\n\r\nexport default {\r\n  components: {\r\n    NavbarUser,\r\n  },\r\n  data() {\r\n    return {\r\n      namaPengirim: \"\",\r\n      selectedPaymentMethod: \"\",\r\n      selectedBank: \"\",\r\n      transactionStatus: null,\r\n      dummyAccountNumber: \"123-456-7890\",\r\n      transferAmount: \"1.000.000\",\r\n      invoiceNumber: `INV-${new Date().getTime()}`, // Generate Nomor Invoice\r\n      selectedFile: null,\r\n    };\r\n  },\r\n  methods: {\r\n    async processTransaction() {\r\n      if (!this.namaPengirim || !this.selectedPaymentMethod) {\r\n        alert(\"Silakan lengkapi semua data transaksi.\");\r\n        return;\r\n      }\r\n\r\n      try {\r\n        const response = await this.fakePaymentAPI();\r\n\r\n        if (response.success) {\r\n          this.transactionStatus = \"success\";\r\n        } else {\r\n          this.transactionStatus = \"failed\";\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Terjadi kesalahan:\", error);\r\n        this.transactionStatus = \"failed\";\r\n      }\r\n    },\r\n\r\n    onFileChange(event) {\r\n      const file = event.target.files[0];\r\n      if (file) {\r\n        this.selectedFile = file;\r\n      }\r\n    },\r\n\r\n    fakePaymentAPI() {\r\n      return new Promise((resolve) => {\r\n        setTimeout(() => {\r\n          const success = Math.random() > 0.5;\r\n          resolve({ success });\r\n        }, 2000);\r\n      });\r\n    },\r\n\r\n    generateInvoice() {\r\n      const doc = new jsPDF();\r\n\r\n      // Header Invoice\r\n      doc.setFontSize(18);\r\n      doc.text(\"Invoice Pembayaran\", 20, 20);\r\n      doc.setFontSize(12);\r\n      doc.text(`Nomor Invoice: ${this.invoiceNumber}`, 20, 30);\r\n      doc.text(`Tanggal: ${new Date().toLocaleDateString()}`, 20, 40);\r\n\r\n      // Detail Transaksi\r\n      doc.setFontSize(14);\r\n      doc.text(\"Detail Pembayaran:\", 20, 55);\r\n      doc.setFontSize(12);\r\n      doc.text(`Nama Pengirim: ${this.namaPengirim}`, 20, 65);\r\n      doc.text(`Metode Pembayaran: ${this.selectedPaymentMethod}`, 20, 75);\r\n      \r\n      if (this.selectedPaymentMethod === \"Transfer Bank\") {\r\n        doc.text(`Bank: ${this.selectedBank}`, 20, 85);\r\n        doc.text(`Nomor Rekening: ${this.dummyAccountNumber}`, 20, 95);\r\n      }\r\n\r\n      doc.text(`Jumlah: Rp ${this.transferAmount}`, 20, 105);\r\n\r\n      // Footer\r\n      doc.setFontSize(10);\r\n      doc.text(\"Terima kasih atas pembayaran Anda!\", 20, 120);\r\n\r\n      // Simpan PDF\r\n      doc.save(`Invoice_${this.invoiceNumber}.pdf`);\r\n    }\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.input-field {\r\n  @apply p-3 border border-gray-300 rounded-lg bg-gray-100 focus:outline-none focus:ring-2 focus:ring-[#03a980];\r\n}\r\n\r\nbutton {\r\n  @apply p-3 bg-[#03a980] text-white rounded-lg font-semibold hover:bg-[#028a66] transition;\r\n}\r\n</style>\r\n"],"mappings":"AA2EA,OAAOA,UAAS,MAAO,6BAA6B;AACpD,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,EAAE;MAChBC,qBAAqB,EAAE,EAAE;MACzBC,YAAY,EAAE,EAAE;MAChBC,iBAAiB,EAAE,IAAI;MACvBC,kBAAkB,EAAE,cAAc;MAClCC,cAAc,EAAE,WAAW;MAC3BC,aAAa,EAAE,OAAO,IAAIC,IAAI,CAAC,CAAC,CAACC,OAAO,CAAC,CAAC,EAAE;MAAE;MAC9CC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,kBAAkBA,CAAA,EAAG;MACzB,IAAI,CAAC,IAAI,CAACX,YAAW,IAAK,CAAC,IAAI,CAACC,qBAAqB,EAAE;QACrDW,KAAK,CAAC,wCAAwC,CAAC;QAC/C;MACF;MAEA,IAAI;QACF,MAAMC,QAAO,GAAI,MAAM,IAAI,CAACC,cAAc,CAAC,CAAC;QAE5C,IAAID,QAAQ,CAACE,OAAO,EAAE;UACpB,IAAI,CAACZ,iBAAgB,GAAI,SAAS;QACpC,OAAO;UACL,IAAI,CAACA,iBAAgB,GAAI,QAAQ;QACnC;MACF,EAAE,OAAOa,KAAK,EAAE;QACdC,OAAO,CAACD,KAAK,CAAC,oBAAoB,EAAEA,KAAK,CAAC;QAC1C,IAAI,CAACb,iBAAgB,GAAI,QAAQ;MACnC;IACF,CAAC;IAEDe,YAAYA,CAACC,KAAK,EAAE;MAClB,MAAMC,IAAG,GAAID,KAAK,CAACE,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;MAClC,IAAIF,IAAI,EAAE;QACR,IAAI,CAACX,YAAW,GAAIW,IAAI;MAC1B;IACF,CAAC;IAEDN,cAAcA,CAAA,EAAG;MACf,OAAO,IAAIS,OAAO,CAAEC,OAAO,IAAK;QAC9BC,UAAU,CAAC,MAAM;UACf,MAAMV,OAAM,GAAIW,IAAI,CAACC,MAAM,CAAC,IAAI,GAAG;UACnCH,OAAO,CAAC;YAAET;UAAQ,CAAC,CAAC;QACtB,CAAC,EAAE,IAAI,CAAC;MACV,CAAC,CAAC;IACJ,CAAC;IAEDa,eAAeA,CAAA,EAAG;MAChB,MAAMC,GAAE,GAAI,IAAIhC,KAAK,CAAC,CAAC;;MAEvB;MACAgC,GAAG,CAACC,WAAW,CAAC,EAAE,CAAC;MACnBD,GAAG,CAACE,IAAI,CAAC,oBAAoB,EAAE,EAAE,EAAE,EAAE,CAAC;MACtCF,GAAG,CAACC,WAAW,CAAC,EAAE,CAAC;MACnBD,GAAG,CAACE,IAAI,CAAC,kBAAkB,IAAI,CAACzB,aAAa,EAAE,EAAE,EAAE,EAAE,EAAE,CAAC;MACxDuB,GAAG,CAACE,IAAI,CAAC,YAAY,IAAIxB,IAAI,CAAC,CAAC,CAACyB,kBAAkB,CAAC,CAAC,EAAE,EAAE,EAAE,EAAE,EAAE,CAAC;;MAE/D;MACAH,GAAG,CAACC,WAAW,CAAC,EAAE,CAAC;MACnBD,GAAG,CAACE,IAAI,CAAC,oBAAoB,EAAE,EAAE,EAAE,EAAE,CAAC;MACtCF,GAAG,CAACC,WAAW,CAAC,EAAE,CAAC;MACnBD,GAAG,CAACE,IAAI,CAAC,kBAAkB,IAAI,CAAC/B,YAAY,EAAE,EAAE,EAAE,EAAE,