{"ast":null,"code":"import NavbarView from '@/components/NavbarView.vue';\nexport default {\n  components: {\n    NavbarView\n  },\n  data() {\n    return {\n      loading: false,\n      pengajuanList: [{\n        id: 1,\n        nama: \"Ahmad Hidayat\",\n        jenisSertifikat: \"Keahlian\",\n        instansi: \"Universitas A\",\n        status: \"Pending\"\n      }, {\n        id: 2,\n        nama: \"Rina Sutomo\",\n        jenisSertifikat: \"Pelatihan\",\n        instansi: \"Perusahaan B\",\n        status: \"Pending\"\n      }, {\n        id: 3,\n        nama: \"Bagas Santoso\",\n        jenisSertifikat: \"Keahlian\",\n        instansi: \"Lembaga C\",\n        status: \"Disetujui\"\n      }, {\n        id: 4,\n        nama: \"Dewi Lestari\",\n        jenisSertifikat: \"Pelatihan\",\n        instansi: \"Organisasi D\",\n        status: \"Ditolak\"\n      }]\n    };\n  },\n  methods: {\n    getStatusClass(status) {\n      switch (status) {\n        case \"Pending\":\n          return \"text-yellow-500\";\n        case \"Disetujui\":\n          return \"text-green-500\";\n        case \"Ditolak\":\n          return \"text-red-500\";\n        default:\n          return \"text-gray-500\";\n      }\n    }\n  }\n};","map":{"version":3,"names":["NavbarView","components","data","loading","pengajuanList","id","nama","jenisSertifikat","instansi","status","methods","getStatusClass"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\admin\\AdminPengajuan.vue"],"sourcesContent":["<template>\r\n    <div class=\"w-screen min-h-screen flex flex-col bg-gray-100\">\r\n      <!-- Navbar -->\r\n      <NavbarView />\r\n  \r\n      <!-- Content Wrapper -->\r\n      <div class=\"flex flex-col items-center px-10 py-6 mt-[100px] flex-grow\">\r\n        <div class=\"bg-white w-full max-w-5xl p-6 rounded-lg shadow-md\">\r\n          <h2 class=\"text-2xl font-semibold text-[#03a980] text-center mb-6\">Pengajuan Sertifikat</h2>\r\n  \r\n          <!-- Loading -->\r\n          <div v-if=\"loading\" class=\"flex justify-center my-4\">\r\n            <span class=\"text-[#03a980] font-semibold\">Loading data...</span>\r\n          </div>\r\n  \r\n          <!-- Tabel Data Pengajuan -->\r\n          <table v-if=\"!loading && pengajuanList.length > 0\" class=\"w-full border-collapse border border-gray-300\">\r\n            <thead class=\"bg-[#03a980] text-white\">\r\n              <tr>\r\n                <th class=\"py-2 px-4 border\">Nama</th>\r\n                <th class=\"py-2 px-4 border\">Jenis Sertifikat</th>\r\n                <th class=\"py-2 px-4 border\">Instansi</th>\r\n                <th class=\"py-2 px-4 border\">Status</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <tr v-for=\"pengajuan in pengajuanList\" :key=\"pengajuan.id\" class=\"border\">\r\n                <td class=\"py-2 px-4\">{{ pengajuan.nama }}</td>\r\n                <td class=\"py-2 px-4\">{{ pengajuan.jenisSertifikat }}</td>\r\n                <td class=\"py-2 px-4\">{{ pengajuan.instansi }}</td>\r\n                <td class=\"py-2 px-4 font-semibold\" :class=\"getStatusClass(pengajuan.status)\">\r\n                  {{ pengajuan.status }}\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n  \r\n          <!-- Jika Tidak Ada Pengajuan -->\r\n          <p v-if=\"!loading && pengajuanList.length === 0\" class=\"text-center text-gray-500 mt-4\">\r\n            Tidak ada pengajuan sertifikat saat ini.\r\n          </p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import NavbarView from '@/components/NavbarView.vue';\r\n  \r\n  export default {\r\n    components: {\r\n      NavbarView\r\n    },\r\n    data() {\r\n      return {\r\n        loading: false,\r\n        pengajuanList: [\r\n          { id: 1, nama: \"Ahmad Hidayat\", jenisSertifikat: \"Keahlian\", instansi: \"Universitas A\", status: \"Pending\" },\r\n          { id: 2, nama: \"Rina Sutomo\", jenisSertifikat: \"Pelatihan\", instansi: \"Perusahaan B\", status: \"Pending\" },\r\n          { id: 3, nama: \"Bagas Santoso\", jenisSertifikat: \"Keahlian\", instansi: \"Lembaga C\", status: \"Disetujui\" },\r\n          { id: 4, nama: \"Dewi Lestari\", jenisSertifikat: \"Pelatihan\", instansi: \"Organisasi D\", status: \"Ditolak\" }\r\n        ]\r\n      };\r\n    },\r\n    methods: {\r\n      getStatusClass(status) {\r\n        switch (status) {\r\n          case \"Pending\":\r\n            return \"text-yellow-500\";\r\n          case \"Disetujui\":\r\n            return \"text-green-500\";\r\n          case \"Ditolak\":\r\n            return \"text-red-500\";\r\n          default:\r\n            return \"text-gray-500\";\r\n        }\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* Input Field Styling */\r\n  .input-field {\r\n    @apply w-full p-2 border border-gray-300 rounded-lg bg-gray-100 focus:outline-none;\r\n  }\r\n  \r\n  /* Responsive Table */\r\n  @media (max-width: 768px) {\r\n    table {\r\n      font-size: 14px;\r\n    }\r\n    th, td {\r\n      padding: 8px;\r\n    }\r\n  }\r\n  </style>\r\n  "],"mappings":"AA+CE,OAAOA,UAAS,MAAO,6BAA6B;AAEpD,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,KAAK;MACdC,aAAa,EAAE,CACb;QAAEC,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,eAAe;QAAEC,eAAe,EAAE,UAAU;QAAEC,QAAQ,EAAE,eAAe;QAAEC,MAAM,EAAE;MAAU,CAAC,EAC3G;QAAEJ,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,aAAa;QAAEC,eAAe,EAAE,WAAW;QAAEC,QAAQ,EAAE,cAAc;QAAEC,MAAM,EAAE;MAAU,CAAC,EACzG;QAAEJ,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,eAAe;QAAEC,eAAe,EAAE,UAAU;QAAEC,QAAQ,EAAE,WAAW;QAAEC,MAAM,EAAE;MAAY,CAAC,EACzG;QAAEJ,EAAE,EAAE,CAAC;QAAEC,IAAI,EAAE,cAAc;QAAEC,eAAe,EAAE,WAAW;QAAEC,QAAQ,EAAE,cAAc;QAAEC,MAAM,EAAE;MAAU;IAE7G,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,cAAcA,CAACF,MAAM,EAAE;MACrB,QAAQA,MAAM;QACZ,KAAK,SAAS;UACZ,OAAO,iBAAiB;QAC1B,KAAK,WAAW;UACd,OAAO,gBAAgB;QACzB,KAAK,SAAS;UACZ,OAAO,cAAc;QACvB;UACE,OAAO