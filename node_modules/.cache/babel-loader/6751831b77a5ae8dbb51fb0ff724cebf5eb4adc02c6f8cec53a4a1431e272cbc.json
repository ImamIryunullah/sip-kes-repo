{"ast":null,"code":"export default {\n  data() {\n    return {\n      anggota: {\n        nama: '',\n        nik: '',\n        id: '',\n        daerahWilayah: '',\n        daerah: '',\n        wilayah: ''\n      }\n    };\n  },\n  methods: {\n    async submitPendaftaran() {\n      // Kirim data pendaftaran ke pusat untuk disetujui\n      const formData = new FormData();\n      formData.append(\"nama\", this.anggota.nama);\n      formData.append(\"nik\", this.anggota.nik);\n      formData.append(\"id\", this.anggota.id);\n      formData.append(\"daerahWilayah\", this.anggota.daerahWilayah);\n      formData.append(\"daerah\", this.anggota.daerah);\n      formData.append(\"wilayah\", this.anggota.wilayah);\n\n      // Kirim data ke pusat (API endpoint misalnya \"/api/pusat/pendaftaran\")\n      try {\n        const response = await fetch(\"/api/pusat/pendaftaran\", {\n          method: \"POST\",\n          body: formData\n        });\n        if (response.ok) {\n          alert(\"Pendaftaran berhasil dikirim. Tunggu persetujuan pusat.\");\n        } else {\n          alert(\"Terjadi kesalahan saat pengajuan.\");\n        }\n      } catch (error) {\n        console.error(\"Error:\", error);\n        alert(\"Gagal mengirim data pendaftaran.\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","anggota","nama","nik","id","daerahWilayah","daerah","wilayah","methods","submitPendaftaran","formData","FormData","append","response","fetch","method","body","ok","alert","error","console"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\user\\PengajuanSertifikat.vue"],"sourcesContent":["<template>\r\n  <div class=\"w-screen h-screen flex flex-col mt-10\">\r\n    <NavbarUser />\r\n    <div class=\"max-w-6xl mx-auto px-4 py-6 mt-[90px]\">\r\n      <div class=\"bg-white w-full max-w-4xl p-6 rounded-lg shadow-md\">\r\n        <h2 class=\"text-2xl font-semibold text-[#03a980] text-center mb-6\">Pendaftaran Anggota</h2>\r\n\r\n        <form @submit.prevent=\"submitPendaftaran\">\r\n          <div class=\"space-y-4\">\r\n            <input v-model=\"anggota.nama\" placeholder=\"Nama\" class=\"input-field w-full\" required />\r\n            <input v-model=\"anggota.nik\" placeholder=\"NIK\" class=\"input-field w-full\" required />\r\n            <input v-model=\"anggota.id\" placeholder=\"ID\" class=\"input-field w-full\" required />\r\n            \r\n            <select v-model=\"anggota.daerahWilayah\" class=\"input-field w-full\" required>\r\n              <option disabled value=\"\">Pilih Daerah/Wilayah</option>\r\n              <option value=\"Daerah\">Daerah</option>\r\n              <option value=\"Wilayah\">Wilayah</option>\r\n            </select>\r\n            \r\n            <select v-if=\"anggota.daerahWilayah === 'Daerah'\" v-model=\"anggota.daerah\" class=\"input-field w-full\" required>\r\n              <option value=\"NTB\">NTB</option>\r\n              <option value=\"Jawa Timur\">Jawa Timur</option>\r\n              <option value=\"Sumatera Barat\">Sumatera Barat</option>\r\n            </select>\r\n\r\n            <select v-if=\"anggota.daerahWilayah === 'Wilayah'\" v-model=\"anggota.wilayah\" class=\"input-field w-full\" required>\r\n              <option value=\"Kota Malang\">Kota Malang</option>\r\n              <option value=\"Surabaya\">Surabaya</option>\r\n            </select>\r\n\r\n            <button type=\"submit\" class=\"mt-6 w-full bg-[#03a980] text-white px-6 py-2 rounded-lg font-semibold hover:bg-[#028a66] transition\">\r\n              Ajukan Pendaftaran\r\n            </button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      anggota: {\r\n        nama: '',\r\n        nik: '',\r\n        id: '',\r\n        daerahWilayah: '',\r\n        daerah: '',\r\n        wilayah: '',\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    async submitPendaftaran() {\r\n      // Kirim data pendaftaran ke pusat untuk disetujui\r\n      const formData = new FormData();\r\n      formData.append(\"nama\", this.anggota.nama);\r\n      formData.append(\"nik\", this.anggota.nik);\r\n      formData.append(\"id\", this.anggota.id);\r\n      formData.append(\"daerahWilayah\", this.anggota.daerahWilayah);\r\n      formData.append(\"daerah\", this.anggota.daerah);\r\n      formData.append(\"wilayah\", this.anggota.wilayah);\r\n      \r\n      // Kirim data ke pusat (API endpoint misalnya \"/api/pusat/pendaftaran\")\r\n      try {\r\n        const response = await fetch(\"/api/pusat/pendaftaran\", {\r\n          method: \"POST\",\r\n          body: formData,\r\n        });\r\n        \r\n        if (response.ok) {\r\n          alert(\"Pendaftaran berhasil dikirim. Tunggu persetujuan pusat.\");\r\n        } else {\r\n          alert(\"Terjadi kesalahan saat pengajuan.\");\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Error:\", error);\r\n        alert(\"Gagal mengirim data pendaftaran.\");\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.input-field {\r\n  @apply p-3 border border-gray-300 rounded-lg bg-gray-100 focus:outline-none focus:ring-2 focus:ring-[#03a980];\r\n}\r\n</style>\r\n"],"mappings":"AAyCA,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;QACPC,IAAI,EAAE,EAAE;QACRC,GAAG,EAAE,EAAE;QACPC,EAAE,EAAE,EAAE;QACNC,aAAa,EAAE,EAAE;QACjBC,MAAM,EAAE,EAAE;QACVC,OAAO,EAAE;MACX;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,iBAAiBA,CAAA,EAAG;MACxB;MACA,MAAMC,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC/BD,QAAQ,CAACE,MAAM,CAAC,MAAM,EAAE,IAAI,CAACX,OAAO,CAACC,IAAI,CAAC;MAC1CQ,QAAQ,CAACE,MAAM,CAAC,KAAK,EAAE,IAAI,CAACX,OAAO,CAACE,GAAG,CAAC;MACxCO,QAAQ,CAACE,MAAM,CAAC,IAAI,EAAE,IAAI,CAACX,OAAO,CAACG,EAAE,CAAC;MACtCM,QAAQ,CAACE,MAAM,CAAC,eAAe,EAAE,IAAI,CAACX,OAAO,CAACI,aAAa,CAAC;MAC5DK,QAAQ,CAACE,MAAM,CAAC,QAAQ,EAAE,IAAI,CAACX,OAAO,CAACK,MAAM,CAAC;MAC9CI,QAAQ,CAACE,MAAM,CAAC,SAAS,EAAE,IAAI,CAACX,OAAO,CAACM,OAAO,