n        username: '',\r\n        firstName: '',\r\n        lastName: '',\r\n        email: '',\r\n        contactNumber: '',\r\n        avatar: ''\r\n      },\r\n      isEditModalOpen: false,\r\n      isLogoutConfirm: false\r\n    };\r\n  },\r\n  mounted() {\r\n    this.fetchUserData();\r\n  },\r\n  methods: {\r\n    openEditModal() {\r\n      this.isEditModalOpen = true;\r\n    },\r\n    closeEditModal() {\r\n      this.isEditModalOpen = false;\r\n    },\r\n    async saveProfile() {\r\n      const formData = new FormData();\r\n      formData.append(\"fullName\", this.user.firstName + ' ' + this.user.lastName);\r\n\r\n      try {\r\n        await axios.put(\"http://192.168.1.11:5000/users/update\", formData);\r\n        alert(\"Profil berhasil diperbarui!\");\r\n        this.isEditModalOpen = false;\r\n      } catch (error) {\r\n        console.error(\"Error updating profile:\", error);\r\n        alert(\"Gagal memperbarui profil.\");\r\n      }\r\n    },\r\n    openChangePasswordModal() {\r\n      // Handle Change Password Logic here\r\n    },\r\n    logout() {\r\n      this.isLogoutConfirm = true;\r\n    },\r\n    confirmLogout() {\r\n      localStorage.removeItem(\"authToken\");\r\n      localStorage.removeItem(\"userData\");\r\n      alert(\"Logout berhasil!\");\r\n      this.$router.push(\"/login\");\r\n    },\r\n    cancelLogout() {\r\n      this.isLogoutConfirm = false;\r\n    },\r\n    goToCompleteProfile() {\r\n      // Redirect to the profile completion page\r\n      this.$router.push(\"/user/complete-profile\");\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n</style>\r\n"],"mappings":";AAqEA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,QAAQ,EAAE,EAAE;QACZC,SAAS,EAAE,EAAE;QACbC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,aAAa,EAAE,EAAE;QACjBC,MAAM,EAAE;MACV,CAAC;MACDC,eAAe,EAAE,KAAK;MACtBC,eAAe,EAAE;IACnB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,aAAa,CAAC,CAAC;EACtB,CAAC;EACDC,OAAO,EAAE;IACPC