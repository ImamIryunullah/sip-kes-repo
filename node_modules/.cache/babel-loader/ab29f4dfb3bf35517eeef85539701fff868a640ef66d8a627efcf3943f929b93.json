{"ast":null,"code":"import NavbarUser from '@/components/NavbarUser.vue';\nexport default {\n  components: {\n    NavbarUser\n  },\n  data() {\n    return {\n      anggota: {\n        nama: \"\",\n        nik: \"\",\n        noTelepon: \"\",\n        email: \"\",\n        daerahWilayah: \"\",\n        daerah: \"\",\n        wilayah: \"\",\n        alamat: \"\",\n        instansi: \"\"\n      }\n    };\n  },\n  methods: {\n    async submitPendaftaran() {\n      // Pastikan data yang dibutuhkan sudah lengkap\n      if (!this.anggota.daerahWilayah || this.anggota.daerahWilayah === 'Daerah' && !this.anggota.daerah || this.anggota.daerahWilayah === 'Wilayah' && !this.anggota.wilayah) {\n        alert(\"Silakan pilih daerah atau wilayah.\");\n        return;\n      }\n      const formData = new FormData();\n      formData.append(\"nama\", this.anggota.nama);\n      formData.append(\"nik\", this.anggota.nik);\n      formData.append(\"noTelepon\", this.anggota.noTelepon);\n      formData.append(\"email\", this.anggota.email);\n      formData.append(\"daerahWilayah\", this.anggota.daerahWilayah);\n      formData.append(\"daerah\", this.anggota.daerah);\n      formData.append(\"wilayah\", this.anggota.wilayah);\n      formData.append(\"alamat\", this.anggota.alamat);\n      formData.append(\"instansi\", this.anggota.instansi);\n\n      // Kirim data ke pusat (misalnya menggunakan axios atau fetch)\n      try {\n        const response = await fetch(\"/api/pusat/pendaftaran\", {\n          method: \"POST\",\n          body: formData\n        });\n        if (response.ok) {\n          alert(\"Pendaftaran Anggota berhasil dikirim!\");\n        } else {\n          alert(\"Terjadi kesalahan. Silakan coba lagi.\");\n        }\n      } catch (error) {\n        console.error(\"Error:\", error);\n        alert(\"Gagal mengirim data ke pusat.\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["NavbarUser","components","data","anggota","nama","nik","noTelepon","email","daerahWilayah","daerah","wilayah","alamat","instansi","methods","submitPendaftaran","alert","formData","FormData","append","response","fetch","method","body","ok","error","console"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\user\\PengajuanSertifikat.vue"],"sourcesContent":["<template>\r\n  <div class=\"w-screen h-screen flex flex-col mt-10\">\r\n    <NavbarUser />\r\n    <div class=\"flex flex-col items-center p-10 bg-gray-100 flex-grow\">\r\n      <div class=\"bg-white w-full max-w-2xl p-6 rounded-lg shadow-md\">\r\n        <h2 class=\"text-2xl font-semibold text-[#03a980] text-center mb-6\">Pengajuan Pendaftaran Anggota</h2>\r\n\r\n        <form @submit.prevent=\"submitPendaftaran\">\r\n          <div class=\"grid grid-cols-1 gap-4\">\r\n            <input v-model=\"anggota.nama\" type=\"text\" placeholder=\"Nama Lengkap\" class=\"input-field\" required />\r\n            <input v-model=\"anggota.nik\" type=\"text\" placeholder=\"NIK\" class=\"input-field\" required />\r\n            <input v-model=\"anggota.noTelepon\" type=\"text\" placeholder=\"Nomor Telepon\" class=\"input-field\" required />\r\n            <input v-model=\"anggota.email\" type=\"email\" placeholder=\"Email\" class=\"input-field\" required />\r\n            \r\n            <select v-model=\"anggota.daerahWilayah\" class=\"input-field\" required>\r\n              <option disabled value=\"\">Pilih Daerah/Wilayah</option>\r\n              <option value=\"Daerah\">Daerah</option>\r\n              <option value=\"Wilayah\">Wilayah</option>\r\n            </select>\r\n\r\n            <!-- Input for Daerah -->\r\n            <select v-if=\"anggota.daerahWilayah === 'Daerah'\" v-model=\"anggota.daerah\" class=\"input-field\" required>\r\n              <option disabled value=\"\">Pilih Daerah</option>\r\n              <option value=\"NTB\">NTB</option>\r\n              <option value=\"Jawa Timur\">Jawa Timur</option>\r\n              <option value=\"Sumatera Barat\">Sumatera Barat</option>\r\n            </select>\r\n\r\n            <!-- Input for Wilayah -->\r\n            <select v-if=\"anggota.daerahWilayah === 'Wilayah'\" v-model=\"anggota.wilayah\" class=\"input-field\" required>\r\n              <option disabled value=\"\">Pilih Wilayah</option>\r\n              <option value=\"Kota Malang\">Kota Malang</option>\r\n              <option value=\"Surabaya\">Surabaya</option>\r\n              <option value=\"Gresik\">Gresik</option>\r\n            </select>\r\n\r\n            <input v-model=\"anggota.alamat\" type=\"text\" placeholder=\"Alamat Lengkap\" class=\"input-field\" required />\r\n            <input v-model=\"anggota.instansi\" type=\"text\" placeholder=\"Instansi/Lembaga\" class=\"input-field\" required />\r\n\r\n            <button type=\"submit\" class=\"mt-6 w-full bg-[#03a980] text-white px-6 py-2 rounded-lg font-bold hover:bg-[#028a66] transition\">\r\n              Ajukan Pendaftaran\r\n            </button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavbarUser from '@/components/NavbarUser.vue';\r\n\r\nexport default {\r\n  components: {\r\n    NavbarUser\r\n  },\r\n  data() {\r\n    return {\r\n      anggota: {\r\n        nama: \"\",\r\n        nik: \"\",\r\n        noTelepon: \"\",\r\n        email: \"\",\r\n        daerahWilayah: \"\",\r\n        daerah: \"\",\r\n        wilayah: \"\",\r\n        alamat: \"\",\r\n        instansi: \"\",\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    async submitPendaftaran() {\r\n      // Pastikan data yang dibutuhkan sudah lengkap\r\n      if (!this.anggota.daerahWilayah || (this.anggota.daerahWilayah === 'Daerah' && !this.anggota.daerah) || (this.anggota.daerahWilayah === 'Wilayah' && !this.anggota.wilayah)) {\r\n        alert(\"Silakan pilih daerah atau wilayah.\");\r\n        return;\r\n      }\r\n\r\n      const formData = new FormData();\r\n      formData.append(\"nama\", this.anggota.nama);\r\n      formData.append(\"nik\", this.anggota.nik);\r\n      formData.append(\"noTelepon\", this.anggota.noTelepon);\r\n      formData.append(\"email\", this.anggota.email);\r\n      formData.append(\"daerahWilayah\", this.anggota.daerahWilayah);\r\n      formData.append(\"daerah\", this.anggota.daerah);\r\n      formData.append(\"wilayah\", this.anggota.wilayah);\r\n      formData.append(\"alamat\", this.anggota.alamat);\r\n      formData.append(\"instansi\", this.anggota.instansi);\r\n\r\n      // Kirim data ke pusat (misalnya menggunakan axios atau fetch)\r\n      try {\r\n        const response = await fetch(\"/api/pusat/pendaftaran\", {\r\n          method: \"POST\",\r\n          body: formData,\r\n        });\r\n\r\n        if (response.ok) {\r\n          alert(\"Pendaftaran Anggota berhasil dikirim!\");\r\n        } else {\r\n          alert(\"Terjadi kesalahan. Silakan coba lagi.\");\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Error:\", error);\r\n        alert(\"Gagal mengirim data ke pusat.\");\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.input-field {\r\n  @apply p-3 border border-gray-300 rounded-lg bg-gray-100 focus:outline-none focus:ring-2 focus:ring-[#03a980];\r\n}\r\n</style>\r\n"],"mappings":"AAkDA,OAAOA,UAAS,MAAO,6BAA6B;AAEpD,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE;QACPC,IAAI,EAAE,EAAE;QACRC,GAAG,EAAE,EAAE;QACPC,SAAS,EAAE,EAAE;QACbC,KAAK,EAAE,EAAE;QACTC,aAAa,EAAE,EAAE;QACjBC,MAAM,EAAE,EAAE;QACVC,OAAO,EAAE,EAAE;QACXC,MAAM,EAAE,EAAE;QACVC,QAAQ,EAAE;MACZ;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,iBAAiBA,CAAA,EAAG;MACxB;MACA,IAAI,CAAC,IAAI,CAACX,OAAO,CAACK,aAAY,IAAM,IAAI,CAACL,OAAO,CAACK,aAAY,KAAM,QAAO,IAAK,CAAC,IAAI,CAACL,OAAO,CAACM,MAAM,IAAM,IAAI,CAACN,OAAO,CAACK,aAAY,KAAM,SAAQ,IAAK,CAAC,IAAI,CAACL,OAAO,CAACO,OAAQ,EAAE;QAC3KK,KAAK,CAAC,oCAAoC,CAAC;QAC3C;MACF;MAEA,MAAMC,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC/BD,QAAQ,CAACE,MAAM,CAAC,MAAM,EAAE,IAAI,CAACf,OAAO,C