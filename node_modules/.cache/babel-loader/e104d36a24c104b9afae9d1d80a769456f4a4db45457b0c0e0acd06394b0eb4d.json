{"ast":null,"code":"import NavbarUser from '@/components/NavbarUser.vue';\nexport default {\n  components: {\n    NavbarUser\n  },\n  data() {\n    return {\n      userComplaints: [{\n        id: \"001\",\n        namapengadu: \"Tatsss\",\n        judul: \"Keterlambatan Sertifikat\",\n        status: \"Diproses\",\n        tanggapan: \"Pengaduan Anda sedang kami proses. Harap bersabar.\"\n      }, {\n        id: \"002\",\n        namapengadu: \"arya\",\n        judul: \"Fasilitas Tidak Memadai\",\n        status: \"Disetujui\",\n        tanggapan: \"Terima kasih atas laporan Anda. Fasilitas akan segera diperbaiki.\"\n      }],\n      currentPage: 1,\n      itemsPerPage: 10\n    };\n  },\n  computed: {\n    totalPages() {\n      return Math.ceil(this.complaints.length / this.itemsPerPage);\n    },\n    userComplaints() {\n      const start = (this.currentPage - 1) * this.itemsPerPage;\n      const end = start + this.itemsPerPage;\n      return this.complaints.slice(start, end);\n    }\n  },\n  methods: {\n    getStatusClass(status) {\n      switch (status) {\n        case \"Diproses\":\n          return \"text-yellow-500\";\n        case \"Disetujui\":\n          return \"text-green-500\";\n        case \"Ditolak\":\n          return \"text-red-500\";\n        default:\n          return \"text-gray-500\";\n      }\n    }\n  }\n};","map":{"version":3,"names":["NavbarUser","components","data","userComplaints","id","namapengadu","judul","status","tanggapan","currentPage","itemsPerPage","computed","totalPages","Math","ceil","complaints","length","start","end","slice","methods","getStatusClass"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\user\\FeedbackPengaduan.vue"],"sourcesContent":["<template>\r\n    <div class=\"w-screen bg-gray-100 mt-12\">\r\n      <NavbarUser />\r\n      <div class=\"flex flex-col items-center p-4 sm:p-6 md:p-10 bg-gray-100 flex-grow\">\r\n        <div class=\"bg-white w-full max-w-4xl p-4 sm:p-6 rounded-lg shadow-md\">\r\n          <h2 class=\"text-xl sm:text-2xl font-semibold text-[#03a980] text-center mb-4 sm:mb-6\">Tanggapan Pengaduan Saya</h2>\r\n        \r\n        <div class=\"flex flex-col sm:flex-row justify-between items-center mb-4\">\r\n            <button @click=\"prevPage\" :disabled=\"currentPage === 1\" class=\"p-2 bg-gray-300 text-gray-700 rounded-md disabled:opacity-50\">Previous</button>\r\n          <span class=\"my-2 sm:my-0\">Page {{ currentPage }} of {{ totalPages }}</span>\r\n          <button @click=\"nextPage\" :disabled=\"currentPage === totalPages\" class=\"p-2 bg-gray-300 text-gray-700 rounded-md disabled:opacity-50\">Next</button>\r\n        </div>\r\n\r\n          <div v-if=\"userComplaints.length > 0\" class=\"space-y-4\">\r\n            <div v-for=\"(complaint, index) in userComplaints\" :key=\"index\" class=\"p-4 bg-gray-50 rounded-lg shadow-md border border-gray-300\">\r\n              <p><strong>ID Pengaduan:</strong> {{ complaint.id }}</p>\r\n              <p><strong>Nama Pengadu:</strong> {{ complaint.namapengadu }}</p>\r\n              <p><strong>Judul:</strong> {{ complaint.judul }}</p>\r\n              <p><strong>Status:</strong> <span :class=\"getStatusClass(complaint.status)\">{{ complaint.status }}</span></p>\r\n              <p v-if=\"complaint.tanggapan\" class=\"mt-2\"><strong>Tanggapan:</strong> {{ complaint.tanggapan }}</p>\r\n              <p v-else class=\"text-gray-500\">Belum ada tanggapan.</p>\r\n            </div>\r\n          </div>\r\n          <div v-else class=\"text-center text-gray-500\">Anda belum memiliki pengaduan.</div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import NavbarUser from '@/components/NavbarUser.vue';\r\n  \r\n  export default {\r\n    components: { NavbarUser },\r\n    data() {\r\n      return {\r\n        userComplaints: [\r\n          {\r\n            id: \"001\",\r\n            namapengadu: \"Tatsss\",\r\n            judul: \"Keterlambatan Sertifikat\",\r\n            status: \"Diproses\",\r\n            tanggapan: \"Pengaduan Anda sedang kami proses. Harap bersabar.\"\r\n          },\r\n          {\r\n            id: \"002\",\r\n            namapengadu: \"arya\",\r\n            judul: \"Fasilitas Tidak Memadai\",\r\n            status: \"Disetujui\",\r\n            tanggapan: \"Terima kasih atas laporan Anda. Fasilitas akan segera diperbaiki.\"\r\n          }\r\n        ],\r\n        currentPage: 1,\r\n        itemsPerPage: 10\r\n      };\r\n    },\r\n    computed: {\r\n        totalPages(){\r\n            return Math.ceil(this.complaints.length / this.itemsPerPage);\r\n        },\r\n        userComplaints(){\r\n      const start = (this.currentPage - 1) * this.itemsPerPage;\r\n      const end = start + this.itemsPerPage;\r\n      return this.complaints.slice(start, end);\r\n    }\r\n    },\r\n    methods: {\r\n      getStatusClass(status) {\r\n        switch (status) {\r\n          case \"Diproses\": return \"text-yellow-500\";\r\n          case \"Disetujui\": return \"text-green-500\";\r\n          case \"Ditolak\": return \"text-red-500\";\r\n          default: return \"text-gray-500\";\r\n        }\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  </style>\r\n  "],"mappings":"AA8BE,OAAOA,UAAS,MAAO,6BAA6B;AAEpD,eAAe;EACbC,UAAU,EAAE;IAAED;EAAW,CAAC;EAC1BE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,cAAc,EAAE,CACd;QACEC,EAAE,EAAE,KAAK;QACTC,WAAW,EAAE,QAAQ;QACrBC,KAAK,EAAE,0BAA0B;QACjCC,MAAM,EAAE,UAAU;QAClBC,SAAS,EAAE;MACb,CAAC,EACD;QACEJ,EAAE,EAAE,KAAK;QACTC,WAAW,EAAE,MAAM;QACnBC,KAAK,EAAE,yBAAyB;QAChCC,MAAM,EAAE,WAAW;QACnBC,SAAS,EAAE;MACb,EACD;MACDC,WAAW,EAAE,CAAC;MACdC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACNC,UAAUA,CAAA,EAAE;MACR,OAAOC,IAAI,CAACC,IAAI,CAAC,IAAI,CAACC,UAAU,CAACC,MAAK,GAAI,IAAI,CAACN,YAAY,CAAC;IAChE,CAAC;IACDP,cAAcA,CAAA,EAAE;MAClB,MAAMc,KAAI,GAAI,CAAC,IAAI,CAACR,WAAU,GAAI,CAAC,IAAI,IAAI,CAACC,YAAY;MACxD,MAAMQ,GAAE,GAAID,KAAI,GAAI,IAAI,CAACP,YAAY;MACrC,OAAO,IAAI,CAAC