;\n      formData.append(\"kotaKabupaten\", this.form.kotaKabupaten);\n      formData.append(\"jabatan\", this.form.jabatan);\n      formData.append(\"alamat\", this.form.alamat);\n      formData.append(\"instansi\", this.form.instansi);\n      formData.append(\"foto3x4\", this.form.foto3x4);\n      formData.append(\"fotoKtp\", this.form.fotoKtp);\n      try {\n        const response = await ApiService.createPendaftaran(formData); // Menggunakan ApiService untuk membuat pendaftaran\n        if (response.status === 200) {\n          alert(\"Pendaftaran Anggota berhasil dikirim!\");\n          this.$router.push(\"/\"); // Redirect to another page\n        } else {\n          alert(\"Terjadi kesalahan saat mengirim pendaftaran.\");\n        }\n      } catch (error) {\n        console.error(\"Error:\", error);\n        alert(\"Terjadi kesalahan saat mengirim pendaftaran.\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["NavbarUser","ApiService","components","data","form","nama","nik","noTelepon","email","tipePendaftaran","provinsi","kotaKabupaten","jabatan","alamat","instansi","foto3x4","fotoKtp","methods","handleFoto3x4Upload","event","target","files","handleKtpUpload","submitForm","alert","formData","FormData","append","response","createPendaftaran","status","$router","push","error","console"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\user\\PendaftaranAnggotaView.vue"],"sourcesContent":["<template>\r\n  <div class=\"w-screen h-screen flex flex-col mt-10\">\r\n    <!-- Navbar -->\r\n    <NavbarUser />\r\n\r\n    <div class=\"flex flex-col items-center p-10 bg-gray-100 flex-grow\">\r\n      <div class=\"bg-white w-full max-w-2xl p-6 rounded-lg shadow-md\">\r\n        <h2 class=\"text-2xl font-semibold text-[#03a980] text-center mb-6\">Pendaftaran Anggota</h2>\r\n\r\n        <!-- Form Pendaftaran -->\r\n        <form @submit.prevent=\"submitForm\">\r\n          <div class=\"grid grid-cols-1 gap-4\">\r\n            <input v-model=\"form.nama\" type=\"text\" placeholder=\"Nama Lengkap\" class=\"input-field\" required />\r\n            <input v-model=\"form.nik\" type=\"text\" placeholder=\"NIK\" class=\"input-field\" required />\r\n            <input v-model=\"form.noTelepon\" type=\"text\" placeholder=\"Nomor Telepon\" class=\"input-field\" required />\r\n            <input v-model=\"form.email\" type=\"email\" placeholder=\"Email\" class=\"input-field\" required />\r\n            \r\n            <!-- Pilih untuk Daftar Provinsi atau Kota/Kabupaten -->\r\n            <select v-model=\"form.tipePendaftaran\" class=\"input-field\" required>\r\n              <option disabled value=\"\">Pilih Tipe Pendaftaran</option>\r\n              <option value=\"Provinsi\">Provinsi</option>\r\n              <option value=\"Kota/Kabupaten\">Kota/Kabupaten</option>\r\n            </select>\r\n\r\n            <!-- Provinsi -->\r\n            <select v-if=\"form.tipePendaftaran === 'Provinsi'\" v-model=\"form.provinsi\" class=\"input-field\" required>\r\n              <option disabled value=\"\">Pilih Provinsi</option>\r\n              <option value=\"NTB\">NTB</option>\r\n              <option value=\"Jawa Timur\">Jawa Timur</option>\r\n              <option value=\"Sumatera Barat\">Sumatera Barat</option>\r\n            </select>\r\n\r\n            <!-- Kota/Kabupaten hanya muncul jika tipe pendaftaran adalah Kota/Kabupaten -->\r\n            <select v-if=\"form.tipePendaftaran === 'Kota/Kabupaten'\" v-model=\"form.kotaKabupaten\" class=\"input-field\" required>\r\n              <option disabled value=\"\">Pilih Kota/Kabupaten</option>\r\n              <option value=\"Kota Malang\">Kota Malang</option>\r\n              <option value=\"Surabaya\">Surabaya</option>\r\n              <option value=\"Gresik\">Gresik</option>\r\n            </select>\r\n\r\n            <!-- Jabatan LPKNI -->\r\n            <select v-model=\"form.jabatan\" class=\"input-field\" required>\r\n              <option disabled value=\"\">Pilih Jabatan LPKNI</option>\r\n              <option value=\"Anggota Direktorat\">Anggota Direktorat</option>\r\n              <option value=\"Anggota Sekretariat\">Anggota Sekretariat</option>\r\n              <option value=\"Anggota Divisi Keuangan\">Anggota Divisi Keuangan</option>\r\n              <option value=\"Anggota Divisi Sumber Daya Manusia\">Anggota Divisi Sumber Daya Manusia</option>\r\n              <option value=\"Lainnya\">Lainnya</option>\r\n            </select>\r\n\r\n            <input v-model=\"form.alamat\" type=\"text\" placeholder=\"Alamat Lengkap\" class=\"input-field\" required />\r\n            \r\n            <!-- Upload Foto 3x4 -->\r\n            <label class=\"text-sm text-gray-600\">Upload Foto 3x4:</label>\r\n            <input type=\"file\" @change=\"handleFoto3x4Upload\" class=\"input-field\" required />\r\n\r\n            <!-- Upload Foto KTP -->\r\n            <label class=\"text-sm text-gray-600\">Upload Foto KTP:</label>\r\n            <input type=\"file\" @change=\"handleKtpUpload\" class=\"input-field\" required />\r\n\r\n            <button type=\"submit\" class=\"mt-6 w-full bg-[#03a980] text-white px-6 py-2 rounded-lg font-bold hover:bg-[#028a66] transition\">\r\n              Ajukan Pendaftaran\r\n            </button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavbarUser from '@/components/NavbarUser.vue';\r\nimport ApiService from '@/service/api.js';\r\n\r\nexport default {\r\n  components: {\r\n    NavbarUser\r\n  },\r\n  data() {\r\n    return {\r\n      form: {\r\n        nama: \"\",\r\n        nik: \"\",\r\n        noTelepon: \"\",\r\n        email: \"\",\r\n        tipePendaftaran: \"\",\r\n        provinsi: \"\",\r\n        kotaKabupaten: \"\",\r\n        jabatan: \"\",\r\n        alamat: \"\",\r\n        instansi: \"\",\r\n        foto3x4: null,\r\n        fotoKtp: null,\r\n      }\r\n    };\r\n  },\r\n  methods: {\r\n    handleFoto3x4Upload(event) {\r\n      this.form.foto3x4 = event.target.files[0];\r\n    },\r\n    handleKtpUpload(event) {\r\n      this.form.fotoKtp = event.target.files[0];\r\n    },\r\n\r\n    // This method handles the form submission\r\n    async submitForm() {\r\n      // Validasi form\r\n      if (!this.form.tipePendaftaran) {\r\n        alert(\"Silakan pilih tipe pendaftaran.\");\r\n        return;\r\n      }\r\n\r\n      if (this.form.tipePendaftaran === 'Kota/Kabupaten' && !this.form.kotaKabupaten) {\r\n        alert(\"Silakan pilih Kota/Kabupaten.\");\r\n        return;\r\n      }\r\n\r\n      if (this.form.tipePendaftaran === 'Provinsi' && !this.form.provinsi) {\r\n        alert(\"Silakan pilih Provinsi.\");\r\n        return;\r\n      }\r\n\r\n      if (!this.form.foto3x4 || !this.form.fotoKtp) {\r\n        alert(\"Silakan unggah Foto 3x4 dan Foto KTP.\");\r\n        return;\r\n      }\r\n\r\n      if (!this.form.jabatan) {\r\n        alert(\"Silakan pilih Jabatan LPKNI.\");\r\n        return;\r\n      }\r\n\r\n      // Create formData to send the data and files to backend\r\n      const formData = new FormData();\r\n      formData.append(\"nama\", this.form.nama);\r\n      formData.append(\"nik\", this.form.nik);\r\n      formData.append(\"noTelepon\", this.form.noTelepon);\r\n      formData.append(\"email\", this.form.email);\r\n      formData.append(\"tipePendaftaran\", this.form.tipePendaftaran);\r\n      formData.append(\"provinsi\", this.form.provinsi);\r\n      formData.append(\"kotaKabupaten\", this.form.kotaKabupaten);\r\n      formData.append(\"jabatan\", this.form.jabatan);\r\n      formData.append(\"alamat\", this.form.alamat);\r\n      formData.append(\"instansi\", this.form.instansi);\r\n      formData.append(\"foto3x4\", this.form.foto3x4);\r\n      formData.append(\"fotoKtp\", this.form.fotoKtp);\r\n\r\n      try {\r\n        const response = await ApiService.createPendaftaran(formData); // Menggunakan ApiService untuk membuat pendaftaran\r\n        if (response.status === 200) {\r\n          alert(\"Pendaftaran Anggota berhasil dikirim!\");\r\n          this.$router.push(\"/\"); // Redirect to another page\r\n        } else {\r\n          alert(\"Terjadi kesalahan saat mengirim pendaftaran.\");\r\n        }\r\n      } catch (error) {\r\n        console.error(\"Error:\", error);\r\n        alert(\"Terjadi kesalahan saat mengirim pendaftaran.\");\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.input-field {\r\n  @apply p-3 border border-gray-300 rounded-lg bg-gray-100 focus:outline-none focus:ring-2 focus:ring-[#03a980];\r\n}\r\n</style>\r\n"],"mappings":";AAuEA,OAAOA,UAAS,MAAO,6BAA6B;AACpD,OAAOC,UAAS,MAAO,kBAAkB;AAEzC,eAAe;EACbC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,IAAI,EAAE,EAAE;QACRC,GAAG,EAAE,EAAE;QACPC,SAAS,EAAE,EAAE;QACbC,KAAK,EAAE,EAAE;QACTC,eAAe,EAAE,EAAE;QACnBC,QAAQ,EAAE,EAAE;QACZC,aAAa,EAAE,EAAE;QACjBC,OAAO,EAAE,EAAE;QACXC,MAAM,EAAE,EAAE;QACVC,QAAQ,EAAE,EAAE;QACZC,OAAO,EAAE,IAAI;QACbC,OAAO,EAAE;MACX;IACF,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,mBAAmBA,CAACC,KAAK,EAAE;MACzB,IAAI,CAACf,IAAI,CAACW,OAAM,GAAII,KAAK,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;IAC3C,CAAC;IACDC,eAAeA,CAACH,KAAK,EAAE;MACrB,IAAI,CAACf,IAAI,CAACY,OAAM,GAAIG,KAAK,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;IAC3C,CAAC;IAED;IACA,MAAME,UAAUA,CAAA,EAAG;MACjB;MACA,IAAI,CAAC,IAAI,CAACnB,IAAI,CAACK,eAAe,EAAE;QAC9Be,KAAK,CAAC,iCAAiC,CAAC;QACxC;MACF;MAEA,IAAI,IAAI,CAACpB,IAAI,CAACK,eAAc,KAAM,gBAAe,IAAK,CAAC,IAAI,CAACL,IAAI,CAACO,aAAa,EAAE;QAC9Ea,KAAK,CAAC,+BAA+B,CAAC;QACtC;MACF;MAEA,IAAI,IAAI,CAACpB,IAAI,CAACK,eAAc,KAAM,UAAS,IAAK,CAAC,IAAI,CAACL,IAAI,CAACM,QAAQ,EAAE;QACnEc,KAAK,CAAC,yBAAyB,CAAC;QAChC;MACF;MAEA,IAAI,CAAC,IAAI,CAACpB,IAAI,CAACW,OAAM,IAAK,CAAC,IAAI,CAACX,IAAI,CAACY,OAAO,EAAE;QAC5CQ,KAAK,CAAC,uCAAuC,CAAC;QAC9C;MACF;MAEA,IAAI,CAAC,IAAI,CAACpB,IAAI,CAACQ,OAAO,EAAE;QACtBY,KAAK,CAAC,8BAA8B,CAAC;QACrC;MACF;;MAEA;MACA,MAAMC,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC/BD,QAAQ,CAACE,MAAM,CAA