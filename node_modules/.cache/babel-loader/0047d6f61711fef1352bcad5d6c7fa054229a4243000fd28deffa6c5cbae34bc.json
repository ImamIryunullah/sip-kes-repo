{"ast":null,"code":"import NavbarPusat from '@/components/NavbarPusat.vue';\nexport default {\n  components: {\n    NavbarPusat\n  },\n  data() {\n    return {\n      pendaftaranData: [{\n        nama: \"John Doe\",\n        nik: \"1234567890123456\",\n        provinsi: \"Jawa Timur\",\n        kotaKabupaten: \"Kota Malang\",\n        instansi: \"LPKNI Malang\",\n        status: \"Diproses\",\n        foto3x4Url: \"https://via.placeholder.com/150\",\n        fotoKtpUrl: \"https://via.placeholder.com/150\"\n      }, {\n        nama: \"Jane Smith\",\n        nik: \"6543210987654321\",\n        provinsi: \"NTB\",\n        kotaKabupaten: \"Mataram\",\n        instansi: \"LPKNI Lombok\",\n        status: \"Disetujui\",\n        foto3x4Url: \"https://via.placeholder.com/150\",\n        fotoKtpUrl: \"https://via.placeholder.com/150\"\n      }] // Data dummy pendaftaran anggota\n    };\n  },\n  methods: {\n    approvePendaftaran(pendaftaran) {\n      // Proses persetujuan pendaftaran anggota\n      alert(`Pendaftaran ${pendaftaran.nama} telah disetujui.`);\n    },\n    rejectPendaftaran(pendaftaran) {\n      // Proses penolakan pendaftaran anggota\n      alert(`Pendaftaran ${pendaftaran.nama} telah ditolak.`);\n    }\n  }\n};","map":{"version":3,"names":["NavbarPusat","components","data","pendaftaranData","nama","nik","provinsi","kotaKabupaten","instansi","status","foto3x4Url","fotoKtpUrl","methods","approvePendaftaran","pendaftaran","alert","rejectPendaftaran"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\pusat\\RegisterPusatView.vue"],"sourcesContent":["<template>\r\n  <div class=\"w-screen h-screen flex flex-col mt-10\">\r\n    <NavbarPusat />\r\n    <div class=\"flex flex-col items-center p-10 bg-gray-100 flex-grow\">\r\n      <div class=\"bg-white w-full max-w-4xl p-6 rounded-lg shadow-md\">\r\n        <h2 class=\"text-2xl font-semibold text-[#03a980] text-center mb-6\">Data Pendaftaran Anggota</h2>\r\n\r\n        <!-- Tabel untuk menampilkan data pendaftaran anggota -->\r\n        <div v-if=\"pendaftaranData.length > 0\">\r\n          <table class=\"min-w-full table-auto\">\r\n            <thead>\r\n              <tr class=\"border-b\">\r\n                <th class=\"px-4 py-2 text-left\">Nama</th>\r\n                <th class=\"px-4 py-2 text-left\">NIK</th>\r\n                <th class=\"px-4 py-2 text-left\">Provinsi</th>\r\n                <th class=\"px-4 py-2 text-left\">Kota/Kabupaten</th>\r\n                <th class=\"px-4 py-2 text-left\">Instansi</th>\r\n                <th class=\"px-4 py-2 text-left\">Status</th>\r\n                <th class=\"px-4 py-2 text-left\">Foto 3x4</th>\r\n                <th class=\"px-4 py-2 text-left\">Foto KTP</th>\r\n                <th class=\"px-4 py-2 text-left\">Aksi</th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <tr v-for=\"(pendaftaran, index) in pendaftaranData\" :key=\"index\" class=\"border-b\">\r\n                <td class=\"px-4 py-2\">{{ pendaftaran.nama }}</td>\r\n                <td class=\"px-4 py-2\">{{ pendaftaran.nik }}</td>\r\n                <td class=\"px-4 py-2\">{{ pendaftaran.provinsi }}</td>\r\n                <td class=\"px-4 py-2\">{{ pendaftaran.kotaKabupaten }}</td>\r\n                <td class=\"px-4 py-2\">{{ pendaftaran.instansi }}</td>\r\n                <td class=\"px-4 py-2\">{{ pendaftaran.status }}</td>\r\n                <td class=\"px-4 py-2\">\r\n                  <img :src=\"pendaftaran.foto3x4Url\" alt=\"Foto 3x4\" class=\"w-12 h-12 object-cover rounded-full\" />\r\n                </td>\r\n                <td class=\"px-4 py-2\">\r\n                  <img :src=\"pendaftaran.fotoKtpUrl\" alt=\"Foto KTP\" class=\"w-12 h-12 object-cover rounded-full\" />\r\n                </td>\r\n                <td class=\"px-3 py-2\">\r\n                  <button @click=\"approvePendaftaran(pendaftaran)\" class=\"bg-green-500 text-white px-4 py-2 rounded-md\">Setujui</button>\r\n                  <button @click=\"rejectPendaftaran(pendaftaran)\" class=\"bg-red-500 text-white px-4 py-2 rounded-md\">Tolak</button>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </div>\r\n        <div v-else class=\"text-center text-gray-500\">Tidak ada data pendaftaran anggota.</div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavbarPusat from '@/components/NavbarPusat.vue';\r\n\r\nexport default {\r\n  components: {\r\n    NavbarPusat\r\n  },\r\n  data() {\r\n    return {\r\n      pendaftaranData: [\r\n        {\r\n          nama: \"John Doe\",\r\n          nik: \"1234567890123456\",\r\n          provinsi: \"Jawa Timur\",\r\n          kotaKabupaten: \"Kota Malang\",\r\n          instansi: \"LPKNI Malang\",\r\n          status: \"Diproses\",\r\n          foto3x4Url: \"https://via.placeholder.com/150\",\r\n          fotoKtpUrl: \"https://via.placeholder.com/150\"\r\n        },\r\n        {\r\n          nama: \"Jane Smith\",\r\n          nik: \"6543210987654321\",\r\n          provinsi: \"NTB\",\r\n          kotaKabupaten: \"Mataram\",\r\n          instansi: \"LPKNI Lombok\",\r\n          status: \"Disetujui\",\r\n          foto3x4Url: \"https://via.placeholder.com/150\",\r\n          fotoKtpUrl: \"https://via.placeholder.com/150\"\r\n        }\r\n      ] // Data dummy pendaftaran anggota\r\n    };\r\n  },\r\n  methods: {\r\n    approvePendaftaran(pendaftaran) {\r\n      // Proses persetujuan pendaftaran anggota\r\n      alert(`Pendaftaran ${pendaftaran.nama} telah disetujui.`);\r\n    },\r\n    rejectPendaftaran(pendaftaran) {\r\n      // Proses penolakan pendaftaran anggota\r\n      alert(`Pendaftaran ${pendaftaran.nama} telah ditolak.`);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Styling untuk tabel */\r\ntable {\r\n  width: 100%;\r\n  border-collapse: collapse;\r\n}\r\nth, td {\r\n  text-align: left;\r\n  padding: 12px;\r\n}\r\nth {\r\n  background-color: #028a66;\r\n  color: white;\r\n}\r\ntr:hover {\r\n  background-color: #f5f5f5;\r\n}\r\nbutton {\r\n  margin-right: 10px;\r\n}\r\n</style>\r\n"],"mappings":"AAoDA,OAAOA,WAAU,MAAO,8BAA8B;AAEtD,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,eAAe,EAAE,CACf;QACEC,IAAI,EAAE,UAAU;QAChBC,GAAG,EAAE,kBAAkB;QACvBC,QAAQ,EAAE,YAAY;QACtBC,aAAa,EAAE,aAAa;QAC5BC,QAAQ,EAAE,cAAc;QACxBC,MAAM,EAAE,UAAU;QAClBC,UAAU,EAAE,iCAAiC;QAC7CC,UAAU,EAAE;MACd,CAAC,EACD;QACEP,IAAI,EAAE,YAAY;QAClBC,GAAG,EAAE,kBAAkB;QACvBC,QAAQ,EAAE,KAAK;QACfC,aAAa,EAAE,SAAS;QACxBC,QAAQ,EAAE,cAAc;QACxBC,MAAM,EAAE,WAAW;QACnBC,UAAU,EAAE,iCAAiC;QAC7CC,UAAU,EAAE;MACd,EACF,CAAE;IACJ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,kBAAkBA,CAACC,WAAW,EAAE;MAC9B;MACAC,KAAK,CAAC,eAAeD,WAAW,CAACV,IAAI,mBAAmB,CAAC;IAC3D,CAAC;IACDY,iBAAiBA,CAACF,WAAW,EAAE;MAC7B;MACAC,KAAK,CAAC,eAAeD,WAAW,CAACV,IAAI,iBAAiB,CAAC;IACzD;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}