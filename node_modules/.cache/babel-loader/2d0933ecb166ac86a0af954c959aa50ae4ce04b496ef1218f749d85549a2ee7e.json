{"ast":null,"code":"import FooterLandingPage from '@/components/FooterLandingPage.vue';\nimport NavbarLandingPage from '@/components/NavbarLandingPage.vue';\nexport default {\n  components: {\n    NavbarLandingPage,\n    FooterLandingPage\n  },\n  name: 'LandingPage',\n  data() {\n    return {\n      filters: [\"About\", \"Services\", \"Contact\"]\n    };\n  },\n  mounted() {\n    this.typeWriterEffect();\n    this.addFadeInAnimation();\n  },\n  methods: {\n    // Method for Typing Effect\n    typeWriterEffect() {\n      const text = \"Lembaga Perlindungan Konsumen Indonesia\";\n      let i = 0;\n      const element = document.getElementById(\"typing\");\n\n      // Function to simulate typing\n      function type() {\n        if (i < text.length) {\n          element.innerHTML += text.charAt(i);\n          i++;\n          setTimeout(type, 100);\n        } else {\n          setTimeout(() => {\n            element.innerHTML = \"\"; // Clear the text\n            i = 0; // Reset the index\n            type(); // Restart typing\n          }, 2000); // Wait for 2 seconds after typing is done before starting again\n        }\n      }\n      type(); // Start typing effect\n    },\n    // Method to add fade-in effect on scroll\n    addFadeInAnimation() {\n      window.addEventListener('scroll', function () {\n        const aboutText = document.getElementById('about-text');\n        const rect = aboutText.getBoundingClientRect();\n        const viewportHeight = window.innerHeight;\n        const trackRecord = document.getElementById('track-record');\n        if (rect.top < viewportHeight - 100) {\n          trackRecord.classList.add('animate__fadeIn');\n        }\n        // Check if the section is in the viewport\n        if (rect.top < viewportHeight - 100) {\n          aboutText.classList.add('animate-fadeIn');\n        }\n      });\n    },\n    setActiveFilter(filter) {\n      console.log(filter);\n    }\n  }\n};","map":{"version":3,"names":["FooterLandingPage","NavbarLandingPage","components","name","data","filters","mounted","typeWriterEffect","addFadeInAnimation","methods","text","i","element","document","getElementById","type","length","innerHTML","charAt","setTimeout","window","addEventListener","aboutText","rect","getBoundingClientRect","viewportHeight","innerHeight","trackRecord","top","classList","add","setActiveFilter","filter","console","log"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\lpkni\\Dashboard.vue"],"sourcesContent":["<template>\r\n  <div class=\"bg-red-100 min-h-screen relative\">\r\n    <NavbarLandingPage />\r\n\r\n    <section class=\"flex flex-col md:flex-row items-center justify-center py-20 bg-red-700 text-white\">\r\n      <!-- Left Section (Text Content) -->\r\n      <div class=\"md:w-1/2 text-center md:text-left\">\r\n        <!-- Typing Effect for Heading -->\r\n        <h1 id=\"typing\" class=\"text-4xl md:text-5xl font-bold mb-4\"></h1>\r\n        <p class=\"mb-6 text-lg\">\r\n          LPKNI merupakan salah satu lembaga atau institusi yang mempunyai tugas pokok dan fungsinya adalah melindungi hak\r\n          setiap warga negara baik perseorangan (konsumen) maupun badan yang harus mendapatkan perlakuan yang layak selaku\r\n          konsumen khususnya dan masyarakat sebagai warga negara Indonesia pada umumnya.\r\n        </p>\r\n        <a href=\"#contact\" class=\"bg-white text-red-600 px-6 py-2 rounded-lg font-semibold hover:bg-gray-100 transition duration-300\">Get Involved</a>\r\n      </div>\r\n\r\n      <!-- Right Section (Image) -->\r\n      <div class=\"md:w-[30%] mt-5 md:mt-0\">\r\n        <img src=\"@/assets/animasi1.png\" alt=\"LPKNI\" class=\"rounded-lg\" />\r\n      </div>\r\n    </section>\r\n\r\n    <!-- Chat Button -->\r\n    <button \r\n      @click=\"toggleChat\" \r\n      class=\"fixed bottom-6 right-6 bg-red-600 text-white p-4 rounded-full shadow-lg hover:bg-red-700 transition-transform transform hover:scale-110\"\r\n    >\r\n      ðŸ’¬\r\n    </button>\r\n\r\n    <!-- Chat Modal -->\r\n    <div v-if=\"isChatOpen\" class=\"fixed inset-0 flex items-center justify-center bg-black bg-opacity-50\">\r\n      <div class=\"bg-white w-full max-w-md rounded-lg shadow-lg p-4 relative\">\r\n        \r\n        <!-- Header -->\r\n        <div class=\"flex justify-between items-center border-b pb-2 mb-2\">\r\n          <h3 class=\"text-lg font-semibold text-red-700\">Live Chat</h3>\r\n          <button @click=\"toggleChat\" class=\"text-gray-600 hover:text-gray-800\">âœ–</button>\r\n        </div>\r\n\r\n        <!-- Chat Body -->\r\n        <div class=\"h-60 overflow-y-auto p-2 border rounded-md bg-gray-100\">\r\n          <div v-for=\"(msg, index) in messages\" :key=\"index\" class=\"mb-2\">\r\n            <div v-if=\"msg.sender === 'user'\" class=\"text-right\">\r\n              <p class=\"inline-block bg-red-600 text-white p-2 rounded-lg text-sm\">{{ msg.text }}</p>\r\n            </div>\r\n            <div v-else class=\"text-left\">\r\n              <p class=\"inline-block bg-gray-300 text-gray-700 p-2 rounded-lg text-sm\">{{ msg.text }}</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Chat Input -->\r\n        <div class=\"flex items-center mt-2\">\r\n          <input \r\n            v-model=\"newMessage\"\r\n            type=\"text\"\r\n            placeholder=\"Tulis pesan...\"\r\n            class=\"flex-grow border p-2 rounded-md\"\r\n            @keyup.enter=\"sendMessage\"\r\n          />\r\n          <button \r\n            @click=\"sendMessage\"\r\n            class=\"ml-2 bg-red-600 text-white px-4 py-2 rounded-md hover:bg-red-700\"\r\n          >\r\n            Kirim\r\n          </button>\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <FooterLandingPage />\r\n  </div>\r\n</template>\r\n  \r\n  <script>\r\nimport FooterLandingPage from '@/components/FooterLandingPage.vue';\r\nimport NavbarLandingPage from '@/components/NavbarLandingPage.vue';\r\n\r\n  export default {\r\n    components: {\r\n        NavbarLandingPage,\r\n        FooterLandingPage\r\n    },\r\n    name: 'LandingPage',\r\n    data() {\r\n      return {\r\n        filters: [\"About\", \"Services\", \"Contact\"]\r\n      };\r\n    },\r\n    mounted() {\r\n      this.typeWriterEffect(); \r\n      this.addFadeInAnimation(); \r\n    },\r\n    methods: {\r\n      // Method for Typing Effect\r\n      typeWriterEffect() {\r\n            const text = \"Lembaga Perlindungan Konsumen Indonesia\"; \r\n            let i = 0;\r\n            const element = document.getElementById(\"typing\"); \r\n            \r\n            // Function to simulate typing\r\n            function type() {\r\n            if (i < text.length) {\r\n                element.innerHTML += text.charAt(i); \r\n                i++;\r\n                setTimeout(type, 100); \r\n            } else {\r\n                setTimeout(() => {\r\n                element.innerHTML = \"\"; // Clear the text\r\n                i = 0; // Reset the index\r\n                type(); // Restart typing\r\n                }, 2000); // Wait for 2 seconds after typing is done before starting again\r\n            }\r\n            }\r\n\r\n            type(); // Start typing effect\r\n        },\r\n\r\n      // Method to add fade-in effect on scroll\r\n      addFadeInAnimation() {\r\n        window.addEventListener('scroll', function() {\r\n          const aboutText = document.getElementById('about-text');\r\n          const rect = aboutText.getBoundingClientRect();\r\n          const viewportHeight = window.innerHeight;\r\n          const trackRecord = document.getElementById('track-record');\r\n          \r\n          if (rect.top < viewportHeight - 100) {\r\n            trackRecord.classList.add('animate__fadeIn');\r\n            }\r\n          // Check if the section is in the viewport\r\n          if (rect.top < viewportHeight - 100) {\r\n            aboutText.classList.add('animate-fadeIn');\r\n          }\r\n        });\r\n      },\r\n      \r\n      setActiveFilter(filter) {\r\n        console.log(filter);\r\n      }\r\n    }\r\n  };\r\n</script>\r\n\r\n  \r\n  <style scoped>\r\n  /* Tailwind CSS animation for fade-in effect */\r\n  @keyframes fadeIn {\r\n    from {\r\n      opacity: 0;\r\n    }\r\n    to {\r\n      opacity: 1;\r\n    }\r\n  }\r\n\r\n  .animate-fadeIn {\r\n    animation: fadeIn 2s ease-out forwards;\r\n  }\r\n  .animate__fadeIn {\r\n    animation: fadeIn 1s ease-in-out;\r\n  }\r\n\r\n  /* For Hover Effect */\r\n  .hover\\:scale-105:hover {\r\n    transform: scale(1.05);\r\n  }\r\n\r\n  /* For the text hover effect */\r\n  .hover\\:text-red-600:hover {\r\n    color: #ef4444;\r\n  }\r\n\r\n  /* Optional: For Smooth Scroll */\r\n  html {\r\n    scroll-behavior: smooth;\r\n  }\r\n</style>"],"mappings":"AA6EA,OAAOA,iBAAgB,MAAO,oCAAoC;AAClE,OAAOC,iBAAgB,MAAO,oCAAoC;AAEhE,eAAe;EACbC,UAAU,EAAE;IACRD,iBAAiB;IACjBD;EACJ,CAAC;EACDG,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,CAAC,OAAO,EAAE,UAAU,EAAE,SAAS;IAC1C,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,gBAAgB,CAAC,CAAC;IACvB,IAAI,CAACC,kBAAkB,CAAC,CAAC;EAC3B,CAAC;EACDC,OAAO,EAAE;IACP;IACAF,gBAAgBA,CAAA,EAAG;MACb,MAAMG,IAAG,GAAI,yCAAyC;MACtD,IAAIC,CAAA,GAAI,CAAC;MACT,MAAMC,OAAM,GAAIC,QAAQ,CAACC,cAAc,CAAC,QAAQ,CAAC;;MAEjD;MACA,SAASC,IAAIA,CAAA,EAAG;QAChB,IAAIJ,CAAA,GAAID,IAAI,CAACM,MAAM,EAAE;UACjBJ,OAAO,CAACK,SAAQ,IAAKP,IAAI,CAACQ,MAAM,CAACP,CAAC,CAAC;UACnCA,CAAC,EAAE;UACHQ,UAAU,CAACJ,IAAI,EAAE,GAAG,CAAC;QACzB,OAAO;UACHI,UAAU,CAAC,MAAM;YACjBP,OAAO,CAACK,SAAQ,GAAI,EAAE,EAAE;YACxBN,CAAA,GAAI,CAAC,EAAE;YACPI,IAAI,CAAC,CAAC,EAAE;UACR,CAAC,EAAE,IAAI,CAAC,EAAE;QACd;MACA;MAEAA,IAAI,CAAC,CAAC,EAAE;IACZ,CAAC;IAEH;IACAP,kBAAkBA,CAAA,EAAG;MACnBY,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAE,YAAW;QAC3C,MAAMC,SAAQ,GAAIT,QAAQ,CAACC,cAAc,CAAC,YAAY,CAAC;QACvD,MAAMS,IAAG,GAAID,SAAS,CAACE,qBAAqB,CAAC,CAAC;QAC9C,MAAMC,cAAa,GAAIL,MAAM,CAACM,WAAW;QACzC,MAAMC,WAAU,GAAId,QAAQ,CAACC,cAAc,CAAC,cAAc,CAAC;QAE3D,IAAIS,IAAI,CAAC