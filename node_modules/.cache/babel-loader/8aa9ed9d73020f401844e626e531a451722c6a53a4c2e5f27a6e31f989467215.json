      this.pendaftaranData.foto3x4 = file;\n      }\n    },\n    handleKtpUpload(event) {\n      const file = event.target.files[0];\n      if (file) {\n        this.pendaftaranData.fotoKtp = file;\n      }\n    },\n    async submitPendaftaran() {\n      this.isLoading = true; // Set loading to true while submitting\n      const formData = new FormData();\n      formData.append(\"nama\", this.pendaftaranData.nama);\n      formData.append(\"nik\", this.pendaftaranData.nik);\n      formData.append(\"noTelepon\", this.pendaftaranData.noTelepon);\n      formData.append(\"email\", this.pendaftaranData.email);\n      formData.append(\"tipePendaftaran\", this.pendaftaranData.tipePendaftaran);\n      formData.append(\"provinsi\", this.pendaftaranData.provinsi);\n      formData.append(\"kotaKabupaten\", this.pendaftaranData.kotaKabupaten);\n      formData.append(\"jabatan\", this.pendaftaranData.jabatan);\n      formData.append(\"alamat\", this.pendaftaranData.alamat);\n      formData.append(\"foto3x4\", this.pendaftaranData.foto3x4);\n      formData.append(\"fotoKtp\", this.pendaftaranData.fotoKtp);\n      try {\n        // Send the form data to the server\n        await ApiService.submitPendaftaran(formData);\n        this.$router.push({\n          name: 'success'\n        }); // Redirect to success page\n      } catch (error) {\n        console.error(\"Pendaftaran failed:\", error);\n        alert(\"Pendaftaran gagal. Silakan coba lagi.\");\n      } finally {\n        this.isLoading = false; // Set loading to false after submission\n      }\n    }\n  }\n};","map":{"version":3,"names":["NavbarUser","ApiService","components","data","pendaftaranData","nama","nik","noTelepon","email","tipePendaftaran","provinsi","kotaKabupaten","jabatan","alamat","foto3x4","fotoKtp","jabatanProvinsi","jabatanKotaKabupaten","isLoading","computed","jabatanOptions","methods","handleFoto3x4Upload","event","file","target","files","handleKtpUpload","submitPendaftaran","formData","FormData","append","$router","push","name","error","console","alert"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\user\\PendaftaranAnggotaView.vue"],"sourcesContent":["<template>\r\n  <div class=\"w-screen h-screen flex flex-col mt-10\">\r\n    <!-- Navbar -->\r\n    <NavbarUser />\r\n\r\n    <div class=\"flex flex-col items-center p-10 bg-gray-100 flex-grow\">\r\n      <div class=\"bg-white w-full max-w-2xl p-6 rounded-lg shadow-md\">\r\n        <h2 class=\"text-2xl font-semibold text-[#03a980] text-center mb-6\">Pendaftaran Anggota</h2>\r\n\r\n        <!-- Form Pendaftaran -->\r\n        <form @submit.prevent=\"submitPendaftaran\">\r\n          <div class=\"grid grid-cols-1 gap-4\">\r\n            <input v-model=\"pendaftaranData.nama\" type=\"text\" placeholder=\"Nama Lengkap\" class=\"input-field\" required />\r\n            <input v-model=\"pendaftaranData.nik\" type=\"text\" placeholder=\"NIK\" class=\"input-field\" required />\r\n            <input v-model=\"pendaftaranData.noTelepon\" type=\"text\" placeholder=\"Nomor Telepon\" class=\"input-field\" required />\r\n            <input v-model=\"pendaftaranData.email\" type=\"email\" placeholder=\"Email\" class=\"input-field\" required />\r\n            \r\n            <!-- Pilih untuk Daftar Provinsi atau Kota/Kabupaten -->\r\n            <select v-model=\"pendaftaranData.tipePendaftaran\" class=\"input-field\" required>\r\n              <option disabled value=\"\">Pilih Tipe Pendaftaran</option>\r\n              <option value=\"Provinsi\">Provinsi</option>\r\n              <option value=\"Kota/Kabupaten\">Kota/Kabupaten</option>\r\n            </select>\r\n\r\n            <!-- Provinsi -->\r\n            <select v-if=\"pendaftaranData.tipePendaftaran === 'Provinsi'\" v-model=\"pendaftaranData.provinsi\" class=\"input-field\" required>\r\n              <option disabled value=\"\">Pilih Provinsi</option>\r\n              <option value=\"NTB\">NTB</option>\r\n              <option value=\"Jawa Timur\">Jawa Timur</option>\r\n              <option value=\"Sumatera Barat\">Sumatera Barat</option>\r\n            </select>\r\n\r\n            <!-- Kota/Kabupaten hanya muncul jika tipe pendaftaran adalah Kota/Kabupaten -->\r\n            <select v-if=\"pendaftaranData.tipePendaftaran === 'Kota/Kabupaten'\" v-model=\"pendaftaranData.kotaKabupaten\" class=\"input-field\" required>\r\n              <option disabled value=\"\">Pilih Kota/Kabupaten</option>\r\n              <option value=\"Kota Malang\">Kota Malang</option>\r\n              <option value=\"Surabaya\">Surabaya</option>\r\n              <option value=\"Gresik\">Gresik</option>\r\n            </select>\r\n\r\n            <!-- Jabatan LPKNI -->\r\n            <select v-model=\"pendaftaranData.jabatan\" class=\"input-field\" required>\r\n              <option disabled value=\"\">Pilih Jabatan LPKNI</option>\r\n              <option v-for=\"jabatan in jabatanOptions\" :key=\"jabatan\" :value=\"jabatan\">{{ jabatan }}</option>\r\n            </select>\r\n\r\n            <input v-model=\"pendaftaranData.alamat\" type=\"text\" placeholder=\"Alamat Lengkap\" class=\"input-field\" required />\r\n            \r\n            <!-- Upload Foto 3x4 -->\r\n            <label class=\"text-sm text-gray-600\">Upload Foto 3x4:</label>\r\n            <input type=\"file\" @change=\"handleFoto3x4Upload\" class=\"input-field\" required />\r\n\r\n            <!-- Upload Foto KTP -->\r\n            <label class=\"text-sm text-gray-600\">Upload Foto KTP:</label>\r\n            <input type=\"file\" @change=\"handleKtpUpload\" class=\"input-field\" required />\r\n\r\n            <button type=\"submit\" class=\"mt-6 w-full bg-[#03a980] text-white px-6 py-2 rounded-lg font-bold hover:bg-[#028a66] transition\">\r\n              Ajukan Pendaftaran\r\n            </button>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavbarUser from '@/components/NavbarUser.vue'