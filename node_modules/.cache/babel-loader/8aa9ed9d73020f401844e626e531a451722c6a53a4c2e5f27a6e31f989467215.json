urn this.jabatanKotaKabupaten;\r\n      } else {\r\n        return [];\r\n      }\r\n    }\r\n  },\r\n  methods: {\r\n    handleFoto3x4Upload(event) {\r\n      const file = event.target.files[0];\r\n      if (file) {\r\n        this.pendaftaranData.foto3x4 = file;\r\n      }\r\n    },\r\n    handleKtpUpload(event) {\r\n      const file = event.target.files[0];\r\n      if (file) {\r\n        this.pendaftaranData.fotoKtp = file;\r\n      }\r\n    },\r\n\r\n    async submitPendaftaran() {\r\n      this.isLoading = true; // Set loading to true while submitting\r\n      const formData = new FormData();\r\n      formData.append(\"nama\", this.pendaftaranData.nama);\r\n      formData.append(\"nik\", this.pendaftaranData.nik);\r\n      formData.append(\"noTelepon\", this.pendaftaranData.noTelepon);\r\n      formData.append(\"email\", this.pendaftaranData.email);\r\n      formData.append(\"tipePendaftaran\", this.pendaftaranData.tipePendaftaran);\r\n      formData.append(\"provinsi\", this.pendaftaranData.provinsi);\r\n      formData.append(\"kotaKabupaten\", this.pendaftaranData.kotaKabupaten);\r\n      formData.append(\"jabatan\", this.pendaftaranData.jabatan);\r\n      formData.append(\"alamat\", this.pendaftaranData.alamat);\r\n      formData.append(\"foto3x4\", this.pendaftaranData.foto3x4);\r\n      formData.append(\"fotoKtp\", this.pendaftaranData.fotoKtp);\r\n\r\n      try {\r\n        // Send the form data to the server\r\n        await ApiService.submitPendaftaran(formData);\r\n        this.$router.push({ name: 'success' }); // Redirect to success page\r\n      } catch (error) {\r\n        console.error(\"Pendaftaran failed:\", error);\r\n        alert(\"Pendaftaran gagal. Silakan coba lagi.\");\r\n      } finally {\r\n        this.isLoading = false; // Set loading to false after submission\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.input-field {\r\n  width: 100%;\r\n  padding: 12px;\r\n  margin-bottom: 12px;\r\n  border: 1px solid #ddd;\r\n  border-radius: 8px;\r\n  font-size: 16px;\r\n}\r\n\r\ninput[type=\"file\"] {\r\n  padding: 6px;\r\n}\r\n\r\nbutton {\r\n  font-size: 16px;\r\n  transition: background-color 0.3s;\r\n}\r\n</style>\r\n\r\n"],"mappings":";AAmEA,OAAOA,UAAS,MAAO,6BAA6B;AACpD,OAAOC,UAAS,MAAO,kBAAkB;AAEzC,eAAe;EACbC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,eAAe,EAAE;QACfC,IAAI,EAAE,EAAE;QACRC,GAAG,EAAE,EAAE;QACPC,SAAS,EAAE,EAAE;QACbC,KAAK,EAAE,EAAE;QACTC,eAAe,EAAE,EAAE;QACnBC,QAAQ,EAAE,EAAE;QACZC,aAAa,EAAE,EAAE;QACjBC,OAAO,EAAE,EAAE;QACXC,MAAM,EAAE,EAAE;QACVC,OAAO,EAAE,IAAI;QACbC,OAAO,EAAE;MACX,CAAC;MACDC,eAAe,EAAE,CACf,wBAAwB,EACxB,aAAa,EACb,8CAA8C,EAC9C,iBAAiB,EACjB,+FAA+F,EAC/F,wCAAwC,EACxC,kDAAkD,EAClD,gDAAgD,EAChD,8CAA8C,EAC9C,6CAA4C,CAC7C;MACDC,oBAAoB,EAAE,CACpB,wBAAwB,EACxB,aAAa,EACb,uCAAuC,EACvC,WAAW,EACX,4CAA4C,EAC5C,mDAAmD,EACnD,kCAAkC,EAClC,yCAAyC,EACzC,0CAA0C,EAC1C,wCAAwC,EACxC,6CAA4C,CAC7C;MACDC,SAAS,EAAE,KAAI,CAAE;IACnB,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,cAAcA,CAAA,EAAG;MACf,IAAI,IAAI,CAAChB,eAAe,CAACK,eAAc,KAAM,UAAU,EAAE;QACvD,OAAO,IAAI,CAACO,eAAe;MAC7B,OAAO,IAAI,IAAI,CAACZ,eAAe,CAACK,eAAc,KAAM,gBAAgB,EAAE;QACpE,OAAO,IAAI,CAACQ,oBAAoB;MAClC,OAAO;QACL,OAAO,EAAE;MACX;IACF;EACF,CAAC;EACDI,OAAO,EAAE;IACPC,mBAAmBA,CAACC,KAAK,EAAE;MACzB,MAAMC,IAAG,GAAID,KAAK,CAACE,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;MAClC,IAAIF,IAAI,EAAE;QACR,IAAI,CAACpB,eAAe,CAACU,OAAM,GAAIU,IAAI;MACrC;IACF,CAAC;IACDG,eAAeA,CAACJ,KAAK,EAAE;MACrB,MAAMC,IAAG,GAAID,KAAK,CAACE,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;MAClC,IAAIF,IAAI,EAAE;QACR,IAAI,CAACpB,eAAe,CAACW,OAAM,GAAIS,IAAI;MACrC;IACF,CAAC;IAED,MAAMI,iBAAiBA,CAAA,EAAG;MACxB,IAAI,CAACV,SAAQ,GAAI,IAAI,EAAE;MACvB,MAAMW,QAAO,GAAI,IAAIC,QAAQ,CAAC,CAAC;MAC/BD,QAAQ,CAACE,MAAM,CAAC,MAAM,EAAE,IAAI,CAAC3B,eAAe,CAACC,IAAI,CAAC;MAClDwB,QAAQ,CAACE,MAAM,CAAC,KAAK,EAAE,IAAI,CAAC3B,eAAe,CAACE,GAAG,CAAC;MAChDuB,QAAQ,CAACE,MAAM,CAAC,WAAW,EAAE,IAAI,CAAC3B,eAAe,CAACG,SAAS,CAAC;MAC5DsB,QAAQ,CAACE,MAAM,CAAC,OAAO,EAAE,IAAI,CAAC3B,eAAe,CAACI,KAAK,CAAC;MACpDqB,QAAQ,