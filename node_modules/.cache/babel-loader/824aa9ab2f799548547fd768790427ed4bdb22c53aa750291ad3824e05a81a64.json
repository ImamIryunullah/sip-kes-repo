{"ast":null,"code":"import FooterLandingPage from '@/components/FooterLandingPage.vue';\nimport NavbarLandingPage from '@/components/NavbarLandingPage.vue';\nexport default {\n  components: {\n    NavbarLandingPage,\n    FooterLandingPage\n  },\n  data() {\n    return {\n      isMounted: false,\n      form: {\n        nama: '',\n        email: '',\n        wilayah: '',\n        daerah: '',\n        kategori: '',\n        judul: '',\n        deskripsi: '',\n        harapan: '',\n        tindakLanjut: false\n      },\n      fileName: '',\n      file: null\n    };\n  },\n  mounted() {\n    this.$nextTick;\n  },\n  methods: {\n    handleFileUpload(event) {\n      this.$nextTick(() => {\n        const file = event.target.files[0];\n        if (file) {\n          this.fileName = file.name;\n          this.file = file;\n        }\n      });\n    },\n    submitForm() {\n      if (!this.form.nama || !this.form.email || !this.form.kategori) {\n        alert(\"Harap isi semua bidang yang diperlukan!\");\n        return;\n      }\n      try {\n        const formData = new FormData();\n        for (const key in this.form) {\n          formData.append(key, this.form[key]);\n        }\n        if (this.file) {\n          formData.append('lampiran', this.file);\n        }\n        console.log(\"Data berhasil dikirim\", formData);\n        alert(\"Pengaduan berhasil dikirim!\");\n      } catch (error) {\n        console.error(\"Terjadi kesalahan:\", error);\n        alert(\"Terjadi kesalahan saat mengirim pengaduan.\");\n      }\n    }\n  }\n};","map":{"version":3,"names":["FooterLandingPage","NavbarLandingPage","components","data","isMounted","form","nama","email","wilayah","daerah","kategori","judul","deskripsi","harapan","tindakLanjut","fileName","file","mounted","$nextTick","methods","handleFileUpload","event","target","files","name","submitForm","alert","formData","FormData","key","append","console","log","error"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\lpkni\\FormPengaduanLpkni.vue"],"sourcesContent":["<template>\r\n  <div v-if=\"isMounted\" class=\"w-screen min-h-screen flex flex-col bg-red-100\">\r\n    <NavbarLandingPage />\r\n\r\n    <!-- Form Container -->\r\n    <div class=\"flex flex-col items-center p-6 flex-grow mt-10\">\r\n      <div class=\"bg-white w-full max-w-screen-md p-6 rounded-lg shadow-lg animate-fadeInUp\">\r\n        <h2 class=\"text-3xl font-semibold text-red-600 text-center mb-6\">Form Pengaduan</h2>\r\n        <form @submit.prevent=\"submitForm\" class=\"space-y-6\">\r\n          <!-- Nama Pengadu -->\r\n          <div>\r\n            <label for=\"nama\" class=\"block text-sm font-semibold text-red-700\">Nama Pengadu</label>\r\n            <input id=\"nama\" v-model=\"form.nama\" type=\"text\" required class=\"input-field\" placeholder=\"Nama Pengadu\" />\r\n          </div>\r\n\r\n          <!-- Email -->\r\n          <div>\r\n            <label for=\"email\" class=\"block text-sm font-semibold text-red-700\">Email</label>\r\n            <input id=\"email\" v-model=\"form.email\" type=\"email\" required class=\"input-field\" placeholder=\"Email Anda\" />\r\n          </div>\r\n\r\n          <!-- Wilayah -->\r\n          <div>\r\n            <label for=\"wilayah\" class=\"block text-sm font-semibold text-red-700\">Wilayah/Provinsi</label>\r\n            <input id=\"wilayah\" v-model=\"form.wilayah\" type=\"text\" required class=\"input-field\" placeholder=\"Provinsi Anda\" />\r\n          </div>\r\n\r\n          <!-- Daerah -->\r\n          <div>\r\n            <label for=\"daerah\" class=\"block text-sm font-semibold text-red-700\">Daerah</label>\r\n            <input id=\"daerah\" v-model=\"form.daerah\" type=\"text\" required class=\"input-field\" placeholder=\"Kota atau Kabupaten\" />\r\n          </div>\r\n\r\n          <!-- Kategori Pengaduan -->\r\n          <div>\r\n            <label for=\"kategori\" class=\"block text-sm font-semibold text-red-700\">Kategori Pengaduan</label>\r\n            <select id=\"kategori\" v-model=\"form.kategori\" required class=\"input-field\">\r\n              <option disabled value=\"\">Pilih Kategori</option>\r\n              <option value=\"Layanan Pendidikan\">Layanan Pendidikan</option>\r\n              <option value=\"Sertifikasi\">Sertifikasi</option>\r\n              <option value=\"Fasilitas\">Fasilitas</option>\r\n              <option value=\"Masalah Administrasi\">Masalah Administrasi</option>\r\n              <option value=\"Lainnya\">Lainnya</option>\r\n            </select>\r\n          </div>\r\n\r\n          <!-- Judul Pengaduan -->\r\n          <div>\r\n            <label for=\"judul\" class=\"block text-sm font-semibold text-red-700\">Judul Pengaduan</label>\r\n            <input id=\"judul\" v-model=\"form.judul\" type=\"text\" required class=\"input-field\" placeholder=\"Judul Pengaduan\" />\r\n          </div>\r\n\r\n          <!-- Deskripsi Pengaduan -->\r\n          <div>\r\n            <label for=\"deskripsi\" class=\"block text-sm font-semibold text-red-700\">Deskripsi Pengaduan</label>\r\n            <textarea id=\"deskripsi\" v-model=\"form.deskripsi\" required class=\"input-field\" placeholder=\"Jelaskan pengaduan Anda\" rows=\"4\"></textarea>\r\n          </div>\r\n\r\n          <!-- Lampiran -->\r\n          <div>\r\n            <label for=\"lampiran\" class=\"block text-sm font-semibold text-red-700\">Lampiran / Bukti</label>\r\n            <input id=\"lampiran\" type=\"file\" @change=\"handleFileUpload\" class=\"input-field\" />\r\n            <p v-if=\"fileName\" class=\"text-sm text-red-700 mt-2\">File yang dipilih: {{ fileName }}</p>\r\n          </div>\r\n\r\n          <!-- Harapan Penyelesaian -->\r\n          <div>\r\n            <label for=\"harapan\" class=\"block text-sm font-semibold text-red-700\">Harapan Penyelesaian</label>\r\n            <input id=\"harapan\" v-model=\"form.harapan\" type=\"text\" required class=\"input-field\" placeholder=\"Harapan Penyelesaian\" />\r\n          </div>\r\n\r\n          <!-- Tindak Lanjut -->\r\n          <div class=\"flex items-center\">\r\n            <input type=\"checkbox\" id=\"tindakLanjut\" v-model=\"form.tindakLanjut\" class=\"mr-2\" />\r\n            <label for=\"tindakLanjut\" class=\"text-sm font-semibold text-red-700\">\r\n              Apakah Anda ingin dihubungi untuk tindak lanjut?\r\n            </label>\r\n          </div>\r\n\r\n          <!-- Submit Button -->\r\n          <button type=\"submit\" class=\"w-full bg-red-600 text-white p-3 rounded-lg font-semibold hover:bg-red-700 transition-transform transform hover:scale-105\">\r\n            Kirim Pengaduan\r\n          </button>\r\n        </form>\r\n      </div>\r\n    </div>\r\n\r\n    <FooterLandingPage />\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport FooterLandingPage from '@/components/FooterLandingPage.vue';\r\nimport NavbarLandingPage from '@/components/NavbarLandingPage.vue';\r\n\r\nexport default {\r\n  components: {\r\n    NavbarLandingPage,\r\n    FooterLandingPage\r\n  },\r\n  data() {\r\n    return {\r\n      isMounted: false,\r\n      form: {\r\n        nama: '',\r\n        email: '',\r\n        wilayah: '',\r\n        daerah: '',\r\n        kategori: '',\r\n        judul: '',\r\n        deskripsi: '',\r\n        harapan: '',\r\n        tindakLanjut: false\r\n      },\r\n      fileName: '',\r\n      file: null\r\n    };\r\n  },\r\n  mounted() {\r\n    this.$nextTick \r\n  },\r\n  methods: {\r\n    handleFileUpload(event) {\r\n      this.$nextTick(() => {\r\n        const file = event.target.files[0];\r\n        if (file) {\r\n          this.fileName = file.name;\r\n          this.file = file;\r\n        }\r\n      });\r\n    },\r\n    submitForm() {\r\n      if (!this.form.nama || !this.form.email || !this.form.kategori) {\r\n        alert(\"Harap isi semua bidang yang diperlukan!\");\r\n        return;\r\n      }\r\n\r\n      try {\r\n        const formData = new FormData();\r\n        for (const key in this.form) {\r\n          formData.append(key, this.form[key]);\r\n        }\r\n        if (this.file) {\r\n          formData.append('lampiran', this.file);\r\n        }\r\n\r\n        console.log(\"Data berhasil dikirim\", formData);\r\n        alert(\"Pengaduan berhasil dikirim!\");\r\n      } catch (error) {\r\n        console.error(\"Terjadi kesalahan:\", error);\r\n        alert(\"Terjadi kesalahan saat mengirim pengaduan.\");\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n@keyframes fadeInUp {\r\n  0% {\r\n    opacity: 0;\r\n    transform: translateY(20px);\r\n  }\r\n  100% {\r\n    opacity: 1;\r\n    transform: translateY(0);\r\n  }\r\n}\r\n\r\n.animate-fadeInUp {\r\n  animation: fadeInUp 0.6s ease-out;\r\n}\r\n\r\n.input-field {\r\n  @apply p-3 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-red-600;\r\n  transition: all 0.3s ease-in-out;\r\n}\r\n\r\nbutton:hover {\r\n  transition: all 0.3s ease-in-out;\r\n}\r\n\r\nbutton:active {\r\n  transform: scale(0.98);\r\n}\r\n</style>\r\n"],"mappings":"AA4FA,OAAOA,iBAAgB,MAAO,oCAAoC;AAClE,OAAOC,iBAAgB,MAAO,oCAAoC;AAElE,eAAe;EACbC,UAAU,EAAE;IACVD,iBAAiB;IACjBD;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,KAAK;MAChBC,IAAI,EAAE;QACJC,IAAI,EAAE,EAAE;QACRC,KAAK,EAAE,EAAE;QACTC,OAAO,EAAE,EAAE;QACXC,MAAM,EAAE,EAAE;QACVC,QAAQ,EAAE,EAAE;QACZC,KAAK,EAAE,EAAE;QACTC,SAAS,EAAE,EAAE;QACbC,OAAO,EAAE,EAAE;QACXC,YAAY,EAAE;MAChB,CAAC;MACDC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,SAAQ;EACf,CAAC;EACDC,OAAO,EAAE;IACPC,gBAAgBA,CAACC,KAAK,EAAE;MACtB,IAAI,CAACH,SAAS,CAAC,MAAM;QACnB,MAAMF,IAAG,GAAIK,KAAK,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;QAClC,IAAIP,IAAI,EAAE;UACR,IAAI,CAACD,QAAO,GAAIC,IAAI,CAACQ,IAAI;UACzB,IAAI,CAACR,IAAG,GAAIA,IAAI;QAClB;MACF,CAAC,CAAC;IACJ,CAAC;IACDS,UAAUA,CAAA,EAAG;MACX,IAAI,CAAC,IAAI,CAACpB,IAAI,CAACC,IAAG,IAAK,CAAC,IAAI,CAACD,IAAI,CAACE,KAAI,IAAK,CAAC,IAAI,CAACF,IAAI,CAACK,QAAQ,EAAE;QAC9Dg