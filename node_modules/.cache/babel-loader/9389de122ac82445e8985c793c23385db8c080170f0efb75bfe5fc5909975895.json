{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport NavbarPusat from '@/components/NavbarPusat.vue';\n// import ApiService from '@/service/api.js';\n\nexport default {\n  components: {\n    NavbarPusat\n  },\n  data() {\n    return {\n      users: [{\n        id: 1,\n        firstName: \"Ahmad\",\n        lastName: \"Hidayat\",\n        jenisPendaftaran: \"Wilayah\",\n        provinsi: \"Jawa Barat\",\n        kotaKabupaten: \"\"\n      }, {\n        id: 2,\n        firstName: \"Rina\",\n        lastName: \"Sutomo\",\n        jenisPendaftaran: \"Daerah\",\n        provinsi: \"Jawa Tengah\",\n        kotaKabupaten: \"Semarang\"\n      }, {\n        id: 3,\n        firstName: \"Bagas\",\n        lastName: \"Santoso\",\n        jenisPendaftaran: \"Wilayah\",\n        provinsi: \"Sumatera Barat\",\n        kotaKabupaten: \"\"\n      }, {\n        id: 4,\n        firstName: \"Dewi\",\n        lastName: \"Lestari\",\n        jenisPendaftaran: \"Daerah\",\n        provinsi: \"Jawa Timur\",\n        kotaKabupaten: \"Surabaya\"\n      }, {\n        id: 5,\n        firstName: \"Fajar\",\n        lastName: \"Maulana\",\n        jenisPendaftaran: \"Wilayah\",\n        provinsi: \"Sulawesi Selatan\",\n        kotaKabupaten: \"\"\n      }, {\n        id: 6,\n        firstName: \"Siti\",\n        lastName: \"Aisyah\",\n        jenisPendaftaran: \"Daerah\",\n        provinsi: \"Kalimantan Timur\",\n        kotaKabupaten: \"Samarinda\"\n      }, {\n        id: 7,\n        firstName: \"Budi\",\n        lastName: \"Hartono\",\n        jenisPendaftaran: \"Wilayah\",\n        provinsi: \"Banten\",\n        kotaKabupaten: \"\"\n      }, {\n        id: 8,\n        firstName: \"Citra\",\n        lastName: \"Putri\",\n        jenisPendaftaran: \"Daerah\",\n        provinsi: \"Bali\",\n        kotaKabupaten: \"Denpasar\"\n      }],\n      selectedFilter: \"\",\n      // Untuk filter daftar pendaftaran\n      loading: false // Untuk menampilkan loading state\n    };\n  },\n  computed: {\n    filteredUsers() {\n      if (this.selectedFilter) {\n        return this.users.filter(user => user.jenisPendaftaran === this.selectedFilter);\n      }\n      return this.users;\n    }\n  },\n  methods: {\n    async fetchUsers() {\n      this.loading = true;\n      try {\n        // Jika ingin menggunakan API, bisa aktifkan baris berikut:\n        // const response = await ApiService.getPendingRegistrations();\n        // this.users = response.data;\n      } catch (error) {\n        console.error(\"Gagal mengambil data pendaftaran:\", error);\n      } finally {\n        this.loading = false;\n      }\n    },\n    async approveUser(userId) {\n      if (confirm(\"Apakah Anda yakin ingin menyetujui user ini?\")) {\n        try {\n          // Jika ada API, ini bisa dipakai\n          // await ApiService.approveUser(userId);\n          this.users = this.users.filter(user => user.id !== userId);\n          alert(\"Pendaftaran berhasil disetujui.\");\n        } catch (error) {\n          console.error(\"Gagal menyetujui user:\", error);\n        }\n      }\n    },\n    async rejectUser(userId) {\n      if (confirm(\"Apakah Anda yakin ingin menolak user ini?\")) {\n        try {\n          // Jika ada API, ini bisa dipakai\n          // await ApiService.rejectUser(userId);\n          this.users = this.users.filter(user => user.id !== userId);\n          alert(\"Pendaftaran telah ditolak.\");\n        } catch (error) {\n          console.error(\"Gagal menolak user:\", error);\n        }\n      }\n    }\n  },\n  mounted() {\n    this.fetchUsers(); // Data dummy tetap di-load\n  }\n};","map":{"version":3,"names":["NavbarPusat","components","data","users","id","firstName","lastName","jenisPendaftaran","provinsi","kotaKabupaten","selectedFilter","loading","computed","filteredUsers","filter","user","methods","fetchUsers","error","console","approveUser","userId","confirm","alert","rejectUser","mounted"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\pusat\\RegisterPusatView.vue"],"sourcesContent":["<template>\r\n  <div class=\"w-screen min-h-screen flex flex-col bg-gray-100\">\r\n    <!-- Navbar -->\r\n    <NavbarPusat />\r\n\r\n    <!-- Content Wrapper -->\r\n    <div class=\"flex flex-col items-center px-10 py-6 mt-[90px] flex-grow\">\r\n      <div class=\"bg-white w-full max-w-4xl p-6 rounded-lg shadow-md\">\r\n        <h2 class=\"text-2xl font-semibold text-[#03a980] text-center mb-6\">Verifikasi Pendaftaran User</h2>\r\n        \r\n        <!-- Filter Jenis Pendaftaran -->\r\n        <div class=\"flex justify-between mb-4\">\r\n          <label class=\"text-gray-700\">Filter Jenis Pendaftaran:</label>\r\n          <select v-model=\"selectedFilter\" class=\"input-field w-1/3\">\r\n            <option value=\"\">Semua ({{ users.length }})</option>\r\n            <option value=\"Wilayah\">Wilayah ({{ users.filter(user => user.jenisPendaftaran === 'Wilayah').length }})</option>\r\n            <option value=\"Daerah\">Daerah ({{ users.filter(user => user.jenisPendaftaran === 'Daerah').length }})</option>\r\n          </select>\r\n        </div>\r\n\r\n        <!-- Loading Spinner -->\r\n        <div v-if=\"loading\" class=\"flex justify-center my-4\">\r\n          <span class=\"text-[#03a980] font-semibold\">Loading data...</span>\r\n        </div>\r\n\r\n        <!-- Tabel Pendaftaran -->\r\n        <table v-if=\"!loading && filteredUsers.length > 0\" class=\"w-full border-collapse border border-gray-300\">\r\n          <thead class=\"bg-[#03a980] text-white\">\r\n            <tr>\r\n              <th class=\"py-2 px-4 border\">Nama</th>\r\n              <th class=\"py-2 px-4 border\">Jenis</th>\r\n              <th class=\"py-2 px-4 border\">Provinsi</th>\r\n              <th class=\"py-2 px-4 border\">Kota/Kab</th>\r\n              <th class=\"py-2 px-4 border\">Aksi</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody>\r\n            <tr v-for=\"user in filteredUsers\" :key=\"user.id\" class=\"border\">\r\n              <td class=\"py-2 px-4\">{{ user.firstName }} {{ user.lastName }}</td>\r\n              <td class=\"py-2 px-4\">{{ user.jenisPendaftaran }}</td>\r\n              <td class=\"py-2 px-4\">{{ user.provinsi }}</td>\r\n              <td class=\"py-2 px-4\">{{ user.kotaKabupaten || '-' }}</td>\r\n              <td class=\"py-2 px-4 flex gap-2\">\r\n                <button @click=\"approveUser(user.id)\" class=\"bg-green-500 text-white px-4 py-1 rounded hover:bg-green-700 transition\">✔ Terima</button>\r\n                <button @click=\"rejectUser(user.id)\" class=\"bg-red-500 text-white px-4 py-1 rounded hover:bg-red-700 transition\">✖ Tolak</button>\r\n              </td>\r\n            </tr>\r\n          </tbody>\r\n        </table>\r\n\r\n        <!-- Jika Tidak Ada Pendaftaran -->\r\n        <p v-if=\"!loading && filteredUsers.length === 0\" class=\"text-center text-gray-500 mt-4\">\r\n          Tidak ada pendaftaran yang perlu diverifikasi.\r\n        </p>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavbarPusat from '@/components/NavbarPusat.vue';\r\n// import ApiService from '@/service/api.js';\r\n\r\nexport default {\r\n  components: {\r\n    NavbarPusat\r\n  },\r\n  data() {\r\n    return {\r\n      users: [\r\n        { id: 1, firstName: \"Ahmad\", lastName: \"Hidayat\", jenisPendaftaran: \"Wilayah\", provinsi: \"Jawa Barat\", kotaKabupaten: \"\" },\r\n        { id: 2, firstName: \"Rina\", lastName: \"Sutomo\", jenisPendaftaran: \"Daerah\", provinsi: \"Jawa Tengah\", kotaKabupaten: \"Semarang\" },\r\n        { id: 3, firstName: \"Bagas\", lastName: \"Santoso\", jenisPendaftaran: \"Wilayah\", provinsi: \"Sumatera Barat\", kotaKabupaten: \"\" },\r\n        { id: 4, firstName: \"Dewi\", lastName: \"Lestari\", jenisPendaftaran: \"Daerah\", provinsi: \"Jawa Timur\", kotaKabupaten: \"Surabaya\" },\r\n        { id: 5, firstName: \"Fajar\", lastName: \"Maulana\", jenisPendaftaran: \"Wilayah\", provinsi: \"Sulawesi Selatan\", kotaKabupaten: \"\" },\r\n        { id: 6, firstName: \"Siti\", lastName: \"Aisyah\", jenisPendaftaran: \"Daerah\", provinsi: \"Kalimantan Timur\", kotaKabupaten: \"Samarinda\" },\r\n        { id: 7, firstName: \"Budi\", lastName: \"Hartono\", jenisPendaftaran: \"Wilayah\", provinsi: \"Banten\", kotaKabupaten: \"\" },\r\n        { id: 8, firstName: \"Citra\", lastName: \"Putri\", jenisPendaftaran: \"Daerah\", provinsi: \"Bali\", kotaKabupaten: \"Denpasar\" },\r\n      ],\r\n      selectedFilter: \"\", // Untuk filter daftar pendaftaran\r\n      loading: false, // Untuk menampilkan loading state\r\n    };\r\n  },\r\n  computed: {\r\n    filteredUsers() {\r\n      if (this.selectedFilter) {\r\n        return this.users.filter(user => user.jenisPendaftaran === this.selectedFilter);\r\n      }\r\n      return this.users;\r\n    }\r\n  },\r\n  methods: {\r\n    async fetchUsers() {\r\n      this.loading = true;\r\n      try {\r\n        // Jika ingin menggunakan API, bisa aktifkan baris berikut:\r\n        // const response = await ApiService.getPendingRegistrations();\r\n        // this.users = response.data;\r\n      } catch (error) {\r\n        console.error(\"Gagal mengambil data pendaftaran:\", error);\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n    async approveUser(userId) {\r\n      if (confirm(\"Apakah Anda yakin ingin menyetujui user ini?\")) {\r\n        try {\r\n          // Jika ada API, ini bisa dipakai\r\n          // await ApiService.approveUser(userId);\r\n          this.users = this.users.filter(user => user.id !== userId);\r\n          alert(\"Pendaftaran berhasil disetujui.\");\r\n        } catch (error) {\r\n          console.error(\"Gagal menyetujui user:\", error);\r\n        }\r\n      }\r\n    },\r\n    async rejectUser(userId) {\r\n      if (confirm(\"Apakah Anda yakin ingin menolak user ini?\")) {\r\n        t