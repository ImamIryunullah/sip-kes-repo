{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nexport default {\n  name: \"SearchAndFilter\",\n  data() {\n    return {\n      // Initial data: A list of sample items\n      items: [{\n        name: \"Aceh News\",\n        region: \"Aceh\"\n      }, {\n        name: \"Jawa Timur Weather\",\n        region: \"Jawa Timur\"\n      }, {\n        name: \"Bali Beach Updates\",\n        region: \"Bali\"\n      }, {\n        name: \"Sumatra Utara News\",\n        region: \"Sumatra Utara\"\n      }, {\n        name: \"Kalimantan Selatan Sports\",\n        region: \"Kalimantan Selatan\"\n      }, {\n        name: \"Sulawesi Selatan Politics\",\n        region: \"Sulawesi Selatan\"\n      }, {\n        name: \"Jawa Barat Economy\",\n        region: \"Jawa Barat\"\n      }],\n      searchQuery: \"\",\n      // User's input in search bar\n      selectedRegion: \"\",\n      // Selected region from dropdown\n      filteredResults: [] // Results after filtering\n    };\n  },\n  methods: {\n    // Method to filter data based on search query and selected region\n    filterData() {\n      const query = this.searchQuery.toLowerCase();\n      const region = this.selectedRegion.toLowerCase();\n      this.filteredResults = this.items.filter(item => {\n        const matchesQuery = item.name.toLowerCase().includes(query); // Check if name contains search query\n        const matchesRegion = region ? item.region.toLowerCase() === region : true; // Filter by region if selected\n        return matchesQuery && matchesRegion;\n      });\n    }\n  },\n  mounted() {\n    // Initialize filtered results when component is mounted\n    this.filteredResults = this.items;\n  }\n};","map":{"version":3,"names":["name","data","items","region","searchQuery","selectedRegion","filteredResults","methods","filterData","query","toLowerCase","filter","item","matchesQuery","includes","matchesRegion","mounted"],"sources":["C:\\Users\\IM