nt-bold hover:bg-[#028a66] transition\">\r\n                    Unduh ID Card Belakang\r\n                </button>\r\n            </a>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  export default {\r\n    data() {\r\n      return {\r\n        user: {\r\n          fullName: \"John Doe\",\r\n          username: \"johndoe123\",\r\n          email: \"johndoe@example.com\",\r\n          phone: \"08123456789\",\r\n          provinsi: \"Jawa Timur\",\r\n          kota: \"Surabaya\"\r\n        },\r\n        profileImage: \"https://via.placeholder.com/150\" \r\n      };\r\n    },\r\n    methods: {\r\n      handleFileUpload(event) {\r\n        const file = event.target.files[0];\r\n        if (file) {\r\n          this.profileImage = URL.createObjectURL(file);\r\n        }\r\n      },\r\n      printIDCard() {\r\n        window.print();\r\n      },\r\n      downloadIDCardBack() {\r\n        window.open(\"/assets/idcardbelakangpusat.pdf\", \"_blank\");\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  .input-field {\r\n    @apply w-full p-3 border border-gray-300 rounded-lg bg-gray-100 focus:outline-none;\r\n  }\r\n  </style>\r\n  "],"mappings":";;;AA+DE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE;QACJC,QAAQ,EAAE,UAAU;QACpBC,QAAQ,EAAE,YAAY;QACtBC,KAAK,EAAE,qBAAqB;QAC5BC,KAAK,EAAE,aAAa;QACpBC,QAAQ,EAAE,YAAY;QACtBC,IAAI,EAAE;MACR,CAAC;MACDC,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,gBAAgBA,CAACC,KAAK,EAAE;MACtB,MAAMC,IAAG,GAAID,KAAK,CAACE,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;MAClC,IAAIF,IAAI,EAAE;QACR,IAAI,CAACJ,YAAW,GAAIO,GAAG,CAACC,eAAe,CAACJ,IAAI,CAAC;MAC/C;IACF,CAAC;IACDK,WAAWA,CAAA,EAAG;MACZC,MAAM,CAACC,KAAK,CAAC,CAAC;IAChB,CAAC;IACDC,kBAAkBA,CAAA,EAAG;MACnBF,MAAM,CAACG,IAAI,CAAC,iCAAiC,EAAE,QAAQ,CAAC;IAC1D;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}