iv>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Grafik Perbulan dan Pertahun -->\r\n    <div class=\"flex justify-center mt-8\">\r\n      <div class=\"grid grid-cols-1 sm:grid-cols-2 md:grid-cols-2 gap-6 w-[90%]\">\r\n        <!-- Data Perbulan -->\r\n        <div class=\"bg-white p-6 rounded-lg shadow-md\">\r\n          <h3 class=\"text-lg font-semibold\">Pendapatan Perbulan</h3>\r\n          <LineChart :chartData=\"monthlyData\" />\r\n        </div>\r\n\r\n        <!-- Data Pertahun -->\r\n        <div class=\"bg-white p-6 rounded-lg shadow-md\">\r\n          <h3 class=\"text-lg font-semibold\">Pendapatan Pertahun</h3>\r\n          <LineChart :chartData=\"yearlyData\" />\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport NavbarView from \"@/components/NavbarView.vue\";\r\nimport LineChart from \"@/components/LineChart.vue\";\r\n\r\nexport default {\r\n  components: {\r\n    NavbarView,\r\n    LineChart,\r\n  },\r\n  data() {\r\n    return {\r\n      filters: [\"Hari ini\", \"7 hari terakhir\", \"28 hari terakhir\", \"Perbulan\", \"Pertahun\"],\r\n      activeFilter: \"7 hari terakhir\",\r\n      chartData: {\r\n        labels: [\"24 Sept\", \"25 Sept\", \"26 Sept\", \"27 Sept\", \"28 Sept\", \"29 Sept\"],\r\n        datasets: [\r\n          {\r\n            label: \"Pendapatan\",\r\n            data: [5000000, 8000000, 6000000, 4000000, 12000000, 9000000],\r\n            borderColor: \"#03a980\",\r\n            backgroundColor: \"rgba(3, 169, 128, 0.2)\",\r\n            tension: 0.3,\r\n            fill: true,\r\n          },\r\n        ],\r\n      },\r\n      monthlyData: {\r\n        labels: [\"Jan\", \"Feb\", \"Mar\", \"Apr\", \"Mei\", \"Jun\", \"Jul\", \"Agu\", \"Sep\", \"Okt\", \"Nov\", \"Des\"],\r\n        datasets: [\r\n          {\r\n            label: \"Pendapatan Bulanan\",\r\n            data: [15000000, 18000000, 21000000, 25000000, 22000000, 27000000, 30000000, 32000000, 31000000, 28000000, 26000000, 29000000],\r\n            borderColor: \"#ff9800\",\r\n            backgroundColor: \"rgba(255, 152, 0, 0.2)\",\r\n            tension: 0.3,\r\n            fill: true,\r\n          },\r\n        ],\r\n      },\r\n      yearlyData: {\r\n        labels: [\"2020\", \"2021\", \"2022\", \"2023\", \"2024\"],\r\n        datasets: [\r\n          {\r\n            label: \"Pendapatan Tahunan\",\r\n            data: [200000000, 220000000, 250000000, 280000000, 310000000],\r\n            borderColor: \"#3f51b5\",\r\n            backgroundColor: \"rgba(63, 81, 181, 0.2)\",\r\n            tension: 0.3,\r\n            fill: true,\r\n          },\r\n        ],\r\n      },\r\n    };\r\n  },\r\n  methods: {\r\n    setActiveFilter(filter) {\r\n      this.activeFilter = filter;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Responsif untuk layar kecil */\r\n@media (max-width: 768px) {\r\n\r\n\r\n  .text-lg {\r\n    font-size: 1rem;\r\n  }\r\n\r\n  .text-2xl {\r\n    font-size: 1.25rem;\r\n  }\r\n\r\n  .font-semibold {\r\n    font-weight: 600;\r\n  }\r\n\r\n  .bg-white {\r\n    background-color: #fff;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AA+DA,OAAOA,UAAS,MAAO,6BAA6B;AACpD,OAAOC,SAAQ,MAAO,4BAA4B;AAElD,eAAe;EACbC,UAAU,EAAE;IACVF,UAAU;IACVC;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,CAAC,UAAU,EAAE,iBAAiB,EAAE,kBAAkB,EAAE,UAAU,EAAE,UAAU,CAAC;MACpFC,YAAY,EAAE,iBAAiB;MAC/BC,SAAS,EAAE;QACTC,MAAM,EAAE,CAAC,SAAS,EAAE,SAAS,EAAE,SAAS,EAAE,SAAS,EAAE,SAAS,EAAE,SAAS,CAAC;QAC1EC,QAAQ,EAAE,CACR;UACEC,KAAK,EAAE,YAAY;UACnBN,IAAI,EAAE,CAAC,OAAO,EAAE,OAAO,EAAE,OAAO,EAAE,OAAO,EAAE,QAAQ,EAAE,OAAO,CAAC;UAC7DO,WAAW,EAAE,SAAS;UACtBC,eAAe,EAAE,wBAAwB;UACzCC,OAAO,EAAE,GAAG;UACZC,IAAI,EAAE;QACR,CAAC;MAEL,CAAC;MACDC,WAAW,EAAE;QACXP,MAAM,EAAE,CAAC,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,EAAE,KAAK,CAAC;QAC5FC,QAAQ,EAAE,CACR;UACEC,KAAK,EAAE,oBAAoB;UAC3BN,IAAI,EAAE,CAAC,QAAQ,EAAE,QAAQ,EAAE,QAAQ,EAAE,QAAQ,EAAE,QAAQ,EA