{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from \"axios\";\nexport default {\n  data() {\n    return {\n      email: \"\",\n      password: \"\",\n      errorMessage: \"\",\n      loading: false\n    };\n  },\n  methods: {\n    async submitForm() {\n      this.errorMessage = \"\";\n      this.loading = true;\n      try {\n        // Kirim data ke backend dengan properti yang sesuai\n        const response = await axios.post(\"http://192.168.1.11:5000/users/login\", {\n          email: this.email,\n          password: this.password\n        });\n\n        // Simpan token dan user data ke localStorage\n        localStorage.setItem(\"authToken\", response.data.token);\n        localStorage.setItem(\"userData\", JSON.stringify(response.data.user));\n\n        // Redirect ke dashboard\n        alert(\"Login berhasil!\");\n        this.$router.push(\"/profil\");\n      } catch (error) {\n        // Tangani error jika login gagal\n        console.error(\"Error Login:\", error);\n        this.errorMessage = error.response?.data?.error || \"Login gagal. Silakan periksa kredensial Anda.\";\n      } finally {\n        this.loading = false;\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","data","email","password","errorMessage","loading","methods","submitForm","response","post","localStorage","setItem","token","JSON","stringify","user","alert","$router","push","error","console"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\LoginView.vue"],"sourcesContent":["<template>\r\n  <div class=\"w-screen h-screen flex justify-center items-center bg-gray-100\">\r\n    <div class=\"main-container w-[1700px] h-[750px] bg-white relative overflow-hidden shadow-lg rounded-lg flex\">\r\n      <!-- Bagian Kiri (Form Login) -->\r\n      <div class=\"w-1/2 h-full flex flex-col justify-center items-center px-8\">\r\n        <img src=\"@/assets/logoswi.png\" alt=\"SWI Logo\" class=\"absolute top-6 w-24 h-auto\" />\r\n        <h2 class=\"text-2xl font-semibold text-[#03a980] mb-2\">Login Sekarang!!</h2>\r\n        <p class=\"text-sm text-gray-500 mb-4\">Silakan masukkan kredensial Anda</p>\r\n\r\n        <div class=\"w-[80%] max-w-md\">\r\n          <input\r\n            v-model=\"email\"\r\n            type=\"email\"\r\n            placeholder=\"Email\"\r\n            class=\"input-field\"\r\n            required\r\n          />\r\n          <input\r\n            v-model=\"password\"\r\n            type=\"password\"\r\n            placeholder=\"Password\"\r\n            class=\"input-field\"\r\n            required\r\n          />\r\n\r\n          <!-- Error message -->\r\n          <p v-if=\"errorMessage\" class=\"text-red-500 text-sm text-center mt-2\">{{ errorMessage }}</p>\r\n\r\n          <a href=\"/lupapassword\" class=\"text-[#03a980] text-sm mb-3 block text-center\">Lupa password?</a>\r\n\r\n          <button\r\n            @click=\"submitForm\"\r\n            class=\"w-full h-10 bg-[#03a980] text-white font-bold rounded-lg hover:bg-[#028a66] transition\"\r\n            :disabled=\"loading\"\r\n          >\r\n            {{ loading ? 'Loading...' : 'MASUK' }}\r\n          </button>\r\n\r\n          <p class=\"mt-4 text-sm text-gray-500\">\r\n            Don't have an account?\r\n            <router-link to=\"/register\" class=\"text-[#03a980] font-semibold hover:underline\">Register here</router-link>\r\n          </p>\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Bagian Kanan (Gambar) -->\r\n      <div class=\"w-1/2 h-full bg-[#03a980] rounded-l-[40px] flex flex-col justify-center items-center text-white\">\r\n        <div class=\"w-[80%] h-[90%] flex flex-col items-center\">\r\n          <div class=\"w-full h-full bg-[url('./assets/frame229.png')] bg-cover bg-no-repeat\"></div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      email: \"\",\r\n      password: \"\",\r\n      errorMessage: \"\",\r\n      loading: false,\r\n    };\r\n  },\r\n  methods: {\r\n    async submitForm() {\r\n      this.errorMessage = \"\";\r\n      this.loading = true;\r\n\r\n      try {\r\n        // Kirim data ke backend dengan properti yang sesuai\r\n        const response = await axios.post(\"http://192.168.1.11:5000/users/login\", {\r\n          email: this.email, \r\n          password: this.password, \r\n        });\r\n\r\n        // Simpan token dan user data ke localStorage\r\n        localStorage.setItem(\"authToken\", response.data.token);\r\n        localStorage.setItem(\"userData\", JSON.stringify(response.data.user));\r\n\r\n        // Redirect ke dashboard\r\n        alert(\"Login berhasil!\");\r\n        this.$router.push(\"/profil\");\r\n      } catch (error) {\r\n        // Tangani error jika login gagal\r\n        console.error(\"Error Login:\", error);\r\n        this.errorMessage = error.response?.data?.error || \"Login gagal. Silakan periksa kredensial Anda.\";\r\n      } finally {\r\n        this.loading = false;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.input-field {\r\n  @apply w-full h-10 px-4 border border-gray-300 rounded-lg focus:outline-none focus:ring-2 focus:ring-[#03a980] mb-3;\r\n}\r\n</style>\r\n"],"mappings":";AAwDA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,YAAY,EAAE,EAAE;MAChBC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,UAAUA,CAAA,EAAG;MACjB,IAAI,CAACH,YAAW,GAAI,EAAE;MACtB,IAAI,CAACC,OAAM,GAAI,IAAI;MAEnB,IAAI;QACF;QACA,MAAMG,QAAO,GAAI,MAAMR,KAAK,CAACS,IAAI,CAAC,sCAAsC,EAAE;UACxEP,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBC,QAAQ,EAAE,IAAI,CAACA;QACjB,CAAC,CAAC;;QAEF;QACAO,YAAY,CAACC,OAAO,CAAC,WAAW,EAAEH,QAAQ,CAACP,IAAI,CAACW,KAAK,CAAC;QACtDF,YAAY,CAACC,OAAO,CAAC,UAAU,EAAEE,IAAI,CAACC,SAAS,CAACN,QAAQ,CAACP,IAAI,CAACc,IAAI,CAAC,CAAC;;QAEpE;QACAC,KAAK,CAAC,iBAAiB,CAAC;QACxB,IAAI,CAAC