"bg-[#028a66] text-white\">\r\n                <tr>\r\n                  <th class=\"px-6 py-4 text-left\">Nama Pengirim</th>\r\n                  <th class=\"px-6 py-4 text-left\">Metode Pembayaran</th>\r\n                  <th class=\"px-6 py-4 text-left\">Jenis Bank</th>\r\n                  <th class=\"px-6 py-4 text-left\">Status Transaksi</th>\r\n                  <th class=\"px-6 py-4 text-left\">Tanggal Transaksi</th>\r\n                  <th class=\"px-6 py-4 text-left\">Bukti Transaksi</th>\r\n                </tr>\r\n              </thead>\r\n              <tbody>\r\n                <tr v-for=\"(transaction, index) in transactions\" :key=\"index\" class=\"border-b hover:bg-gray-100\">\r\n                  <td class=\"px-6 py-4\">{{ transaction.userName }}</td>\r\n                  <td class=\"px-6 py-4\">{{ transaction.amount }}</td>\r\n                  <td class=\"px-6 py-4\">{{ transaction.paymentMethod }}</td>\r\n                  <td class=\"px-6 py-4\">\r\n                    <span :class=\"getTransactionStatusClass(transaction.status)\">\r\n                      {{ transaction.status }}\r\n                    </span>\r\n                  </td>\r\n                  <td class=\"px-6 py-4\">{{ transaction.date }}</td>\r\n                  <td class=\"px-6 py-4\">\r\n                    <a :href=\"transaction.proofUrl\" target=\"_blank\" class=\"text-blue-500 hover:underline\">\r\n                      Lihat Bukti Transaksi\r\n                    </a>\r\n                  </td>\r\n                </tr>\r\n              </tbody>\r\n            </table>\r\n          </div>\r\n  \r\n          <!-- Jika tidak ada transaksi -->\r\n          <div v-else class=\"text-center text-gray-500\">\r\n            Belum ada transaksi yang diproses.\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import NavbarPusat from '@/components/NavbarPusat.vue';\r\n  \r\n  export default {\r\n    components: {\r\n      NavbarPusat\r\n    },\r\n    data() {\r\n      return {\r\n        transactions: [\r\n          {\r\n            userName: \"John Doe\",\r\n            amount: 500000,\r\n            paymentMethod: \"Transfer Bank\",\r\n            status: \"Berhasil\",\r\n            date: \"2023-08-01\",\r\n            proofUrl: \"/assets/proof_1.jpg\" // URL bukti transaksi\r\n          },\r\n          {\r\n            userName: \"Jane Smith\",\r\n            amount: 300000,\r\n            paymentMethod: \"Kartu Kredit\",\r\n            status: \"Gagal\",\r\n            date: \"2023-08-02\",\r\n            proofUrl: \"/assets/proof_2.jpg\" // URL bukti transaksi\r\n          }\r\n        ]\r\n      };\r\n    },\r\n    methods: {\r\n      getTransactionStatusClass(status) {\r\n        switch (status) {\r\n          case 'Berhasil':\r\n            return 'text-green-500';\r\n          case 'Gagal':\r\n            return 'text-red-500';\r\n          case 'Diproses':\r\n            return 'text-yellow-500';\r\n          default:\r\n            return 'text-gray-500';\r\n        }\r\n      }\r\n    }\r\n  };\r\n  </script>\r\n  \r\n  <style scoped>\r\n  /* Styling untuk tabel dan tampilan umum */\r\n  table {\r\n    width: 100%;\r\n    border-collapse: collapse;\r\n  }\r\n  \r\n  th, td {\r\n    padding: 16px;\r\n  }\r\n  \r\n  th {\r\n    background-color: #028a66;\r\n    color: white;\r\n  }\r\n  \r\n  td {\r\n    border-bottom: 1px solid #ddd;\r\n  }\r\n  \r\n  tr:hover {\r\n    background-color: #f5f5f5;\r\n  }\r\n  \r\n  button {\r\n    padding: 12px 20px;\r\n    font-size: 16px;\r\n    border-radius: 8px;\r\n  }\r\n  \r\n  button:hover {\r\n    cursor: pointer;\r\n  }\r\n  \r\n  button + button {\r\n    margin-left: 12px;\r\n  }\r\n  </style>\r\n  "],"mappings":"AAqDE,OAAOA,WAAU,MAAO,8BAA8B;AAEtD,eAAe;EACbC,UAAU,EAAE;IACVD;EACF,CAAC;EACDE,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,YAAY,EAAE,CACZ;QACEC,QAAQ,EAAE,UAAU;QACpBC,MAAM,EAAE,MAAM;QACdC,aAAa,EAAE,eAAe;QAC9BC,MAAM,EAAE,UAAU;QAClBC,I