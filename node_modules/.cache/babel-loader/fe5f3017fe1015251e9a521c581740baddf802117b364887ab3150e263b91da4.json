{"ast":null,"code":"import FooterLandingPage from '@/components/FooterLandingPage.vue';\nimport NavbarLandingPage from '@/components/NavbarLandingPage.vue';\nexport default {\n  components: {\n    NavbarLandingPage,\n    FooterLandingPage\n  },\n  name: 'LandingPage',\n  data() {\n    return {\n      filters: [\"About\", \"Services\", \"Contact\"]\n    };\n  },\n  mounted() {\n    this.typeWriterEffect();\n    this.addFadeInAnimation();\n  },\n  methods: {\n    // Method for Typing Effect\n    typeWriterEffect() {\n      const text = \"Lembaga Perlindungan Konsumen Indonesia\";\n      let i = 0;\n      const element = document.getElementById(\"typing\");\n\n      // Function to simulate typing\n      function type() {\n        if (i < text.length) {\n          element.innerHTML += text.charAt(i);\n          i++;\n          setTimeout(type, 100);\n        } else {\n          setTimeout(() => {\n            element.innerHTML = \"\"; // Clear the text\n            i = 0; // Reset the index\n            type(); // Restart typing\n          }, 2000); // Wait for 2 seconds after typing is done before starting again\n        }\n      }\n      type(); // Start typing effect\n    },\n    // Method to add fade-in effect on scroll\n    addFadeInAnimation() {\n      window.addEventListener('scroll', function () {\n        const aboutText = document.getElementById('about-text');\n        // const rect = aboutText.getBoundingClientRect();\n        const viewportHeight = window.innerHeight;\n        const trackRecord = document.getElementById('track-record');\n        if (rect.top < viewportHeight - 100) {\n          trackRecord.classList.add('animate__fadeIn');\n        }\n        // Check if the section is in the viewport\n        if (rect.top < viewportHeight - 100) {\n          aboutText.classList.add('animate-fadeIn');\n        }\n      });\n    },\n    setActiveFilter(filter) {\n      console.log(filter);\n    }\n  }\n};","map":{"version":3,"names":["FooterLandingPage","NavbarLandingPage","components","name","data","filters","mounted","typeWriterEffect","addFadeInAnimation","methods","text","i","element","document","getElementById","type","length","innerHTML","charAt","setTimeout","window","addEventListener","aboutText","viewportHeight","innerHeight","trackRecord","rect","top","classList","add","setActiveFilter","filter","console","log"],"sources":["C:\\Users\\IMAM\\OneDrive\\Desktop\\LPKNI\\lpkniweb\\src\\views\\lpkni\\Dashboard.vue"],"sourcesContent":["<template>\r\n  <div class=\"bg-red-100 min-h-screen\">\r\n    <NavbarLandingPage />\r\n\r\n    <!-- Hero Section -->\r\n    <section class=\"bg-red-700 text-white py-20\">\r\n      <div class=\"container mx-auto max-w-screen-xl px-4 md:px-8 flex flex-col-reverse md:flex-row items-center justify-between\">\r\n        <!-- Left Section (Text Content) -->\r\n        <div class=\"w-full md:w-1/2 text-center md:text-left\">\r\n          <h1 id=\"typing\" class=\"text-3xl sm:text-4xl md:text-5xl font-bold mb-4\">LPKNI</h1>\r\n          <p class=\"mb-6 text-base sm:text-lg leading-relaxed\">\r\n            LPKNI merupakan salah satu lembaga yang mempunyai tugas pokok dalam melindungi hak setiap warga negara baik perseorangan maupun badan sebagai konsumen.\r\n          </p>\r\n          <a href=\"#contact\" class=\"bg-white text-red-600 px-6 py-2 rounded-lg font-semibold hover:bg-gray-100 transition duration-300 inline-block\">\r\n            Get Involved\r\n          </a>\r\n        </div>\r\n\r\n        <!-- Right Section (Image) -->\r\n        <div class=\"w-full md:w-1/2 flex justify-center md:justify-end\">\r\n          <img src=\"@/assets/animasi1.png\" alt=\"LPKNI\" class=\"rounded-lg w-[80%] sm:w-[60%] md:w-full max-w-[300px] sm:max-w-[400px] md:max-w-full\" />\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- About Section -->\r\n    <section id=\"about\" class=\"py-20 px-4 md:px-8 bg-red-100 text-center\">\r\n      <h2 class=\"text-3xl font-semibold mb-4 text-gray-800\">TENTANG KAMI</h2>\r\n      <p class=\"text-lg text-gray-600 max-w-3xl mx-auto mb-8 leading-relaxed\">\r\n        LPKNI adalah lembaga yang bertugas melindungi hak konsumen terhadap perbankan, koperasi, dan pinjaman online yang tidak adil.\r\n      </p>\r\n      <div class=\"flex justify-center\">\r\n        <button class=\"bg-red-600 text-white py-3 px-6 rounded-full shadow-lg hover:bg-red-700 transition-all transform hover:scale-105\">\r\n          <i class=\"fas fa-info-circle mr-2\"></i> Learn More\r\n        </button>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- Track Record Section -->\r\n    <section id=\"track-record\" class=\"py-20 px-4 md:px-8 bg-white\">\r\n      <h2 class=\"text-3xl font-semibold text-center mb-10 text-red-700\">Perjalanan LPKNI</h2>\r\n      <p class=\"text-lg text-gray-600 text-center max-w-3xl mx-auto mb-12\">\r\n        Berikut adalah beberapa kegiatan dan pencapaian penting kami.\r\n      </p>\r\n\r\n      <div class=\"grid grid-cols-1 sm:grid-cols-2 md:grid-cols-3 gap-6\">\r\n        <!-- Card 1 -->\r\n        <div class=\"bg-gray-100 p-4 rounded-lg shadow-lg hover:shadow-2xl transition-transform transform hover:scale-105\">\r\n          <img src=\"@/assets/kegiatan1.png\" alt=\"LPKNI Event 1\" class=\"rounded-lg w-full object-cover\">\r\n          <h3 class=\"text-xl font-semibold mt-4 hover:text-red-600\">Standarisasi Warung</h3>\r\n          <p class=\"text-gray-600 mt-2\">\r\n            Presentasi Standarisasi Warung Indonesia bersama Kementerian Pariwisata.\r\n          </p>\r\n        </div>\r\n\r\n        <!-- Card 2 -->\r\n        <div class=\"bg-gray-100 p-4 rounded-lg shadow-lg hover:shadow-2xl transition-transform transform hover:scale-105\">\r\n          <img src=\"@/assets/kegiatan2.jpeg\" alt=\"LPKNI Event 2\" class=\"rounded-lg w-full object-cover\">\r\n          <h3 class=\"text-xl font-semibold mt-4 hover:text-red-600\">Operasi Pasar</h3>\r\n          <p class=\"text-gray-600 mt-2\">\r\n            Operasi pasar minyak goreng murah Letizia di Kota Malang.\r\n          </p>\r\n        </div>\r\n\r\n        <!-- Card 3 -->\r\n        <div class=\"bg-gray-100 p-4 rounded-lg shadow-lg hover:shadow-2xl transition-transform transform hover:scale-105\">\r\n          <img src=\"@/assets/kegiatan3.png\" alt=\"LPKNI Event 3\" class=\"rounded-lg w-full object-cover\">\r\n          <h3 class=\"text-xl font-semibold mt-4 hover:text-red-600\">Kampanye Hak Konsumen</h3>\r\n          <p class=\"text-gray-600 mt-2\">\r\n            Edukasi masyarakat mengenai perlindungan konsumen.\r\n          </p>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- Contact Section -->\r\n    <section id=\"contact\" class=\"py-20 px-4 md:px-8 bg-white\">\r\n      <div class=\"container mx-auto text-center\">\r\n        <h2 class=\"text-4xl font-bold text-red-600 mb-4\">Hubungi Kami</h2>\r\n        <p class=\"text-xl text-gray-600 mb-8\">Have questions? Contact us now!</p>\r\n\r\n        <div class=\"flex flex-col sm:flex-row justify-center items-center space-y-4 sm:space-y-0 sm:space-x-4\">\r\n          <a href=\"tel:+62341492174\" class=\"bg-red-600 text-white px-10 py-4 rounded-lg font-semibold hover:bg-red-700 transition-transform transform hover:scale-105\">\r\n            Call Us: 0341 492174\r\n          </a>\r\n          <a href=\"mailto:lpkni.jt@gmail.com\" class=\"bg-red-600 text-white px-10 py-4 rounded-lg font-semibold hover:bg-red-700 transition-transform transform hover:scale-105\">\r\n            Email Us\r\n          </a>\r\n        </div>\r\n\r\n        <div class=\"mt-10 flex justify-center space-x-6\">\r\n          <a href=\"https://www.facebook.com/lpkni.pusat/\" class=\"text-red-600 hover:text-red-800 text-2xl\">\r\n            <i class=\"fab fa-facebook-f\"></i>\r\n          </a>\r\n          <a href=\"https://www.instagram.com/lpkni.jaktim/\" class=\"text-red-600 hover:text-red-800 text-2xl\">\r\n            <i class=\"fab fa-instagram\"></i>\r\n          </a>\r\n          <a href=\"https://www.youtube.com/user/ConsumersIntl\" class=\"text-red-600 hover:text-red-800 text-2xl\">\r\n            <i class=\"fab fa-youtube\"></i>\r\n          </a>\r\n        </div>\r\n      </div>\r\n    </section>\r\n\r\n    <FooterLandingPage />\r\n  </div>\r\n</template>\r\n\r\n  \r\n  <script>\r\nimport FooterLandingPage from '@/components/FooterLandingPage.vue';\r\nimport NavbarLandingPage from '@/components/NavbarLandingPage.vue';\r\n\r\n  export default {\r\n    components: {\r\n        NavbarLandingPage,\r\n        FooterLandingPage\r\n    },\r\n    name: 'LandingPage',\r\n    data() {\r\n      return {\r\n        filters: [\"About\", \"Services\", \"Contact\"]\r\n      };\r\n    },\r\n    mounted() {\r\n      this.typeWriterEffect(); \r\n      this.addFadeInAnimation(); \r\n    },\r\n    methods: {\r\n      // Method for Typing Effect\r\n      typeWriterEffect() {\r\n            const text = \"Lembaga Perlindungan Konsumen Indonesia\"; \r\n            let i = 0;\r\n            const element = document.getElementById(\"typing\"); \r\n            \r\n            // Function to simulate typing\r\n            function type() {\r\n            if (i < text.length) {\r\n                element.innerHTML += text.charAt(i); \r\n                i++;\r\n                setTimeout(type, 100); \r\n            } else {\r\n                setTimeout(() => {\r\n                element.innerHTML = \"\"; // Clear the text\r\n                i = 0; // Reset the index\r\n                type(); // Restart typing\r\n                }, 2000); // Wait for 2 seconds after typing is done before starting again\r\n            }\r\n            }\r\n\r\n            type(); // Start typing effect\r\n        },\r\n\r\n      // Method to add fade-in effect on scroll\r\n      addFadeInAnimation() {\r\n        window.addEventListener('scroll', function() {\r\n          const aboutText = document.getElementById('about-text');\r\n          // const rect = aboutText.getBoundingClientRect();\r\n          const viewportHeight = window.innerHeight;\r\n          const trackRecord = document.getElementById('track-record');\r\n          \r\n          if (rect.top < viewportHeight - 100) {\r\n            trackRecord.classList.add('animate__fadeIn');\r\n            }\r\n          // Check if the section is in the viewport\r\n          if (rect.top < viewportHeight - 100) {\r\n            aboutText.classList.add('animate-fadeIn');\r\n          }\r\n        });\r\n      },\r\n      \r\n      setActiveFilter(filter) {\r\n        console.log(filter);\r\n      }\r\n    }\r\n  };\r\n</script>\r\n\r\n  \r\n  <style scoped>\r\n  /* Tailwind CSS animation for fade-in effect */\r\n  @keyframes fadeIn {\r\n    from {\r\n      opacity: 0;\r\n    }\r\n    to {\r\n      opacity: 1;\r\n    }\r\n  }\r\n\r\n  .animate-fadeIn {\r\n    animation: fadeIn 2s ease-out forwards;\r\n  }\r\n  .animate__fadeIn {\r\n    animation: fadeIn 1s ease-in-out;\r\n  }\r\n\r\n  /* For Hover Effect */\r\n  .hover\\:scale-105:hover {\r\n    transform: scale(1.05);\r\n  }\r\n\r\n  /* For the text hover effect */\r\n  .hover\\:text-red-600:hover {\r\n    color: #ef4444;\r\n  }\r\n\r\n  /* Optional: For Smooth Scroll */\r\n  html {\r\n    scroll-behavior: smooth;\r\n  }\r\n</style>"],"mappings":"AA8GA,OAAOA,iBAAgB,MAAO,oCAAoC;AAClE,OAAOC,iBAAgB,MAAO,oCAAoC;AAEhE,eAAe;EACbC,UAAU,EAAE;IACRD,iBAAiB;IACjBD;EACJ,CAAC;EACDG,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,CAAC,OAAO,EAAE,UAAU,EAAE,SAAS;IAC1C,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,gBAAgB,CAAC,CAAC;IACvB,IAAI,CAACC,kBAAkB,CAAC,CAAC;EAC3B,CAAC;EACDC,OAAO,EAAE;IACP;IACAF,gBAAgBA,CAAA,EAAG;MACb,MAAMG,IAAG,GAAI,yCAAyC;MACtD,IAAIC,CAAA,GAAI,CAAC;MACT,MAAMC,OAAM,GAAIC,QAAQ,CAACC,cAAc,CAAC,QAAQ,CAAC;;MAEjD;MACA,SAASC,IAAIA,CAAA,EAAG;QAChB,IAAIJ,CAAA,GAAID,IAAI,CAACM,MAAM,EAAE;UACjBJ,OAAO,CAACK,SAAQ,IAAKP,IAAI,CAACQ,MAAM,CAACP,CAAC,CAAC;UACnCA,CAAC,EAAE;UACHQ,UAAU,CAACJ,IAAI,EAAE,GAAG,CAAC;QACzB,OAAO